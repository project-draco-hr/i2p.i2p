{
  try {
    List records=_manager._removeAll();
    if (records == null)     return;
    for (int i=0; i < records.size(); i++) {
      LogRecord rec=(LogRecord)records.get(i);
      writeRecord(rec);
    }
  }
 catch (  Throwable t) {
    t.printStackTrace();
  }
 finally {
    if (shouldWait) {
      try {
synchronized (this) {
          this.wait(10 * 1000);
        }
      }
 catch (      InterruptedException ie) {
      }
    }
  }
}
