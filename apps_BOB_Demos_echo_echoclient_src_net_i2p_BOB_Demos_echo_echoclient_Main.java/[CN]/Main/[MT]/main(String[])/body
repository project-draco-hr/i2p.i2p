{
  try {
    setupconn(args);
    chatter(args);
  }
 catch (  UnknownHostException ex) {
    Logger.getLogger(Main.class.getName()).log(Level.SEVERE,null,ex);
  }
catch (  IOException ex) {
    Logger.getLogger(Main.class.getName()).log(Level.SEVERE,null,ex);
  }
  try {
    deleteconn(args);
  }
 catch (  UnknownHostException ex) {
    Logger.getLogger(Main.class.getName()).log(Level.SEVERE,null,ex);
  }
catch (  IOException ex) {
    Logger.getLogger(Main.class.getName()).log(Level.SEVERE,null,ex);
  }
catch (  RuntimeException ex) {
    Logger.getLogger(Main.class.getName()).log(Level.SEVERE,null,ex);
  }
}
