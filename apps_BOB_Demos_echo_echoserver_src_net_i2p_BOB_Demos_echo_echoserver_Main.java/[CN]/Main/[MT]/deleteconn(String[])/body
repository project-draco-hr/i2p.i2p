{
  String line;
  Socket CMDsock=new Socket("localhost",0xB0B);
  InputStream CMDin=CMDsock.getInputStream();
  OutputStream CMDout=CMDsock.getOutputStream();
  line=Lread(CMDin);
  System.out.println(line);
  line=Lread(CMDin);
  System.out.println(line);
  wrtxt(CMDout,"getnick " + args[2]);
  line=Lread(CMDin);
  checkline(line);
  wrtxt(CMDout,"stop");
  line=Lread(CMDin);
  checkline(line);
  try {
    Thread.sleep(2000);
  }
 catch (  Exception e) {
  }
  wrtxt(CMDout,"clear");
  line=Lread(CMDin);
  while (line.startsWith("ERROR")) {
    wrtxt(CMDout,"clear");
    line=Lread(CMDin);
  }
  System.out.println(line);
  CMDsock.close();
}
