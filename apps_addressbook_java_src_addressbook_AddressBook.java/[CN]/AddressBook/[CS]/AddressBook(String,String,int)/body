{
  this.location=url;
  EepGet get=new EepGet(I2PAppContext.getGlobalContext(),true,proxyHost,proxyPort,0,"addressbook.tmp",url,true,null);
  get.fetch();
  try {
    this.addresses=ConfigParser.parse("addressbook.tmp");
  }
 catch (  IOException exp) {
    this.addresses=new HashMap();
  }
  new File("addressbook.tmp").delete();
}
