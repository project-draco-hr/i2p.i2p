{
  this.location=subscription.getLocation();
  EepGet get=new EepGet(I2PAppContext.getGlobalContext(),true,proxyHost,proxyPort,0,-1l,MAX_SUB_SIZE,"addressbook.tmp",null,subscription.getLocation(),true,subscription.getEtag(),subscription.getLastModified(),null);
  if (get.fetch()) {
    subscription.setEtag(get.getETag());
    subscription.setLastModified(get.getLastModified());
  }
  try {
    this.addresses=ConfigParser.parse(new File("addressbook.tmp"));
  }
 catch (  IOException exp) {
    this.addresses=new HashMap();
  }
  new File("addressbook.tmp").delete();
}
