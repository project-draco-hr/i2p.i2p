{
  Iterator iter=this.addresses.keySet().iterator();
  int removed=0;
  while (iter.hasNext()) {
    String aKey=(String)iter.next();
    if (!aKey.endsWith(".i2p")) {
      iter.remove();
      removed++;
      if (log != null)       log.append("Invalid address " + aKey + " filtered out.");
    }
  }
  if (removed > 0)   this.modified=true;
  return removed;
}
