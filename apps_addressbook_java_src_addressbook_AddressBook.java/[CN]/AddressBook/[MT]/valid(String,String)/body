{
  return host.endsWith(".i2p") && host.length() > 4 && host.length() <= 67 && (!host.startsWith(".")) && (!host.startsWith("-")) && (!host.endsWith("-.i2p")) && host.indexOf("..") < 0 && host.replaceAll("[a-z0-9.-]","").length() == 0 && dest.length() == 516 && dest.endsWith("AAAA") && dest.replaceAll("[a-zA-Z0-9~-]","").length() == 0;
}
