{
  if (input == null)   return false;
  if (next != null)   return true;
  try {
    String inputLine;
    while ((inputLine=input.readLine()) != null) {
      HostTxtEntry he=HostTxtParser.parse(inputLine);
      if (he == null)       continue;
      next=new MapEntry(he.getName(),he);
      return true;
    }
  }
 catch (  IOException ioe) {
  }
  try {
    input.close();
  }
 catch (  IOException ioe) {
  }
  input=null;
  next=null;
  return false;
}
