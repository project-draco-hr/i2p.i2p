{
  File f=new File("tmp-hosts.txt");
  Map<String,HostTxtEntry> map=parse(f);
  for (  HostTxtEntry e : map.values()) {
    System.out.println("Host: " + e.getName() + "\nDest: "+ e.getDest()+ "\nAction: "+ (e.getProps() != null ? e.getProps().getProperty("action") : "(none)")+ "\nValid Inner? "+ e.hasValidInnerSig()+ "\nValid? "+ e.hasValidSig()+ '\n');
  }
}
