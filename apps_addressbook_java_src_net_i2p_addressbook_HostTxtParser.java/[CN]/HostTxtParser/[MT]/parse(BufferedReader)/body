{
  try {
    Map<String,HostTxtEntry> result=new HashMap<String,HostTxtEntry>();
    String inputLine;
    while ((inputLine=input.readLine()) != null) {
      HostTxtEntry he=parse(inputLine);
      if (he == null)       continue;
      result.put(he.getName(),he);
    }
    return result;
  }
  finally {
    try {
      input.close();
    }
 catch (    IOException ioe) {
    }
  }
}
