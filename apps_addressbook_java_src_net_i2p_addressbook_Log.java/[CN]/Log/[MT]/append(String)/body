{
  BufferedWriter bw=null;
  try {
    bw=new BufferedWriter(new FileWriter(this.file,true));
    String timestamp=new Date().toString();
    bw.write(timestamp + " -- " + entry);
    bw.newLine();
  }
 catch (  IOException exp) {
  }
 finally {
    if (bw != null)     try {
      bw.close();
    }
 catch (    IOException ioe) {
    }
  }
}
