{
  Bogoparser bogoparser;
  if (args.length < 1 || args.length > 2)   displayUsageAndExit();
  if (args.length == 2) {
    if (!args[0].equals("--by-duration"))     displayUsageAndExit();
    bogoparser=new Bogoparser(args[1],true);
  }
  if (args.length == 1)   bogoparser=new Bogoparser(args[0],false);
}
