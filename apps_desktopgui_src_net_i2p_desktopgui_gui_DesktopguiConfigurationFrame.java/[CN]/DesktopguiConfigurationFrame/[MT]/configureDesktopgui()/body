{
  String property="desktopgui.enabled";
  String value;
  if (!desktopguiEnabled.isSelected()) {
    value="false";
  }
 else {
    value="true";
  }
  try {
    _context.router().saveConfig(property,value);
  }
 catch (  Exception ex) {
    Logger.getLogger(DesktopguiConfigurationFrame.class.getName()).log(Level.SEVERE,null,ex);
  }
  this.dispose();
}
