{
  try {
    Desktop.getDesktop().browse(new URI("http://127.0.0.1:7657/configupdate.jsp"));
  }
 catch (  URISyntaxException ex) {
    Logger.getLogger(GeneralConfiguration.class.getName()).log(Level.SEVERE,null,ex);
  }
catch (  IOException ex) {
    Logger.getLogger(GeneralConfiguration.class.getName()).log(Level.SEVERE,null,ex);
  }
}
