{
  if (status == IndexBean.NOT_RUNNING) {
    return "Not running";
  }
 else   if (status == IndexBean.RUNNING) {
    return "Running";
  }
 else   if (status == IndexBean.STANDBY) {
    return "Standby";
  }
 else   if (status == IndexBean.STARTING) {
    return "Starting";
  }
 else {
    return "Error: status not found";
  }
}
