{
  if (uploadkbps.getSelectedIndex() == KILOBIT)   props.setProperty(SpeedSelectorConstants.MAXUPLOADCAPABLE,uploadChoice.getSelectedItem().toString());
 else   props.setProperty(SpeedSelectorConstants.MAXUPLOADCAPABLE,"" + Integer.parseInt(uploadChoice.getSelectedItem().toString()) * 8);
  if (downloadkbps.getSelectedIndex() == KILOBIT)   props.setProperty(SpeedSelectorConstants.MAXDOWNLOADCAPABLE,downloadChoice.getSelectedItem().toString());
 else   props.setProperty(SpeedSelectorConstants.MAXDOWNLOADCAPABLE,"" + Integer.parseInt(downloadChoice.getSelectedItem().toString()) * 8);
  PropertyManager.saveProps(props);
  new SpeedSelector2(this.getLocationOnScreen());
  this.dispose();
}
