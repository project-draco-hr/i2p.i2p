{
  String upload="";
  if (kbpsUpload.getSelectedIndex() == KILOBIT)   upload=uploadField.getText();
 else   upload="" + Integer.parseInt(uploadField.getText()) / 8;
  String download="";
  if (kbpsDownload.getSelectedIndex() == KILOBIT)   download=downloadField.getText();
 else   download="" + Integer.parseInt(downloadField.getText()) / 8;
  initUsage(upload,download);
}
