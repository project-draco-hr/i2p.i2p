{
  try {
    String separator=System.getProperty("file.separator");
    String homeDirectory=System.getProperty("user.home");
    String location=ConfigurationManager.getInstance().getStringConfiguration("I2PLocation",homeDirectory + separator + "i2p");
    Runtime.getRuntime().exec(location + separator + "i2psvc"+ location+ separator+ "wrapper.config");
  }
 catch (  IOException e) {
    log.log(Log.WARN,"Failed to start I2P",e);
  }
}
