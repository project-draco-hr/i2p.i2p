{
  _location=location;
  if (config == null)   config=new ClientConfig(location);
  _config=config;
  if (currentSeriesConfig != null)   _currentSeriesConfig=currentSeriesConfig;
 else   _currentSeriesConfig=new PlotSeriesConfig(0);
  if (averageSeriesConfigs != null) {
    _averageSeriesConfigs=averageSeriesConfigs;
  }
 else {
    rebuildAverageSeriesConfigs();
  }
  _listeners=Collections.synchronizedSet(new HashSet(2));
  _disabled=false;
}
