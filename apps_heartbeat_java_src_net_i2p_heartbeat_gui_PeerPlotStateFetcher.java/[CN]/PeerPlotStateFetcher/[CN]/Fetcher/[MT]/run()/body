{
  String loc=_state.getPlotConfig().getLocation();
  _log.debug("Load called [" + loc + "]");
  InputStream in=null;
  try {
    try {
      URL location=new URL(loc);
      in=location.openStream();
    }
 catch (    MalformedURLException mue) {
      _log.debug("Not a url [" + loc + "]");
      in=null;
    }
    if (in == null)     in=new FileInputStream(loc);
    BufferedReader reader=new BufferedReader(new InputStreamReader(in));
    String line=null;
    while ((line=reader.readLine()) != null) {
      handleLine(line);
    }
    if (valid())     _receptor.peerPlotStateFetched(_state);
  }
 catch (  IOException ioe) {
    _log.error("Error retrieving from the location [" + loc + "]",ioe);
  }
 finally {
    if (in != null)     try {
      in.close();
    }
 catch (    IOException ioe) {
    }
  }
}
