{
  if (maxManagers < 1) {
    throw new IllegalArgumentException("maxManagers < 1");
  }
  this.host=host;
  this.port=port;
  this.shouldThrowAwayManagers=shouldThrowAwayManagers;
  if (initialManagers != null) {
    myManagers.addAll(Arrays.asList(initialManagers));
  }
  this.maxManagers=maxManagers;
  this.shouldThrowAwayManagers=shouldThrowAwayManagers;
  setDaemon(true);
  start();
}
