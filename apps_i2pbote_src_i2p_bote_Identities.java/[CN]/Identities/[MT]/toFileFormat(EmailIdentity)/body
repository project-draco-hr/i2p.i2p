{
  StringBuilder string=new StringBuilder();
  string=string.append(identity.getFullKey());
  string=string.append("\t");
  string=string.append(identity.getPublicName());
  string=string.append("\t");
  if (identity.getDescription() != null)   string=string.append(identity.getDescription());
  string=string.append("\t");
  if (identity.getEmailAddress() != null)   string=string.append(identity.getEmailAddress());
  return string.toString();
}
