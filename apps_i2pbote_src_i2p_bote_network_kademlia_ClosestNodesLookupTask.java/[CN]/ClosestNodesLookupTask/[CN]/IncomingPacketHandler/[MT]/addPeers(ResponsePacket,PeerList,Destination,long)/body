{
  log.debug("Peer List Packet received: #peers=" + peerListPacket.getPeers().size() + ", sender="+ sender);
  FindClosePeersPacket request=getPacketById(pendingRequests,responsePacket.getPacketId());
  if (request != null) {
    responseReceived.add(sender);
    Collection<KademliaPeer> peersReceived=peerListPacket.getPeers();
    for (    KademliaPeer peer : peersReceived)     if (contains(notQueriedYet,peer))     notQueriedYet.add(peer);
    pendingRequests.remove(request);
  }
 else   log.debug("No Find Close Nodes packet found for Peer List: " + peerListPacket);
}
