{
  this.key=key;
  this.sendQueue=sendQueue;
  this.i2pReceiver=i2pReceiver;
  this.bucketManager=bucketManager;
  randomNumberGenerator=new Random(getTime());
  responseReceived=new TreeSet<Destination>(new HashDistanceComparator(key));
  notQueriedYet=new ConcurrentHashSet<KademliaPeer>();
  pendingRequests=new ConcurrentHashSet<FindClosePeersPacket>();
}
