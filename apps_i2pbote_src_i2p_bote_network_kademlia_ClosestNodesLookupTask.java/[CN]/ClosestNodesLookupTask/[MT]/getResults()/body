{
  List<Destination> resultsList=new ArrayList<Destination>();
  for (  Destination destination : responseReceived)   resultsList.add(destination);
  Collections.sort(resultsList,new HashDistanceComparator(key));
  if (resultsList.size() > KademliaConstants.S)   resultsList=resultsList.subList(0,KademliaConstants.S);
  return resultsList;
}
