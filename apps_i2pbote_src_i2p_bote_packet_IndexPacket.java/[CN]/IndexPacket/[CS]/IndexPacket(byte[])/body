{
  ByteBuffer dataBuffer=ByteBuffer.wrap(data);
  if (dataBuffer.get() != getPacketTypeCode())   log.error("Wrong type code for IndexPacket. Expected <" + getPacketTypeCode() + ">, got <"+ (char)data[0]+ ">");
  destinationHash=readHash(dataBuffer);
  int numKeys=dataBuffer.get();
  dhtKeys=new ArrayList<Hash>();
  for (int i=0; i < numKeys; i++) {
    Hash dhtKey=readHash(dataBuffer);
    dhtKeys.add(dhtKey);
  }
}
