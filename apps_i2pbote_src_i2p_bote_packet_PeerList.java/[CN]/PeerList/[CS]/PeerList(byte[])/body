{
  ByteBuffer buffer=ByteBuffer.wrap(data);
  if (buffer.get() != getPacketTypeCode())   log.error("Wrong type code for PeerList. Expected <" + getPacketTypeCode() + ">, got <"+ (char)data[0]+ ">");
  int numPeers=buffer.getShort();
  peers=new ArrayList<KademliaPeer>();
  for (int i=0; i < numPeers; i++) {
    Destination destination=new Destination();
    byte[] peerData=new byte[388];
    buffer.get(peerData,0,384);
    destination.readBytes(peerData,0);
    KademliaPeer peer=new KademliaPeer(destination,0);
    peers.add(peer);
  }
  if (buffer.hasRemaining())   log.debug("Peer List has " + buffer.remaining() + " extra bytes.");
}
