{
  List<T> rv=new ArrayList(_buckets.size());
  long old=_context.clock().now() - age;
  int prevSize=-1;
  getReadLock();
  try {
    for (    KBucket b : _buckets) {
      int curSize=b.getKeyCount();
      if ((prevSize != 0 || curSize != 0) && (b.getLastChanged() < old || curSize < BUCKET_SIZE * 3 / 4))       rv.add(generateRandomKey(b));
      prevSize=curSize;
    }
  }
  finally {
    releaseReadLock();
  }
  return rv;
}
