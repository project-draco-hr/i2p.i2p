{
  List<T> rv=new ArrayList(_buckets.size());
  long old=_context.clock().now() - age;
  getReadLock();
  try {
    for (    KBucket b : _buckets) {
      if (b.getLastChanged() < old || b.getKeyCount() < BUCKET_SIZE * 3 / 4)       rv.add(generateRandomKey(b));
    }
  }
  finally {
    releaseReadLock();
  }
  return rv;
}
