{
  int range=getRange(key);
  if (range < 0)   return -1;
  int rv=pickBucket(range);
  if (rv >= 0) {
    return rv;
  }
  _log.error("Key does not fit in any bucket?! WTF!\nKey  : [" + DataHelper.toHexString(key.getData()) + "]"+ "\nUs   : "+ _us+ "\nDelta: ["+ DataHelper.toHexString(DataHelper.xor(_us.getData(),key.getData()))+ "]",new Exception("WTF"));
  _log.error(toString());
  throw new IllegalStateException("pickBucket returned " + rv);
}
