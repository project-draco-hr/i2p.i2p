{
  File out=null;
  try {
    out=File.createTempFile("i2psnark","url");
  }
 catch (  IOException ioe) {
    ioe.printStackTrace();
    return null;
  }
  EepGet get=new EepGet(_context,_shouldProxy,_proxyHost,_proxyPort,1,out.getAbsolutePath(),url);
  if (get.fetch()) {
    return out;
  }
 else {
    out.delete();
    return null;
  }
}
