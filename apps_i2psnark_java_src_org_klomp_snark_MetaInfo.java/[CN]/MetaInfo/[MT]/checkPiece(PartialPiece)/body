{
  MessageDigest sha1=SHA1.getInstance();
  int piece=pp.getPiece();
  byte[] hash;
  try {
    hash=pp.getHash();
  }
 catch (  IOException ioe) {
    _log.warn("Error checking",ioe);
    return false;
  }
  for (int i=0; i < 20; i++)   if (hash[i] != piece_hashes[20 * piece + i])   return false;
  return true;
}
