{
  if (bs != null) {
    din.readFully(bs,off,len);
  }
 else {
    ByteArray ba;
    byte[] tmp;
    if (len == BUFSIZE) {
      ba=_cache.acquire();
      tmp=ba.getData();
    }
 else {
      ba=null;
      tmp=new byte[len];
    }
    din.readFully(tmp);
synchronized (this) {
      if (raf == null)       createTemp();
      raf.seek(off);
      raf.write(tmp);
    }
    if (ba != null)     _cache.release(ba,false);
  }
}
