{
  this.my_id=my_id;
  this.infohash=infohash;
  this.metainfo=metainfo;
  this.sock=sock;
  byte[] id=handshake(in,out);
  this.peerID=new PeerID(id,sock.getPeerDestination());
  _id=++__id;
  if (_log.shouldLog(Log.DEBUG))   _log.debug("Creating a new peer with " + peerID.getAddress().calculateHash().toBase64(),new Exception("creating " + _id));
}
