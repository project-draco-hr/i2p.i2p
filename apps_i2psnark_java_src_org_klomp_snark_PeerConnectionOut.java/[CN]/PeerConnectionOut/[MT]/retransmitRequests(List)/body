{
  long now=System.currentTimeMillis();
  Iterator it=requests.iterator();
  while (it.hasNext()) {
    Request req=(Request)it.next();
    if (now > req.sendTime + REQ_TIMEOUT) {
      if (_log.shouldLog(Log.DEBUG))       _log.debug("Retransmit request " + req + " to peer "+ peer);
      sendRequest(req);
    }
  }
}
