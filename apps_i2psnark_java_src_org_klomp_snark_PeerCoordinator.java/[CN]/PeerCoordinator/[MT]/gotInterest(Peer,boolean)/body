{
  if (interest) {
synchronized (peers) {
      if (uploaders < MAX_UPLOADERS) {
        if (peer.isChoking()) {
          uploaders++;
          peer.setChoking(false);
          if (_log.shouldLog(Log.INFO))           _log.info("Unchoke: " + peer);
        }
      }
    }
  }
  if (listener != null)   listener.peerChange(this,peer);
}
