{
synchronized (this) {
    if (Snark.debug >= Snark.DEBUG)     Snark.debug(peer + " rcv bitfield",Snark.DEBUG);
    if (bitfield != null) {
      if (Snark.debug >= Snark.INFO)       Snark.debug("Got unexpected bitfield message from " + peer,Snark.INFO);
      return;
    }
    bitfield=new BitField(bitmap,metainfo.getPieces());
  }
  setInteresting(listener.gotBitField(peer,bitfield));
}
