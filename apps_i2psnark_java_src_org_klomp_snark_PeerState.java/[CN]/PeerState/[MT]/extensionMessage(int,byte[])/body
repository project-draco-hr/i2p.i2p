{
  if (id == 0) {
    InputStream is=new ByteArrayInputStream(bs);
    try {
      BDecoder dec=new BDecoder(is);
      BEValue bev=dec.bdecodeMap();
      Map map=bev.getMap();
      if (_log.shouldLog(Log.DEBUG))       _log.debug("Got extension handshake message " + bev.toString());
    }
 catch (    Exception e) {
      if (_log.shouldLog(Log.DEBUG))       _log.debug("Failed extension decode",e);
    }
  }
 else {
    if (_log.shouldLog(Log.DEBUG))     _log.debug("Got extended message type: " + id + " length: "+ bs.length);
  }
}
