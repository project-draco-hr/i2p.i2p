{
  int size=outstandingRequests.size();
  int[] arr=new int[size + 2];
  int pc=-1;
  int pos=0;
  if (pendingRequest != null) {
    pc=pendingRequest.piece;
    arr[pos++]=pc;
  }
  Request req=null;
  for (int i=0; i < size; i++) {
    Request r1=(Request)outstandingRequests.get(i);
    if (pc != r1.piece) {
      pc=r1.piece;
      arr[pos++]=pc;
    }
  }
  arr[pos]=-1;
  return (arr);
}
