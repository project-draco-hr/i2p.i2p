{
  if (Snark.debug >= Snark.DEBUG)   Snark.debug(peer + " rcv have(" + piece+ ")",Snark.DEBUG);
  if (piece < 0 || piece >= metainfo.getPieces()) {
    if (Snark.debug >= Snark.INFO)     Snark.debug("Got strange 'have: " + piece + "' message from "+ peer,+Snark.INFO);
    return;
  }
synchronized (this) {
    if (bitfield == null)     bitfield=new BitField(metainfo.getPieces());
    bitfield.set(piece);
  }
  if (listener.gotHave(peer,piece))   setInteresting(true);
}
