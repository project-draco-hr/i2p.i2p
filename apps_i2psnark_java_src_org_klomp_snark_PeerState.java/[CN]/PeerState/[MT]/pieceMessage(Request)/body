{
  int size=req.len;
  downloaded+=size;
  listener.downloaded(peer,size);
  if (getFirstOutstandingRequest(req.piece) == -1) {
    if (listener.gotPiece(peer,req.piece,req.bs)) {
      if (Snark.debug >= Snark.DEBUG)       Snark.debug("Got " + req.piece + ": "+ peer,Snark.DEBUG);
    }
 else {
      if (Snark.debug >= Snark.DEBUG)       Snark.debug("Got BAD " + req.piece + " from "+ peer,Snark.DEBUG);
      downloaded=0;
    }
  }
}
