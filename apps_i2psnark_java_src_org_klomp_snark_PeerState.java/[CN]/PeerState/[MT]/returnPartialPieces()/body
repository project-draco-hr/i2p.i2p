{
  Set<Integer> pcs=getRequestedPieces();
  List<Request> rv=new ArrayList<Request>(pcs.size());
  for (  Integer p : pcs) {
    Request req=getLowestOutstandingRequest(p.intValue());
    if (req != null) {
      req.getPartialPiece().setDownloaded(req.off);
      rv.add(req);
    }
  }
  outstandingRequests.clear();
  pendingRequest=null;
  lastRequest=null;
  return rv;
}
