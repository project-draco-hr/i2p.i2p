{
  Set<Integer> pcs=getRequestedPieces();
  List<PartialPiece> rv=new ArrayList(pcs.size());
  for (  Integer p : pcs) {
    Request req=getLowestOutstandingRequest(p.intValue());
    if (req != null)     rv.add(new PartialPiece(req));
  }
  outstandingRequests.clear();
  pendingRequest=null;
  lastRequest=null;
  return rv;
}
