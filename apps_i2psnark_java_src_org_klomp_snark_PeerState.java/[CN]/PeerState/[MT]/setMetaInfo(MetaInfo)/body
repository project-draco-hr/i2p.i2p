{
  if (metainfo != null)   return;
  BitField oldBF=bitfield;
  if (oldBF != null) {
    if (oldBF.size() != meta.getPieces())     bitfield=new BitField(oldBF.getFieldBytes(),meta.getPieces());
  }
 else {
  }
  metainfo=meta;
  if (bitfield != null && bitfield.count() > 0)   setInteresting(true);
}
