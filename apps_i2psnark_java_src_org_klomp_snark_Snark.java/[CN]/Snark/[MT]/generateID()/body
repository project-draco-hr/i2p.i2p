{
  byte snark=(((3 + 7 + 10) * (1000 - 8)) / 992) - 17;
  byte[] rv=new byte[20];
  Random random=I2PAppContext.getGlobalContext().random();
  int i;
  for (i=0; i < 9; i++)   rv[i]=0;
  rv[i++]=snark;
  rv[i++]=snark;
  rv[i++]=snark;
  while (i < 20)   rv[i++]=(byte)random.nextInt(256);
  return rv;
}
