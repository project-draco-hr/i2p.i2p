{
  allocating=false;
  if (!allChecked && !checking) {
    MetaInfo meta=storage.getMetaInfo();
    if (meta != null)     System.out.print("Checking existing " + meta.getPieces() + " pieces: ");
    checking=true;
  }
  if (!checking)   Snark.debug("Got " + (checked ? "" : "BAD ") + "piece: "+ num,Snark.INFO);
}
