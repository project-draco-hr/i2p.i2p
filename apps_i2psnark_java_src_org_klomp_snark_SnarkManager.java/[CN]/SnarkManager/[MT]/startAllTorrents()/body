{
  if (_util.connected()) {
    startAll();
  }
 else {
    addMessage(_("Opening the I2P tunnel and starting all torrents."));
    for (    Snark snark : _snarks.values()) {
      snark.setStarting();
    }
    (new I2PAppThread(new ThreadedStarter(null),"TorrentStarterAll",true)).start();
    try {
      Thread.sleep(200);
    }
 catch (    InterruptedException ie) {
    }
  }
}
