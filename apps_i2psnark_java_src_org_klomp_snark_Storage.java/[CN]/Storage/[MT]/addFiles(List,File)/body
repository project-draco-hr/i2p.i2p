{
  if (!f.isDirectory())   l.add(f);
 else {
    File[] files=f.listFiles();
    if (files == null) {
      if (_log.shouldLog(Log.WARN))       _log.warn("WARNING: Skipping '" + f + "' not a normal file.");
      return;
    }
    for (int i=0; i < files.length; i++)     addFiles(l,files[i]);
  }
}
