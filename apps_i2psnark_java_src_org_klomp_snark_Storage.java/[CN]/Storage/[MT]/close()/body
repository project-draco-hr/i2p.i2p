{
  if (rafs == null)   return;
  for (int i=0; i < rafs.length; i++) {
    if (RAFlock[i] == null)     continue;
    try {
synchronized (RAFlock[i]) {
        closeRAF(i);
      }
    }
 catch (    IOException ioe) {
      _util.debug("Error closing " + RAFfile[i],Snark.ERROR,ioe);
    }
  }
  changed=false;
}
