{
  if (!bitfield.get(piece))   return null;
  byte[] bs;
  try {
    bs=new byte[len];
  }
 catch (  OutOfMemoryError oom) {
    if (_log.shouldLog(Log.WARN))     _log.warn("Out of memory, can't honor request for piece " + piece,oom);
    return null;
  }
  getUncheckedPiece(piece,bs,off,len);
  return bs;
}
