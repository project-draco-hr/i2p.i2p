{
  if (complete() || metainfo.getFiles() == null || priorities == null)   return 0;
  for (int i=0; i < rafs.length; i++) {
    File f=RAFfile[i];
    if (f != null) {
      try {
        String canonical=f.getCanonicalPath();
        if (canonical.equals(file))         return priorities[i];
      }
 catch (      IOException ioe) {
      }
    }
  }
  return 0;
}
