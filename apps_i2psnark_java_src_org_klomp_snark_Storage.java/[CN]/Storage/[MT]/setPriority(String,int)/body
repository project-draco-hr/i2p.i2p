{
  if (complete() || metainfo.getFiles() == null)   return;
  for (  TorrentFile tf : _torrentFiles) {
    File f=tf.RAFfile;
    if (f != null) {
      try {
        String canonical=f.getCanonicalPath();
        if (canonical.equals(file)) {
          tf.priority=pri;
          return;
        }
      }
 catch (      IOException ioe) {
      }
    }
  }
}
