{
  if (complete() || metainfo.getFiles() == null || priorities == null)   return;
  for (int i=0; i < rafs.length; i++) {
    File f=RAFfile[i];
    if (f != null) {
      try {
        String canonical=f.getCanonicalPath();
        if (canonical.equals(file)) {
          priorities[i]=pri;
          return;
        }
      }
 catch (      IOException ioe) {
      }
    }
  }
}
