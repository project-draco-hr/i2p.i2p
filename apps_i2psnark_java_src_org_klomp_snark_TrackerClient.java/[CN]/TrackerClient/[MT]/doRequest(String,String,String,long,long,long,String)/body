{
  String s=announce + "?info_hash=" + infoHash+ "&peer_id="+ peerID+ "&port="+ port+ "&ip="+ I2PSnarkUtil.instance().getOurIPString()+ ".i2p"+ "&uploaded="+ uploaded+ "&downloaded="+ downloaded+ "&left="+ left+ ((event != NO_EVENT) ? ("&event=" + event) : "");
  Snark.debug("Sending TrackerClient request: " + s,Snark.INFO);
  File fetched=I2PSnarkUtil.instance().get(s);
  if (fetched == null) {
    throw new IOException("Error fetching " + s);
  }
  fetched.deleteOnExit();
  InputStream in=null;
  try {
    in=new FileInputStream(fetched);
    TrackerInfo info=new TrackerInfo(in,coordinator.getID(),coordinator.getMetaInfo());
    Snark.debug("TrackerClient response: " + info,Snark.INFO);
    lastRequestTime=System.currentTimeMillis();
    String failure=info.getFailureReason();
    if (failure != null)     throw new IOException(failure);
    interval=info.getInterval() * 1000;
    return info;
  }
  finally {
    if (in != null)     try {
      in.close();
    }
 catch (    IOException ioe) {
    }
    fetched.delete();
  }
}
