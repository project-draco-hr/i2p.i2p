{
  long begin=Clock.getInstance().now();
  if (_log.shouldLog(Log.DEBUG))   _log.debug("Start " + Thread.currentThread().getName());
  try {
    if (!_initialized) {
      setup();
      if (trackers.isEmpty()) {
        stop=true;
        return;
      }
      _initialized=true;
      long delay=I2PAppContext.getGlobalContext().random().nextInt(30 * 1000);
      try {
        Thread.sleep(delay);
      }
 catch (      InterruptedException ie) {
      }
    }
    loop();
  }
  finally {
    _thread=null;
    if (_log.shouldLog(Log.DEBUG))     _log.debug("Finish " + Thread.currentThread().getName() + " after "+ DataHelper.formatDuration(Clock.getInstance().now() - begin));
  }
}
