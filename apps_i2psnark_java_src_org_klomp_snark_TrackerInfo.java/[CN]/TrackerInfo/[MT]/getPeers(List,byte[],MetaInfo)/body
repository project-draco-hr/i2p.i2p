{
  Set peers=new HashSet(l.size());
  Iterator it=l.iterator();
  while (it.hasNext()) {
    PeerID peerID;
    try {
      peerID=new PeerID(((BEValue)it.next()).getMap());
    }
 catch (    InvalidBEncodingException ibe) {
      continue;
    }
    peers.add(new Peer(peerID,my_id,metainfo));
  }
  return peers;
}
