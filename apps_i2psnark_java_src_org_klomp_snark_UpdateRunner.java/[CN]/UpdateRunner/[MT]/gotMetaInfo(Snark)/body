{
  MetaInfo info=snark.getMetaInfo();
  if (info.getFiles() != null) {
    fatal("more than 1 file");
    return null;
  }
  if (info.isPrivate()) {
    fatal("private torrent");
    return null;
  }
  if (info.getTotalLength() > MAX_LENGTH) {
    fatal("too big");
    return null;
  }
  _hasMetaInfo=true;
  notifyProgress();
  return _smgr.gotMetaInfo(snark);
}
