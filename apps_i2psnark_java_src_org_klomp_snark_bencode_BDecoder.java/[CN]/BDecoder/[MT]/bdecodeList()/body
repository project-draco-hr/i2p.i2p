{
  int c=getNextIndicator();
  if (c != 'l')   throw new InvalidBEncodingException("Expected 'l', not '" + (char)c + "'");
  indicator=0;
  List result=new ArrayList();
  c=getNextIndicator();
  while (c != 'e') {
    result.add(bdecode());
    c=getNextIndicator();
  }
  indicator=0;
  return new BEValue(result);
}
