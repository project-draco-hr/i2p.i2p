{
  int c=getNextIndicator();
  if (c != 'd')   throw new InvalidBEncodingException("Expected 'd', not '" + (char)c + "'");
  indicator=0;
  Map result=new HashMap();
  c=getNextIndicator();
  while (c != 'e') {
    String key=bdecode().getString();
    boolean special=special_map.equals(key);
    if (special)     in_special_map=true;
    BEValue value=bdecode();
    result.put(key,value);
    if (special)     in_special_map=false;
    c=getNextIndicator();
  }
  indicator=0;
  return new BEValue(result);
}
