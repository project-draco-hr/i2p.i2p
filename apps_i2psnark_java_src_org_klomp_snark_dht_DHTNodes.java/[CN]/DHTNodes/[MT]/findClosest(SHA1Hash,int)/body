{
  Set<NID> all=new TreeSet(new SHA1Comparator(h));
  all.addAll(keySet());
  int sz=all.size();
  int max=Math.min(numWant,sz);
  List<NodeInfo> rv=new ArrayList(max);
  int count=0;
  for (  NID nid : all) {
    if (count++ >= max)     break;
    NodeInfo nInfo=get(nid);
    if (nInfo == null)     continue;
    rv.add(nInfo);
  }
  return rv;
}
