{
  Token token=new Token(tok);
  NID nid=_outgoingTokens.get(token);
  if (nid == null) {
    if (_log.shouldLog(Log.WARN))     _log.warn("Unknown token in announce_peer: " + token);
    if (_log.shouldLog(Log.INFO))     _log.info("Current known tokens: " + _outgoingTokens.keySet());
    return;
  }
  NodeInfo nInfo=_knownNodes.get(nid);
  if (nInfo == null) {
    if (_log.shouldLog(Log.WARN))     _log.warn("Unknown node in announce_peer for: " + nid);
    return;
  }
  if (_log.shouldLog(Log.INFO))   _log.info("Rcvd announce from: " + nInfo + " for: "+ ih);
  _tracker.announce(ih,nInfo.getHash());
  sendPong(nInfo,msgID);
}
