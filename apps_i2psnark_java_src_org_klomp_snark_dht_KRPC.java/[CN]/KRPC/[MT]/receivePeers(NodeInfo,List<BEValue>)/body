{
  if (_log.shouldLog(Log.INFO))   _log.info("Rcvd peers from: " + nInfo);
  List<Hash> rv=new ArrayList(peers.size());
  for (  BEValue bev : peers) {
    byte[] b=bev.getBytes();
    Hash h=new Hash(b);
    rv.add(h);
  }
  if (_log.shouldLog(Log.INFO))   _log.info("Rcvd peers from: " + nInfo + ": "+ DataHelper.toString(rv));
  return rv;
}
