{
  _isRunning=false;
  _session.removeListener(I2PSession.PROTO_DATAGRAM,_qPort);
  _session.removeListener(I2PSession.PROTO_DATAGRAM_RAW,_rPort);
  _tracker.stop();
  PersistDHT.saveDHT(_knownNodes,_dhtFile);
  _knownNodes.stop();
  _sentQueries.clear();
  _outgoingTokens.clear();
  _incomingTokens.clear();
}
