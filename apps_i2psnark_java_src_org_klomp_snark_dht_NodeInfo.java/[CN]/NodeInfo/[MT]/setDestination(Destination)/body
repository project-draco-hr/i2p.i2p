{
  if (!dest.calculateHash().equals(this.hash))   throw new IllegalArgumentException("Hash mismatch, was: " + this.hash + " new: "+ dest.calculateHash());
  if (this.dest == null)   this.dest=dest;
 else   if (!this.dest.equals(dest))   throw new IllegalArgumentException("Dest mismatch, was: " + this.dest + " new: "+ dest);
}
