{
  if (!KRPC.SECURE_NID)   return;
  byte[] nb=nID.getData();
  byte[] hb=hash.getData();
  if ((!DataHelper.eq(nb,2,hb,2,NID.HASH_LENGTH - 2)) || ((nb[0] ^ (port >> 8)) & 0xff) != (hb[0] & 0xff) || ((nb[1] ^ port) & 0xff) != (hb[1] & 0xff))   throw new IllegalArgumentException("NID/Hash mismatch");
}
