{
  ByteArray ba=_cache.acquire();
  byte[] buf=ba.getData();
  try {
    int read=0;
    while ((read=in.read(buf)) != -1) {
      out.write(buf,0,read);
    }
  }
  finally {
    _cache.release(ba,false);
    if (in != null)     try {
      in.close();
    }
 catch (    IOException ioe) {
    }
    if (out != null)     try {
      out.close();
    }
 catch (    IOException ioe) {
    }
  }
}
