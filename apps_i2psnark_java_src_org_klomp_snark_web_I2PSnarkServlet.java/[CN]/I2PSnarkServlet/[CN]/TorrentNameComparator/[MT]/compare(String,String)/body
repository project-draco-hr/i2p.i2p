{
  if (l.startsWith(skip))   l=l.substring(skip.length());
  if (r.startsWith(skip))   r=r.substring(skip.length());
  String llc=l.toLowerCase();
  if (llc.startsWith("the ") || llc.startsWith("the."))   l=l.substring(4);
  String rlc=r.toLowerCase();
  if (rlc.startsWith("the ") || rlc.startsWith("the."))   r=r.substring(4);
  return collator.compare(l,r);
}
