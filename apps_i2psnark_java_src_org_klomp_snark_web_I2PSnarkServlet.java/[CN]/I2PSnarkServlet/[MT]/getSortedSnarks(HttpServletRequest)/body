{
  ArrayList<Snark> rv=new ArrayList<Snark>(_manager.getTorrents());
  int sort=0;
  String ssort=req.getParameter("sort");
  if (ssort != null) {
    try {
      sort=Integer.parseInt(ssort);
    }
 catch (    NumberFormatException nfe) {
    }
  }
  Collections.sort(rv,Sorters.getComparator(sort));
  return rv;
}
