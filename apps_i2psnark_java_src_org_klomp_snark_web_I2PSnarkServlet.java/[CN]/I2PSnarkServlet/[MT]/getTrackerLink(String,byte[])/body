{
  String linkUrl=getTrackerLinkUrl(announce,infohash);
  if (linkUrl != null) {
    StringBuilder buf=new StringBuilder(128);
    buf.append(linkUrl).append("<img alt=\"").append(_("Info")).append("\" border=\"0\" src=\"").append(_imgPath).append("details.png\"></a>");
    return buf.toString();
  }
  return null;
}
