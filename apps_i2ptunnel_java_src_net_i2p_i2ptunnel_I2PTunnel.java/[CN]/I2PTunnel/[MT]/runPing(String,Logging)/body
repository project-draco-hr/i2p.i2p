{
  if (allargs.length() != 0) {
    _clientOptions.setProperty(I2Ping.PROP_COMMAND,allargs);
    I2PTunnelTask task=new I2Ping(l,ownDest,this,this);
    addtask(task);
    notifyEvent("pingTaskId",Integer.valueOf(task.getId()));
  }
 else {
    l.log(I2Ping.usage());
    notifyEvent("pingTaskId",Integer.valueOf(-1));
  }
}
