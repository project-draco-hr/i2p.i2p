{
  try {
    I2PClient client=I2PClientFactory.createClient();
    Destination d=new Destination();
    d.readBytes(readFrom);
    l.log("Public key: " + d.toBase64());
    readFrom.close();
    writePubKey(d,pubDest,l);
  }
 catch (  I2PException ex) {
    ex.printStackTrace();
  }
catch (  IOException ex) {
    ex.printStackTrace();
  }
}
