{
  boolean stillWaiting=false;
synchronized (_waitingSockets) {
    stillWaiting=_waitingSockets.remove(_s);
  }
  if (stillWaiting) {
    try {
      _s.close();
    }
 catch (    IOException ioe) {
    }
    if (_log.shouldLog(Log.INFO))     _log.info("Closed a waiting socket because of backlog");
  }
}
