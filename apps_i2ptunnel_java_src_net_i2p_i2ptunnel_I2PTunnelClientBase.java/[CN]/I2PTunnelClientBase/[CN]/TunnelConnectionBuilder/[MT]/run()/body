{
  Socket s=null;
  while (open) {
    try {
synchronized (_waitingSockets) {
        if (_waitingSockets.size() <= 0)         _waitingSockets.wait();
 else         s=(Socket)_waitingSockets.remove(0);
      }
    }
 catch (    InterruptedException ie) {
    }
    if (s != null)     clientConnectionRun(s);
    s=null;
  }
}
