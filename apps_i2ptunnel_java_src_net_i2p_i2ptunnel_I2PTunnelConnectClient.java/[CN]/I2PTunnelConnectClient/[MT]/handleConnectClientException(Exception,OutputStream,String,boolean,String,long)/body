{
  if (out == null)   return;
  try {
    String str;
    byte[] header;
    if (usingWWWProxy)     str=FileUtil.readTextFile((new File(_errorDir,"dnfp-header.ht")).getAbsolutePath(),100,true);
 else     str=FileUtil.readTextFile((new File(_errorDir,"dnf-header.ht")).getAbsolutePath(),100,true);
    if (str != null)     header=str.getBytes();
 else     header=ERR_DESTINATION_UNKNOWN;
    writeErrorMessage(header,out,targetRequest,usingWWWProxy,wwwProxy);
  }
 catch (  IOException ioe) {
  }
}
