{
  if (out != null) {
    out.write(errMessage);
    if (targetRequest != null) {
      out.write(targetRequest.getBytes());
      if (usingWWWProxy)       out.write(("<br />WWW proxy: " + wwwProxy).getBytes());
    }
    out.write("</div>".getBytes());
    out.write("\n</body></html>\n".getBytes());
    out.flush();
  }
}
