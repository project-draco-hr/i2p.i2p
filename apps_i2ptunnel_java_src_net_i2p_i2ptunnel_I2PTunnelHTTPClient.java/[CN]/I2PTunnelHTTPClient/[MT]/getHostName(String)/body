{
  if (host == null) {
    return null;
  }
  if (host.length() == 60 && host.toLowerCase(Locale.US).endsWith(".b32.i2p")) {
    return host;
  }
  Destination _dest=_context.namingService().lookup(host);
  if (_dest == null) {
    return "i2p";
  }
  return Base32.encode(_dest.calculateHash().getData()) + ".b32.i2p";
}
