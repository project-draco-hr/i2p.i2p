{
  if (_log.shouldLog(Log.WARN))   _log.warn(getPrefix(requestId) + "Error sending to " + wwwProxy+ " (proxy? "+ usingWWWProxy+ ", request: "+ targetRequest,ex);
  if (out != null) {
    try {
      String str;
      byte[] header;
      if (usingWWWProxy)       str=FileUtil.readTextFile("docs/dnfp-header.ht",100,true);
 else       str=FileUtil.readTextFile("docs/dnf-header.ht",100,true);
      if (str != null)       header=str.getBytes();
 else       header=ERR_DESTINATION_UNKNOWN;
      writeErrorMessage(header,out,targetRequest,usingWWWProxy,wwwProxy);
    }
 catch (    IOException ioe) {
      _log.warn(getPrefix(requestId) + "Error writing out the 'destination was unknown' " + "message",ioe);
    }
  }
 else {
    _log.warn(getPrefix(requestId) + "Client disconnected before we could say that destination " + "was unknown",ex);
  }
}
