{
  while (open) {
    try {
      handle(_serverSocket.accept());
    }
 catch (    I2PException ex) {
      _log.error("Error while waiting for I2PConnections",ex);
      return;
    }
catch (    IOException ex) {
      _log.error("Error while waiting for I2PConnections",ex);
      return;
    }
  }
}
