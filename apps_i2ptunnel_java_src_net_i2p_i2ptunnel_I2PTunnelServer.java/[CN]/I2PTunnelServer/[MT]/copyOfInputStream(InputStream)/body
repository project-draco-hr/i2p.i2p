{
  byte[] buf=new byte[128];
  ByteArrayOutputStream os=new ByteArrayOutputStream(768);
  try {
    int read;
    while ((read=is.read(buf)) >= 0) {
      os.write(buf,0,read);
    }
  }
  finally {
    try {
      is.close();
    }
 catch (    IOException ioe) {
    }
  }
  return new ByteArrayInputStream(os.toByteArray());
}
