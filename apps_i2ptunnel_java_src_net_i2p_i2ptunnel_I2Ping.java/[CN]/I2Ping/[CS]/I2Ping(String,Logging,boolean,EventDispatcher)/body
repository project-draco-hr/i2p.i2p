{
  super("I2Ping [" + cmd + "]",notifyThis);
  this.l=l;
  command=cmd;
synchronized (slock) {
    if (ownDest) {
      sockMgr=I2PTunnelClient.buildSocketManager();
    }
 else {
      sockMgr=I2PTunnelClient.getSocketManager();
    }
  }
  Thread t=new Thread(this);
