{
  super("I2Ping [" + cmd + "]",notifyThis,tunnel);
  this.l=l;
  command=cmd;
  if (ownDest) {
    sockMgr=I2PTunnelClient.buildSocketManager(tunnel);
  }
 else {
    sockMgr=I2PTunnelClient.getSocketManager(tunnel);
  }
  Thread t=new I2PAppThread(this);
  t.setName("Client");
  t.start();
  open=true;
}
