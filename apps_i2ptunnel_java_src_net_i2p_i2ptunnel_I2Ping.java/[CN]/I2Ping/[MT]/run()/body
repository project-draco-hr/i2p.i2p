{
  l.log("*** I2Ping results:");
  try {
    runCommand(command);
  }
 catch (  InterruptedException ex) {
    l.log("*** Interrupted");
    _log.error("Pinger interrupted",ex);
  }
catch (  IOException ex) {
    _log.error("Pinger exception",ex);
  }
  l.log("*** Finished.");
synchronized (lock) {
    finished=true;
  }
  close(false);
}
