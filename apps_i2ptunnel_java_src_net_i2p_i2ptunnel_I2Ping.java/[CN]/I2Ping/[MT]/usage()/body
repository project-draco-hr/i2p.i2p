  return "ping <opts> <b64dest|host>\n" + "ping <opts> -h (pings all hosts in hosts.txt)\n" + "ping <opts> -l <destlistfile> (pings a list of hosts in a file)\n"+ "Options:\n"+ "     -c (require 5 consecutive pings to report success)\n"+ "     -m maxSimultaneousPings (default 10)\n"+ "     -n numberOfPings (default 3)\n"+ "     -t timeout (ms, default 30000)\n"+ "     -f fromPort\n"+ "     -p toPort";
