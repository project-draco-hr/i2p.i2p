{
  List sessions=_tunnel.getSessions();
  if (sessions != null) {
    for (int i=0; i < sessions.size(); i++) {
      I2PSession session=(I2PSession)sessions.get(i);
      TunnelControllerGroup.getInstance().acquire(this,session);
    }
    _sessions=sessions;
  }
 else {
    _log.error("No sessions to acquire?");
  }
}
