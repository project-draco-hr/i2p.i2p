{
  if (_tunnel != null) {
    List sessions=_tunnel.getSessions();
    for (int i=0; i < sessions.size(); i++) {
      I2PSession session=(I2PSession)sessions.get(i);
      Destination dest=session.getMyDestination();
      if (dest != null)       return dest.toBase64();
    }
  }
  return null;
}
