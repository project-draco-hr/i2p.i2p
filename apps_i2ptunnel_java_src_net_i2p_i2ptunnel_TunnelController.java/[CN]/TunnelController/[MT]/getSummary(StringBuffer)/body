{
  String type=getType();
  if ("httpclient".equals(type))   getHttpClientSummary(buf);
 else   if ("client".equals(type))   getClientSummary(buf);
 else   if ("server".equals(type))   getServerSummary(buf);
 else   if ("httpserver".equals(type))   getHttpServerSummary(buf);
 else   buf.append("Unknown type ").append(type);
}
