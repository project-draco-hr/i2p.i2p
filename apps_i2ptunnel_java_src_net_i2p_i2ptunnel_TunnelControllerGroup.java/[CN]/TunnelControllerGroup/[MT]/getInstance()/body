{
synchronized (TunnelControllerGroup.class) {
    if (_instance == null) {
      I2PAppContext ctx=I2PAppContext.getGlobalContext();
      if (SystemVersion.isAndroid() || !ctx.isRouterContext()) {
        _instance=new TunnelControllerGroup(ctx,null,null);
        _instance.startup();
      }
    }
    return _instance;
  }
}
