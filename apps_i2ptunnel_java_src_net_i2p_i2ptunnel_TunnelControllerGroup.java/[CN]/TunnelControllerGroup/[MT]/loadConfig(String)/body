{
  File cfgFile=new File(configFile);
  if (!cfgFile.exists()) {
    if (_log.shouldLog(Log.ERROR))     _log.error("Unable to load the controllers from " + configFile);
    return null;
  }
  Properties props=new Properties();
  try {
    DataHelper.loadProps(props,cfgFile);
    return props;
  }
 catch (  IOException ioe) {
    if (_log.shouldLog(Log.ERROR))     _log.error("Error reading the controllers from " + configFile,ioe);
    return null;
  }
}
