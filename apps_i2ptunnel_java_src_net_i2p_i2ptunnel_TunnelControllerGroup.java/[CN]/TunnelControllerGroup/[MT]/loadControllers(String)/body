{
  Properties cfg=loadConfig(configFile);
  if (cfg == null) {
    if (_log.shouldLog(Log.WARN))     _log.warn("Unable to load the config from " + configFile);
    return;
  }
  int i=0;
  while (true) {
    String type=cfg.getProperty("tunnel." + i + ".type");
    if (type == null)     break;
    TunnelController controller=new TunnelController(cfg,"tunnel." + i + ".");
    _controllers.add(controller);
    i++;
  }
  I2PThread startupThread=new I2PThread(new StartControllers(),"Startup tunnels");
  startupThread.start();
  if (_log.shouldLog(Log.INFO))   _log.info(i + " controllers loaded from " + configFile);
}
