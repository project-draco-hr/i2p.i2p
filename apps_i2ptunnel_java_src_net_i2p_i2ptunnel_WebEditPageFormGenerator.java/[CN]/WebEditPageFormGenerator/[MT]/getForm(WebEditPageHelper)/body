{
  TunnelController controller=helper.getTunnelController();
  if ((helper.getType() == null) && (controller == null))   return SELECT_TYPE_FORM;
  String id=helper.getNum();
  String type=helper.getType();
  if (controller != null)   type=controller.getType();
  if ("httpclient".equals(type))   return getEditHttpClientForm(controller,id);
 else   if ("client".equals(type))   return getEditClientForm(controller,id);
 else   if ("server".equals(type))   return getEditServerForm(controller,id);
 else   if ("httpserver".equals(type))   return getEditHttpServerForm(controller,id);
 else   return "WTF, unknown type [" + type + "]";
}
