{
  if (msgs == null)   return "";
  int num=msgs.size();
switch (num) {
case 0:
    return "";
case 1:
  return (String)msgs.get(0);
default :
StringBuffer buf=new StringBuffer(512);
buf.append("<ul>");
for (int i=0; i < num; i++) buf.append("<li>").append((String)msgs.get(i)).append("</li>\n");
buf.append("</ul>\n");
return buf.toString();
}
}
