{
  if (_id == null)   return null;
  int id=-1;
  try {
    id=Integer.parseInt(_id);
    List controllers=TunnelControllerGroup.getInstance().getControllers();
    if ((id < 0) || (id >= controllers.size()))     return null;
 else     return (TunnelController)controllers.get(id);
  }
 catch (  NumberFormatException nfe) {
    if (_log.shouldLog(Log.WARN))     _log.warn("Invalid tunnel id [" + _id + "]",nfe);
    return null;
  }
}
