{
  if ((_action == null) || (_action.trim().length() <= 0))   return "";
  String expected=System.getProperty(getClass().getName() + ".nonce");
  if ((expected == null) || (!expected.equals(Long.toString(_nonce))))   return "<b>Invalid nonce, are you being spoofed?</b>";
  if ("Save".equals(_action))   return save();
 else   if ("Remove".equals(_action))   return remove();
 else   return "Action <i>" + _action + "</i> unknown";
}
