{
  if (setupCompleted) {
    return;
  }
  DataInputStream in;
  DataOutputStream out;
  try {
    in=new DataInputStream(clientSock.getInputStream());
    out=new DataOutputStream(clientSock.getOutputStream());
    init(in,out);
    manageRequest(in,out);
  }
 catch (  IOException e) {
    throw new SOCKSException("Connection error (" + e.getMessage() + ")");
  }
  setupCompleted=true;
}
