{
  byte[] payload;
  if (!this.raw) {
synchronized (this.maker) {
      payload=this.maker.makeI2PDatagram(data);
    }
  }
 else   payload=data;
  try {
    this.sess.sendMessage(this.dest,payload,(this.raw ? I2PSession.PROTO_DATAGRAM_RAW : I2PSession.PROTO_DATAGRAM),I2PSession.PORT_UNSPECIFIED,I2PSession.PORT_UNSPECIFIED);
  }
 catch (  I2PSessionException ise) {
    throw new RuntimeException("failed to send data",ise);
  }
}
