{
  byte[] payload;
  if (!this.raw) {
synchronized (this.maker) {
      payload=this.maker.makeI2PDatagram(data);
    }
  }
 else   payload=data;
  try {
    this.sess.sendMessage(to,payload,I2PSession.PROTO_DATAGRAM,I2PSession.PORT_UNSPECIFIED,I2PSession.PORT_UNSPECIFIED);
  }
 catch (  I2PSessionException exc) {
    exc.printStackTrace();
  }
}
