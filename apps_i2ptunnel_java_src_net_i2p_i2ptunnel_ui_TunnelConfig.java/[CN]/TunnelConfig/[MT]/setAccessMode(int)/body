{
switch (mode) {
case 1:
    _booleanOptions.add(PROP_ENABLE_ACCESS_LIST);
  _booleanOptions.remove(PROP_ENABLE_BLACKLIST);
break;
case 2:
_booleanOptions.remove(PROP_ENABLE_ACCESS_LIST);
_booleanOptions.add(PROP_ENABLE_BLACKLIST);
break;
default :
_booleanOptions.remove(PROP_ENABLE_ACCESS_LIST);
_booleanOptions.remove(PROP_ENABLE_BLACKLIST);
}
}
