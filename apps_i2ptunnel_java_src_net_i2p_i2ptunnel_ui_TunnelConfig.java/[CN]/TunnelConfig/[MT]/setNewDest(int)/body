{
switch (mode) {
case 1:
    _booleanOptions.add("i2cp.newDestOnResume");
  _booleanOptions.remove("persistentClientKey");
break;
case 2:
_booleanOptions.remove("i2cp.newDestOnResume");
_booleanOptions.add("persistentClientKey");
break;
default :
_booleanOptions.remove("i2cp.newDestOnResume");
_booleanOptions.remove("persistentClientKey");
}
}
