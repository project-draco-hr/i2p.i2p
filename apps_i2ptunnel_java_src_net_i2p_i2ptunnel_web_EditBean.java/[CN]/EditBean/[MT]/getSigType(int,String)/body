{
  SigType type;
  String ttype;
  boolean isShared;
  if (tunnel >= 0) {
    String stype=getProperty(tunnel,I2PClient.PROP_SIGTYPE,null);
    type=stype != null ? SigType.parseSigType(stype) : null;
    ttype=getTunnelType(tunnel);
    isShared=isSharedClient(tunnel);
  }
 else {
    type=null;
    ttype=newTunnelType;
    isShared=false;
  }
  if (type == null) {
    if ((TunnelController.TYPE_IRC_CLIENT.equals(ttype) || TunnelController.TYPE_SOCKS_IRC.equals(ttype) || TunnelController.TYPE_STD_CLIENT.equals(ttype)) && !isShared && SigType.ECDSA_SHA256_P256.isAvailable())     type=SigType.ECDSA_SHA256_P256;
 else     type=SigType.DSA_SHA1;
  }
  return type.getCode();
}
