{
  TunnelController tun=getController(tunnel);
  if (tun != null) {
    Properties opts=getOptions(tun);
    if (opts != null) {
      String wsiz=opts.getProperty("i2p.streaming.maxWindowSize");
      if ((wsiz == null) || (!"1".equals(wsiz)))       return false;
 else       return true;
    }
 else {
      return false;
    }
  }
 else {
    return false;
  }
}
