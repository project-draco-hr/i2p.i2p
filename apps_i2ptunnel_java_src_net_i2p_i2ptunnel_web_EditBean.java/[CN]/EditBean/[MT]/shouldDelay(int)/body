{
  TunnelController tun=getController(tunnel);
  if (tun != null) {
    Properties opts=getOptions(tun);
    if (opts != null) {
      String delay=opts.getProperty("i2p.streaming.connectDelay");
      if ((delay == null) || ("0".equals(delay)))       return false;
 else       return true;
    }
 else {
      return false;
    }
  }
 else {
    return false;
  }
}
