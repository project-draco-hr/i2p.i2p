{
  List<String> rv=_group.clearAllMessages();
  try {
    _group.saveConfig();
    rv.add(0,_("Configuration changes saved"));
  }
 catch (  IOException ioe) {
    _log.error("Failed to save config file",ioe);
    rv.add(0,_("Failed to save configuration") + ": " + ioe.toString());
  }
  return rv;
}
