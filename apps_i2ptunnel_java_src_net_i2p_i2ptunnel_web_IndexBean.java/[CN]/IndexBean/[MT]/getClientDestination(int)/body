{
  TunnelController tun=getController(tunnel);
  if (tun == null)   return "";
  String rv;
  if ("client".equals(tun.getType()) || "ircclient".equals(tun.getType()) || "streamrclient".equals(tun.getType()))   rv=tun.getTargetDestination();
 else   rv=tun.getProxyList();
  return rv != null ? rv : "";
}
