{
  TunnelController tun=getController(tunnel);
  if (tun == null)   return "";
  if ("client".equals(tun.getType()) || "ircclient".equals(tun.getType())) {
    if (tun.getTargetDestination() != null)     return tun.getTargetDestination();
 else     return "";
  }
 else   return tun.getProxyList();
}
