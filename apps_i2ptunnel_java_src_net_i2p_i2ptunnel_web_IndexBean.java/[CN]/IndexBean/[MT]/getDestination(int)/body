{
  TunnelController tun=getController(tunnel);
  if (tun != null) {
    Destination rv=tun.getDestination();
    if (rv != null)     return rv;
    File keyFile=tun.getPrivateKeyFile();
    if (keyFile != null) {
      PrivateKeyFile pkf=new PrivateKeyFile(keyFile);
      try {
        rv=pkf.getDestination();
        if (rv != null)         return rv;
      }
 catch (      Exception e) {
      }
    }
  }
  return null;
}
