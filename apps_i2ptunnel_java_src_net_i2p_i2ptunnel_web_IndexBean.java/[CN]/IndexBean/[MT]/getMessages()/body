{
  if (_group == null)   return _fatalError;
  StringBuilder buf=new StringBuilder(512);
  if (_action != null) {
    try {
      buf.append(processAction()).append("\n");
    }
 catch (    Exception e) {
      _log.log(Log.CRIT,"Error processing " + _action,e);
    }
  }
  getMessages(_group.clearAllMessages(),buf);
  return buf.toString();
}
