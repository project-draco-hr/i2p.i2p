{
  return ((TunnelController.TYPE_STD_CLIENT.equals(type)) || (TunnelController.TYPE_HTTP_CLIENT.equals(type)) || (TunnelController.TYPE_SOCKS.equals(type))|| (TunnelController.TYPE_SOCKS_IRC.equals(type))|| (TunnelController.TYPE_CONNECT.equals(type))|| (TunnelController.TYPE_STREAMR_CLIENT.equals(type))|| (TunnelController.TYPE_IRC_CLIENT.equals(type)));
}
