{
  if ((_action == null) || (_action.trim().length() <= 0))   return "";
  if ((_prevNonce != _curNonce) && (!validPassphrase(_passphrase)))   return "Invalid nonce, are you being spoofed?";
  if ("Stop all tunnels".equals(_action))   return stopAll();
 else   if ("Start all tunnels".equals(_action))   return startAll();
 else   if ("Restart all".equals(_action))   return restartAll();
 else   if ("Reload config".equals(_action))   return reloadConfig();
 else   if ("stop".equals(_action))   return stop();
 else   if ("start".equals(_action))   return start();
 else   if ("Save changes".equals(_action))   return saveChanges();
 else   if ("Delete this proxy".equals(_action))   return deleteTunnel();
 else   return "Action " + _action + " unknown";
}
