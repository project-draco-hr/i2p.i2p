{
  if ((count - offset) >= nonceCount || (nonceCount > count - offset + seen.length)) {
    return false;
  }
  int checkIndex=(int)((nonceCount + offset) % seen.length);
  if (seen[checkIndex]) {
    return false;
  }
 else {
    seen[checkIndex]=true;
    seen[count % seen.length]=false;
    count++;
    return true;
  }
}
