{
  if (welcomeFile.startsWith("/") || welcomeFile.startsWith(java.io.File.separator) || welcomeFile.endsWith("/")|| welcomeFile.endsWith(java.io.File.separator))   log.warn("Invalid welcome file: " + welcomeFile);
  List list=new ArrayList(Arrays.asList(_welcomes));
  list.add(welcomeFile);
  _welcomes=(String[])list.toArray(_welcomes);
}
