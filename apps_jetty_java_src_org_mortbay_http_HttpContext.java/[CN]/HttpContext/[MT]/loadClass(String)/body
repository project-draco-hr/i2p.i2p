{
  if (_loader == null) {
    try {
      initClassLoader(false);
    }
 catch (    Exception e) {
      log.warn(LogSupport.EXCEPTION,e);
      return null;
    }
  }
  if (className == null)   return null;
  if (_loader == null)   return Class.forName(className);
  return _loader.loadClass(className);
}
