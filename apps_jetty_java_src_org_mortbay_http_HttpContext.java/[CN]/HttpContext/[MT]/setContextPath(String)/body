{
  if (_httpServer != null)   _httpServer.removeMappings(this);
  contextPathSpec=canonicalContextPathSpec(contextPathSpec);
  if (contextPathSpec.length() > 1)   _contextPath=contextPathSpec.substring(0,contextPathSpec.length() - 2);
 else   _contextPath="/";
  _contextName=null;
  if (_httpServer != null)   _httpServer.addMappings(this);
}
