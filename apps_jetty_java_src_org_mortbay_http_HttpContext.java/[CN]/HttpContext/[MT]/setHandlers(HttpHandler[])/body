{
  List old=new ArrayList(_handlers);
  if (handlers != null) {
    for (int i=0; i < handlers.length; i++) {
      boolean existing=old.remove(handlers[i]);
      if (!existing)       addHandler(handlers[i]);
    }
  }
  for (int i=0; i < old.size(); i++)   removeHandler((HttpHandler)old.get(i));
}
