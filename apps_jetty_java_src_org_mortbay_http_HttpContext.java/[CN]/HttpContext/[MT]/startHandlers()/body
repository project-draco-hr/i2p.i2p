{
  MultiException mx=new MultiException();
  Iterator handlers=_handlers.iterator();
  while (handlers.hasNext()) {
    HttpHandler handler=(HttpHandler)handlers.next();
    if (!handler.isStarted())     try {
      handler.start();
    }
 catch (    Exception e) {
      mx.add(e);
    }
  }
  mx.ifExceptionThrow();
}
