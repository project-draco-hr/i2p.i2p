{
  String referer=request.getField(HttpFields.__Referer);
  if (referer == null)   log.write("\"-\" ");
 else {
    log.write('"');
    log.write(referer);
    log.write("\" ");
  }
  String agent=request.getField(HttpFields.__UserAgent);
  if (agent == null)   log.write("\"-\"");
 else {
    log.write('"');
    log.write(agent);
    log.write('"');
  }
}
