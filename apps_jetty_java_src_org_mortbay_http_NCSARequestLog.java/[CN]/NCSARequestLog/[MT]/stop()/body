{
  try {
    if (_writer != null)     _writer.flush();
  }
 catch (  IOException e) {
    LogSupport.ignore(log,e);
  }
  if (_out != null && _closeOut)   try {
    _out.close();
  }
 catch (  IOException e) {
    LogSupport.ignore(log,e);
  }
  _out=null;
  _fileOut=null;
  _closeOut=false;
  _logDateCache=null;
  _writer=null;
}
