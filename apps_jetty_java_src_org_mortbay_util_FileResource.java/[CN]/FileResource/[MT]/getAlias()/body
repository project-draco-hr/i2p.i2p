{
  if (__checkAliases) {
    if (!_aliasChecked) {
      try {
        String abs=_file.getAbsolutePath();
        String can=_file.getCanonicalPath();
        if (abs.length() != can.length() || !abs.equals(can))         _alias=new File(can).toURI().toURL();
        _aliasChecked=true;
        if (_alias != null && log.isDebugEnabled()) {
          log.debug("ALIAS abs=" + abs);
          log.debug("ALIAS can=" + can);
        }
      }
 catch (      Exception e) {
        log.warn(LogSupport.EXCEPTION,e);
        return getURL();
      }
    }
  }
 else   return null;
  return _alias;
}
