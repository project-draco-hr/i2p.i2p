{
  StringBuffer sb=new StringBuffer("[CDPStatusBlock: OFFSET=0x");
  sb.append(Long.toHexString(offset));
  sb.append(", SIZE=0x");
  sb.append(Long.toHexString(size));
  sb.append(", unknownDatapoints=");
  sb.append(unknownDatapoints);
  sb.append(", value=");
  sb.append(value);
  sb.append("]");
  return sb.toString();
}
