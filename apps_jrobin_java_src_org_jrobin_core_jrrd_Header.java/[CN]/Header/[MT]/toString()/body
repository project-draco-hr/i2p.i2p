{
  StringBuffer sb=new StringBuffer("[Header: OFFSET=0x00, SIZE=0x");
  sb.append(Long.toHexString(size));
  sb.append(", version=");
  sb.append(version);
  sb.append(", dsCount=");
  sb.append(dsCount);
  sb.append(", rraCount=");
  sb.append(rraCount);
  sb.append(", pdpStep=");
  sb.append(pdpStep);
  sb.append("]");
  return sb.toString();
}
