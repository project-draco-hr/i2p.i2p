{
  StringBuffer sb=new StringBuffer("[PDPStatus: OFFSET=0x");
  sb.append(Long.toHexString(offset));
  sb.append(", SIZE=0x");
  sb.append(Long.toHexString(size));
  sb.append(", lastReading=");
  sb.append(lastReading);
  sb.append(", unknownSeconds=");
  sb.append(unknownSeconds);
  sb.append(", value=");
  sb.append(value);
  sb.append("]");
  return sb.toString();
}
