{
  if (_log.shouldLog(Log.DEBUG))   _log.debug("accept() called, pending: " + pendingSockets.size());
  I2PSocket ret=null;
  while ((ret == null) && (!closing)) {
    this.waitIncoming(-1);
    ret=accept(false);
  }
  if (_log.shouldLog(Log.DEBUG))   _log.debug("TIMING: handed out accept result " + ret.hashCode());
  return ret;
}
