{
  I2PSocketImpl s=null;
synchronized (lock) {
    s=(I2PSocketImpl)_outSockets.get(id);
  }
  _log.debug(getName() + ": *Disconnect outgoing for socket " + s+ " on id "+ getReadableForm(id));
  try {
    if (s != null) {
      if (payload.length > 0) {
        _log.debug(getName() + ": Disconnect packet had " + payload.length+ " bytes");
      }
      if (s.getRemoteID(false) == null) {
        s.setRemoteID(null);
        return;
      }
      s.internalClose();
synchronized (lock) {
        _outSockets.remove(id);
      }
    }
    return;
  }
 catch (  Exception t) {
    _log.error(getName() + ": Ignoring error on disconnect for socket " + s,t);
  }
}
