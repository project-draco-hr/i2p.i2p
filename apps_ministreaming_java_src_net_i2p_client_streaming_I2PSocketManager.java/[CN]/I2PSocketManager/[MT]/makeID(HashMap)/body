{
  String newID;
  try {
    do {
      int id=(int)(Math.random() * 16777215 + 1);
      byte[] nid=new byte[3];
      nid[0]=(byte)(id / 65536);
      nid[1]=(byte)((id / 256) % 256);
      nid[2]=(byte)(id % 256);
      newID=new String(nid,"ISO-8859-1");
    }
 while (uniqueIn.get(newID) != null);
    return newID;
  }
 catch (  UnsupportedEncodingException ex) {
    ex.printStackTrace();
    return null;
  }
}
