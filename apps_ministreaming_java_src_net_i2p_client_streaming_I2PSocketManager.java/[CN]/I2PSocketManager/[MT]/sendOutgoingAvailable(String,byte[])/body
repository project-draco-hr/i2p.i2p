{
  I2PSocketImpl s=null;
synchronized (lock) {
    s=(I2PSocketImpl)_outSockets.get(id);
  }
  if (_log.shouldLog(Log.DEBUG))   _log.debug(getName() + ": *Packet send outgoing [" + payload.length+ "] for socket "+ s);
  if (s != null) {
    s.queueData(payload);
    return;
  }
 else {
    if (_log.shouldLog(Log.WARN))     _log.warn(getName() + ": Null socket with data available");
    throw new IllegalStateException("Null socket with data available");
  }
}
