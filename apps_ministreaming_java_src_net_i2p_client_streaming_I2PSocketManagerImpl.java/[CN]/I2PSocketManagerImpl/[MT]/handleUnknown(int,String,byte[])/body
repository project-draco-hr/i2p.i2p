{
  _log.error(getName() + ": \n\n=============== Unknown packet! " + "============"+ "\nType: "+ (int)type+ "\nID:   "+ getReadableForm(id)+ "\nBase64'ed Data: "+ Base64.encode(payload)+ "\n\n\n");
  if (id != null) {
synchronized (lock) {
      _inSockets.remove(id);
      _outSockets.remove(id);
    }
  }
}
