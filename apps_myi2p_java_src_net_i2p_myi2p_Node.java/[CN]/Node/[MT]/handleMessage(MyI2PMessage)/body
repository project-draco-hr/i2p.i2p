{
  Service service=(Service)_services.get(msg.getServiceType());
  if (service == null) {
    if (_log.shouldLog(Log.ERROR))     _log.error("Message received for an unknown service [" + msg.getServiceType() + "] from "+ msg.getPeer().calculateHash().toBase64());
  }
 else {
    service.receiveMessage(msg);
  }
}
