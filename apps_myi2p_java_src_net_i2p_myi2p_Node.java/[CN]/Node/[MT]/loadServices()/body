{
  Properties config=getConfig();
  int i=0;
  while (true) {
    String classname=config.getProperty("service." + i + ".classname");
    if ((classname == null) || (classname.trim().length() <= 0))     break;
    boolean ok=loadService("service." + i + ".",config);
    if (ok)     i++;
  }
  if (_log.shouldLog(Log.INFO))   _log.info(i + " services loaded");
}
