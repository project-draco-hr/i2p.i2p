{
  String filename=DEFAULT_CONFIG_FILE;
  if ((args != null) && (args.length == 1))   filename=args[0];
  Node node=new Node(I2PAppContext.getGlobalContext());
  node.setConfigFile(filename);
  node.loadConfig();
  node.startup();
  while (true) {
synchronized (node) {
      try {
        node.wait();
      }
 catch (      InterruptedException ie) {
      }
    }
  }
}
