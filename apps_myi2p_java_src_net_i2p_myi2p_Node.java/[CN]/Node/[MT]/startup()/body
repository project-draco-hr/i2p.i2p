{
  boolean connected=connect();
  if (connected) {
    Runtime.getRuntime().addShutdownHook(new Thread(new Runnable(){
      public void run(){
        shutdown();
      }
    }
));
    loadServices();
    startServices();
synchronized (_nodes) {
      _nodes.add(this);
    }
    if (_log.shouldLog(Log.INFO))     _log.info("Node started");
  }
 else {
    if (_log.shouldLog(Log.ERROR))     _log.error("Unable to connect, startup didn't do much");
  }
}
