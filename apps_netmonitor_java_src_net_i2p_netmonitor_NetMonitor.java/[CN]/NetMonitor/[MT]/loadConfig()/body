{
  Properties props=new Properties();
  FileInputStream fis=null;
  try {
    fis=new FileInputStream(_configLocation);
    props.load(fis);
  }
 catch (  IOException ioe) {
    _log.warn("Error loading the net monitor config",ioe);
  }
 finally {
    if (fis != null)     try {
      fis.close();
    }
 catch (    IOException ioe) {
    }
  }
  parseConfig(props);
}
