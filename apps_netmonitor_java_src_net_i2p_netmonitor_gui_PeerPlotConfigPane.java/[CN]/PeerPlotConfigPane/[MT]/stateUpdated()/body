{
  for (int i=0; i < _options.length; i++) {
    for (int j=0; j < _options[i]._statAttributes.size(); j++) {
      JCheckBox box=(JCheckBox)_options[i]._statAttributes.get(j);
      if (_config.getSeriesConfig().getShouldPlotValue(_options[i].getStatName(),box.getText(),false))       box.setSelected(true);
 else       box.setSelected(false);
    }
  }
}
