{
  File identDir=getIdentDir(target);
  if (identDir.exists()) {
    File identFile=new File(identDir,"identity.dat");
    if (identFile.exists()) {
      RouterIdentity ident=new RouterIdentity();
      ident.readBytes(new FileInputStream(identFile));
      return ident;
    }
 else {
      return null;
    }
  }
 else {
    return null;
  }
}
