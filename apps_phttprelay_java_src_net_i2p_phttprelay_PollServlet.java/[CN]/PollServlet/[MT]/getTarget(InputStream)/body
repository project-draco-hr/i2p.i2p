{
  StringBuffer buf=new StringBuffer(64);
  int numBytes=0;
  int c=0;
  while ((c=in.read()) != -1) {
    if (c == (int)'&')     break;
    buf.append((char)c);
    numBytes++;
    if (numBytes > 128) {
      log("Target didn't find the & after 128 bytes [" + buf.toString() + "]");
      return null;
    }
  }
  if (buf.toString().indexOf("target=") != 0) {
    log("Did not start with target= [" + buf.toString() + "]");
    return null;
  }
  return buf.substring("target=".length());
}
