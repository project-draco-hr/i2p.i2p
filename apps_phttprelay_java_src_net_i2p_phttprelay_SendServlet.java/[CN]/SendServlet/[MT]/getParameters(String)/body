{
  Map params=new HashMap();
  while (line != null) {
    String key=null;
    String val=null;
    int firstAmp=line.indexOf('&');
    int firstEq=line.indexOf('=');
    if (firstAmp > 0) {
      key=line.substring(0,firstEq);
      val=line.substring(firstEq + 1,firstAmp);
      line=line.substring(firstAmp + 1);
      params.put(key,val);
    }
 else {
      line=null;
    }
  }
  return params;
}
