{
  BufferedReader br=new BufferedReader(handle);
  String line;
  Element e=null;
  if (elements.empty())   e=__template__;
 else   e=(Element)elements.pop();
  max_includes--;
  while ((line=br.readLine()) != null) {
    Util.debug_print("Line: " + line);
    e=parseLine(line + "\n",e);
  }
  max_includes++;
  br.close();
  br=null;
}
