{
  String value=null;
  if (params.containsKey(this.name))   value=stringify(params.get(this.name));
 else   value=this.default_value;
  if (value == null)   return "";
  if (this.escape == ESCAPE_HTML)   return Util.escapeHTML(value);
 else   if (this.escape == ESCAPE_URL)   return Util.escapeURL(value);
 else   if (this.escape == ESCAPE_QUOTE)   return Util.escapeQuote(value);
 else   return value;
}
