{
  String type=p.getProperty("type");
  if (type.equals("if"))   return new If(p.getProperty("name"));
 else   if (type.equals("unless"))   return new Unless(p.getProperty("name"));
 else   if (type.equals("loop"))   return new Loop(p.getProperty("name"),loop_context_vars,global_vars);
 else   throw new NoSuchElementException(type);
}
