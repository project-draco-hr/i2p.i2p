{
  try {
    socketIn=sessSocket.getInputStream();
    socketOutStream=sessSocket.getOutputStream();
    socketOut=new OutputStreamWriter(socketOutStream);
    start();
    while (true) {
      String line=DataHelper.readLine(System.in);
      print("sent: '" + line + "'");
      socketOut.write(line + "\n");
      socketOut.flush();
    }
  }
 catch (  IOException e) {
    e.printStackTrace();
    return;
  }
}
