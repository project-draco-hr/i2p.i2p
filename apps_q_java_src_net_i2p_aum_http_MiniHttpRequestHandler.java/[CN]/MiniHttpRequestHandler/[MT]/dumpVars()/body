{
  Tag t=new Tag("table " + "width=90% cellspacing=0 cellpadding=4 border=1");
  tag(t,"tr").nest("td colspan=2").nest("big").nest("bold").raw("Dump of session vars");
  dumpVarsFor(t,headerVars,"HTTP header variables");
  dumpVarsFor(t,urlVars,"URL variables");
  dumpVarsFor(t,postVars,"POST variables");
  return t;
}
