{
  super.init(config);
  try {
    nodeProxy=new XmlRpcClient("http://localhost:7651");
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  options=new Properties();
  try {
    FileInputStream f=new FileInputStream("qconsole.conf");
    options.load(f);
  }
 catch (  Exception e) {
    e.printStackTrace();
    System.out.println("BAD OR MISSING CONF");
  }
}
