{
  Hashtable h=new Hashtable();
  if (raw == null) {
    return h;
  }
  URLDecoder u=new URLDecoder();
  String[] items=raw.split("[&]");
  String dec;
  for (int i=0; i < items.length; i++) {
    try {
      dec=u.decode(items[i],"ISO-8859-1");
      String[] items1=dec.split("[=]",2);
      h.put(items1[0],items1.length > 1 ? items1[1] : "");
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
  return h;
}
