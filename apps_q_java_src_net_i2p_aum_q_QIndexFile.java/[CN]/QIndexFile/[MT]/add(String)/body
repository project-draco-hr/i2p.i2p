{
  if (h.length() != hashLen) {
    System.out.println("hash=" + h);
    throw new IOException("Incorrect hash length (" + h.length() + "), should be "+ hashLen);
  }
  Date d=new Date();
  String ds=String.valueOf(d.getTime());
  while (ds.length() < timeLen) {
    ds="0" + ds;
  }
  String rec=ds + "," + h+ "\n";
  file.seek(numRecs * recordLen);
  file.writeBytes(rec);
  numRecs+=1;
  rawLength+=recordLen;
}
