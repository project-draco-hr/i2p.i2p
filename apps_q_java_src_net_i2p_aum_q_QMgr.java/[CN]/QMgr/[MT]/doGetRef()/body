{
  if (!isServer) {
    System.err.println("Cannot get noderefs for client nodes");
    return 1;
  }
  if (argi < argc) {
    String path=args[argi];
    try {
      new SimpleFile(path,"rws").write(nodeDest);
    }
 catch (    IOException e) {
      e.printStackTrace();
      usage("getref: Cannot create/write output file " + path);
    }
  }
 else {
    System.out.println(nodeDest);
  }
  return 0;
}
