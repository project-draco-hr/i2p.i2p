{
  log.error("addMetadataToCatalog: metadata=" + metadata);
  String key=(String)metadata.get("key");
  String catPath=makeCatalogPath(key);
  try {
    new PropertiesFile(catPath,metadata);
    catalogIdx.add(key);
  }
 catch (  IOException e) {
    log.error("failed to add key '" + key + "' to catalog");
  }
}
