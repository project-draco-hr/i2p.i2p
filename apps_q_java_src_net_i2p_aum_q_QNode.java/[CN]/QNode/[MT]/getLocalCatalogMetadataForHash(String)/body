{
  SimpleFile f;
  try {
    f=new SimpleFile(metaPath,"r");
  }
 catch (  FileNotFoundException e) {
    System.out.println("getLocalCatalogMetadata: no such file '" + metaPath + "'");
    return null;
  }
  try {
    PropertiesFile pf=new PropertiesFile(metaPath);
    QDataItem item=new QDataItem(pf);
    return item;
  }
 catch (  IOException e) {
    e.printStackTrace();
    return null;
  }
}
