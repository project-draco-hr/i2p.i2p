{
  String metaPath=makeDataPath(filename);
  SimpleFile f;
  try {
    f=new SimpleFile(metaPath,"r");
  }
 catch (  FileNotFoundException e) {
    return null;
  }
  try {
    PropertiesFile pf=new PropertiesFile(metaPath);
    QDataItem item=new QDataItem(pf);
    return item;
  }
 catch (  IOException e) {
    return null;
  }
}
