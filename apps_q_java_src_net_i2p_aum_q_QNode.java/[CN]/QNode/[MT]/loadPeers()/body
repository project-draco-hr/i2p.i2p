{
  Iterator peerIds=peersIdx.getItemsSince(0);
  QPeer peerRec;
  peers=new Hashtable();
  while (peerIds.hasNext()) {
    String peerId=(String)peerIds.next();
    try {
      peerRec=getPeerRecord(peerId);
    }
 catch (    Exception e) {
      log.error("Failed to load peer '" + peerId + "'",e);
      continue;
    }
    peers.put(peerId,peerRec);
  }
}
