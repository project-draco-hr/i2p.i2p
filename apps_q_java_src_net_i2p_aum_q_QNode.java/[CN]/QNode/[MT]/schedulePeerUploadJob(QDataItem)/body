{
  String uri=(String)item.get("uri");
  Hashtable job=new Hashtable();
  job.put("cmd","uploadItem");
  job.put("item",item);
  job.put("uri",uri);
  job.put("peersUploaded",new Hashtable());
  job.put("peersPending",new Hashtable());
  job.put("peersFailed",new Hashtable());
  job.put("peersNumTries",new Hashtable());
  String desc="uploadItem:uri=" + uri;
  runNow(job,desc);
}
