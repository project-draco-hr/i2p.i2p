{
  QServerNode node;
  try {
    if (args.length > 0) {
      node=new QServerNode(args[0]);
    }
 else {
      node=new QServerNode();
    }
    node.log.info("QServerNode: entering endless loop...");
    while (true) {
      Thread.sleep(1000);
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
    System.exit(1);
  }
}
