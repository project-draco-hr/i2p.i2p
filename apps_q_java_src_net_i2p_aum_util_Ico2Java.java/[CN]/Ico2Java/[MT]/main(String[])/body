{
  if (args.length != 1) {
    System.out.println("Usage: ico2java filename.ico");
    System.exit(1);
  }
  File f=new File(args[0]);
  if (!f.isFile()) {
    System.out.println("No such file '" + args[0] + "'");
    System.exit(1);
  }
  try {
    convertToJava(args[0]);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
