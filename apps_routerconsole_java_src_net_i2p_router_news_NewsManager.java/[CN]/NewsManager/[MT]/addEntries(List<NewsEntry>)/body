{
  for (  NewsEntry e : entries) {
    String id=e.id;
    if (id == null)     continue;
    boolean found=false;
    for (int i=0; i < _currentNews.size(); i++) {
      NewsEntry old=_currentNews.get(i);
      if (id.equals(old.id)) {
        _currentNews.set(i,e);
        found=true;
        break;
      }
    }
    if (!found)     _currentNews.add(e);
  }
  Collections.sort(_currentNews);
}
