{
  s=s.trim();
  int len=s.length();
  if (!SystemVersion.isJava7() && s.charAt(len - 1) != 'Z' && s.charAt(len - 3) == ':' && (s.charAt(len - 6) == '+' || s.charAt(len - 6) == '-')) {
    s=s.substring(0,len - 3) + s.substring(len - 2);
  }
  for (int i=0; i < rfc3339DateFormats.length; i++) {
    try {
      Date date=rfc3339DateFormats[i].parse(s);
      if (date != null)       return date.getTime();
    }
 catch (    ParseException pe) {
    }
  }
  return -1;
}
