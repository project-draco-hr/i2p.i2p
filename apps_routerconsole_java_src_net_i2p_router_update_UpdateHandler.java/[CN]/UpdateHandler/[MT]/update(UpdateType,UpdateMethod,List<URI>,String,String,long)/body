{
  if ((type != ROUTER_SIGNED && type != ROUTER_SIGNED_SU3) || (method != HTTP && method != HTTP_CLEARNET && method != HTTPS_CLEARNET) || updateSources.isEmpty())   return null;
  UpdateRunner update=new UpdateRunner(_context,_mgr,type,method,updateSources);
  _mgr.notifyProgress(update,"<b>" + _mgr._("Updating") + "</b>");
  return update;
}
