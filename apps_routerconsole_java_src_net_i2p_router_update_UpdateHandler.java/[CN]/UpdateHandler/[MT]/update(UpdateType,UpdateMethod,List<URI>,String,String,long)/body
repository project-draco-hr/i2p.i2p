{
  if ((type != UpdateType.ROUTER_SIGNED && type != UpdateType.ROUTER_SIGNED_SU3) || method != UpdateMethod.HTTP || updateSources.isEmpty())   return null;
  UpdateRunner update=new UpdateRunner(_context,_mgr,type,updateSources);
  _mgr.notifyProgress(update,"<b>" + _mgr._("Updating") + "</b>");
  return update;
}
