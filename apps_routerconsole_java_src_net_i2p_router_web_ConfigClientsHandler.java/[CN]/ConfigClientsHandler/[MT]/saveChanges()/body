{
  _log=_context.logManager().getLog(ConfigClientsHandler.class);
  boolean saveRequired=false;
  int updated=0;
  int index=0;
  if (updated > 0)   addFormNotice("Updated settings");
  if (saveRequired) {
    boolean saved=_context.router().saveConfig();
    if (saved)     addFormNotice("Exploratory tunnel configuration saved successfully");
 else     addFormNotice("Error saving the configuration (applied but not saved) - please see the error logs");
  }
}
