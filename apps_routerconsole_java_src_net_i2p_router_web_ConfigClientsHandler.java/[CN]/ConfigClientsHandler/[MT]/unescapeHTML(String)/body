{
  Map<String,String> map=new HashMap<String,String>();
  map.put("&quot;","\"");
  map.put("&amp;","&");
  map.put("&lt;","<");
  map.put("&gt;",">");
  String unescaped=escaped;
  for (  Map.Entry<String,String> entry : map.entrySet()) {
    String k=entry.getKey();
    String v=entry.getValue();
    unescaped=unescaped.replaceAll(k,v);
  }
  return unescaped;
}
