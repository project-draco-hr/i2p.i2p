{
  StringBuilder buf=new StringBuilder(128);
  StringTokenizer tok=new StringTokenizer(s,",");
  buf.append("<form action=\"").append(url).append("\" method=\"GET\">\n");
  buf.append("<input type=\"hidden\" name=\"consoleNonce\" value=\"").append(nonce).append("\" >\n");
  while (tok.hasMoreTokens())   buf.append("<button type=\"submit\" name=\"action\" value=\"").append(tok.nextToken()).append("\" >").append(tok.nextToken()).append("</button>\n");
  buf.append("</form>\n");
  return buf.toString();
}
