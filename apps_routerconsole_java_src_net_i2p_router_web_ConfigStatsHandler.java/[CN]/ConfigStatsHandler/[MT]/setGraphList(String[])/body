{
  if (stats != null) {
    StringBuilder s=new StringBuilder(128);
    for (int i=0; i < stats.length; i++) {
      String cur=stats[i].trim();
      if (cur.length() > 0) {
        if (s.length() > 0)         s.append(",");
        s.append(cur);
      }
    }
    _graphs=s.toString();
  }
 else {
    _graphs="";
  }
}
