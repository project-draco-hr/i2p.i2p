{
  if (stats != null) {
    String s="";
    for (int i=0; i < stats.length; i++) {
      String cur=stats[i].trim();
      if (cur.length() > 0) {
        if (s.length() > 0)         s=s + ",";
        s=s + cur;
      }
    }
    _graphs=s;
  }
 else {
    _graphs="";
  }
  if (_log.shouldLog(Log.DEBUG))   _log.debug("Updated graphs: " + _graphs);
}
