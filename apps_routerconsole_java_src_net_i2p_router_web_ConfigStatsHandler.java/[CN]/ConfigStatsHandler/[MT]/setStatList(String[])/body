{
  if (stats != null) {
    for (int i=0; i < stats.length; i++) {
      String cur=stats[i].trim();
      if (_log.shouldLog(Log.DEBUG))       _log.debug("Stat: [" + cur + "]");
      if ((cur.length() > 0) && (!_stats.contains(cur)))       _stats.add(cur);
    }
  }
  if (_log.shouldLog(Log.DEBUG))   _log.debug("Updated stats: " + _stats);
}
