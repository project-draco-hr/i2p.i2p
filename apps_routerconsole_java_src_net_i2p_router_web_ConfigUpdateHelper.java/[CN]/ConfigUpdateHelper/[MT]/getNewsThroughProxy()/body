{
  if (_context.getProperty(ConfigUpdateHandler.PROP_SHOULD_PROXY_NEWS,ConfigUpdateHandler.DEFAULT_SHOULD_PROXY_NEWS))   return "<input type=\"checkbox\" class=\"optbox\" value=\"true\" name=\"newsThroughProxy\" checked=\"checked\" >";
 else   return "<input type=\"checkbox\" class=\"optbox\" value=\"true\" name=\"newsThroughProxy\" >";
}
