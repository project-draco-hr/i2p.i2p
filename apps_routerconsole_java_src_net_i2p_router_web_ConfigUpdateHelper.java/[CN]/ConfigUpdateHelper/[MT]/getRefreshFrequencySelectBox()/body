  return "<select name=\"refreshFrequency\">" + "<option value=\"" + (12 * 60 * 60* 1000) + "\">Twice daily</option>"+ "<option value=\""+ (24 * 60 * 60* 1000)+ "\" selected=\"true\" >Daily</option>"+ "<option value=\""+ (48 * 60 * 60* 1000)+ "\">Every two days</option>"+ "<option value=\""+ -1+ "\">Never</option>"+ "</select>";
