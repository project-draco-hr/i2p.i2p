{
  _out.write("<option value=\"");
  _out.write(Long.toString(age));
  _out.write("\"");
  if (age == _age)   _out.write(" selected=\"selected\"");
  _out.write(">");
  if (age == 0)   _out.write(_t("All events"));
 else   _out.write(DataHelper.formatDuration2(age));
  _out.write("</option>\n");
}
