{
  List<String> names=new ArrayList(counter.objects());
  if (names.size() < 4)   return;
  buf.append("<table style=\"width: 30%; margin-left: 100px;\">\n" + "<tr><th>Job</th><th>Queued<th>");
  Collections.sort(names,new JobCountComparator(counter));
  for (  String name : names) {
    buf.append("<tr><td>").append(name).append("</td><td>").append(counter.count(name)).append("</td></tr>\n");
  }
  buf.append("</table>\n");
}
