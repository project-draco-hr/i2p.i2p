{
  try {
    if (_out != null) {
      renderStatusHTML(_out);
      return "";
    }
 else {
      ByteArrayOutputStream baos=new ByteArrayOutputStream(32 * 1024);
      renderStatusHTML(new OutputStreamWriter(baos));
      return new String(baos.toByteArray());
    }
  }
 catch (  IOException ioe) {
    ioe.printStackTrace();
    return "";
  }
}
