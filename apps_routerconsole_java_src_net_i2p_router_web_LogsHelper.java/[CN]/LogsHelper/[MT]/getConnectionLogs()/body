{
  List msgs=_context.commSystem().getMostRecentErrorMessages();
  StringBuffer buf=new StringBuffer(16 * 1024);
  buf.append("<ul>");
  buf.append("<code>\n");
  for (int i=msgs.size(); i > 0; i--) {
    String msg=(String)msgs.get(i - 1);
    buf.append("<li>");
    buf.append(msg);
    buf.append("</li>\n");
  }
  buf.append("</code></ul>\n");
  return buf.toString();
}
