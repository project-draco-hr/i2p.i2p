{
  File f=new File(_context.getLogDir(),"wrapper.log");
  if (!f.exists())   f=new File(_context.getBaseDir(),"wrapper.log");
  String str=FileUtil.readTextFile(f.getAbsolutePath(),250,false);
  if (str == null)   return "";
 else {
    str=str.replaceAll("<","&lt;").replaceAll(">","&gt;");
    return "<pre>" + str + "</pre>";
  }
}
