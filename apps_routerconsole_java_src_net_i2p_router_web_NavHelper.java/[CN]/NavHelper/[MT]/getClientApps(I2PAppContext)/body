{
  if (_apps.isEmpty())   return Collections.EMPTY_LIST;
  List<HomeHelper.App> rv=new ArrayList(_apps.size());
  for (  Map.Entry<String,String> e : _apps.entrySet()) {
    String name=e.getKey();
    String path=e.getValue();
    if (path == null)     continue;
    String tip=_tooltips.get(name);
    if (tip == null)     tip="";
    String icon;
    if (path.equals("/i2pbote/index.jsp"))     icon="/themes/console/images/email.png";
 else     icon="/themes/console/images/plugin.png";
    HomeHelper.App app=new HomeHelper.App(name,tip,path,icon);
    rv.add(app);
  }
  return rv;
}
