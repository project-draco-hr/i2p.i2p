{
  if (_log.shouldLog(Log.INFO))   _log.info("News fetched from " + url + " with "+ (alreadyTransferred + bytesTransferred));
  File temp=new File(TEMP_NEWS_FILE);
  if (temp.exists()) {
    boolean copied=FileUtil.copy(TEMP_NEWS_FILE,NEWS_FILE,true);
    if (copied) {
      temp.delete();
    }
 else {
      if (_log.shouldLog(Log.ERROR))       _log.error("Failed to copy the news file!");
    }
  }
 else {
    if (_log.shouldLog(Log.ERROR))     _log.error("Transfer complete, but no file?");
  }
  checkForUpdates();
}
