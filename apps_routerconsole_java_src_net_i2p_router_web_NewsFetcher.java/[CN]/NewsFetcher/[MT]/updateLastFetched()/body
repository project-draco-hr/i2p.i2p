{
  if (_newsFile.exists()) {
    if (_lastUpdated == 0)     _lastUpdated=_newsFile.lastModified();
    if (_lastFetch == 0)     _lastFetch=_lastUpdated;
    if (_lastModified == null)     _lastModified=RFC822Date.to822Date(_lastFetch);
  }
 else {
    _lastUpdated=0;
    _lastFetch=0;
    _lastModified=null;
  }
}
