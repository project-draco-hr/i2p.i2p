{
  Log log=ctx.logManager().getLog(PluginStopper.class);
  for (  String app : getPlugins()) {
    if (isPluginRunning(app,ctx)) {
      try {
        stopPlugin(ctx,app);
      }
 catch (      Throwable e) {
        if (log.shouldLog(Log.WARN))         log.warn("Failed to stop plugin: " + app,e);
      }
    }
  }
}
