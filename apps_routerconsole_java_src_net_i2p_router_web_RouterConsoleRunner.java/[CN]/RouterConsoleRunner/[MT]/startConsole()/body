{
  _server=new Server();
  try {
    _server.addListener(_listenHost + ':' + _listenPort);
    _server.setRootWebApp("routerconsole");
    _server.addWebApplications(_webAppsDir);
  }
 catch (  IOException ioe) {
    ioe.printStackTrace();
  }
  try {
    _server.start();
  }
 catch (  MultiException me) {
    me.printStackTrace();
  }
}
