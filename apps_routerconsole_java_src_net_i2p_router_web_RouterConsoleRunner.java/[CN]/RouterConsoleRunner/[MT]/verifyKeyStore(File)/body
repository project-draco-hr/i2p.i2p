{
  if (ks.exists()) {
    I2PAppContext ctx=I2PAppContext.getGlobalContext();
    boolean rv=ctx.getProperty(PROP_KEY_PASSWORD) != null;
    if (!rv)     System.err.println("Console SSL error, must set " + PROP_KEY_PASSWORD + " in "+ (new File(ctx.getConfigDir(),"router.config")).getAbsolutePath());
    return rv;
  }
  File dir=ks.getParentFile();
  if (!dir.exists()) {
    File sdir=new SecureDirectory(dir.getAbsolutePath());
    if (!sdir.mkdir())     return false;
  }
  return createKeyStore(ks);
}
