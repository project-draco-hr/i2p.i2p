{
  try {
    Options options=getOptions(args);
    SAMBridge bridge=new SAMBridge(options.host,options.port,options.opts,options.keyFile);
    bridge.startThread();
  }
 catch (  RuntimeException e) {
    e.printStackTrace();
    usage();
    throw e;
  }
catch (  Exception e) {
    e.printStackTrace();
    usage();
    throw new RuntimeException(e);
  }
}
