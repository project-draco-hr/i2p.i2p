{
  if (_state != INITIALIZED)   return;
  changeState(STARTING);
  loadKeys();
  try {
    openSocket();
  }
 catch (  IOException e) {
    if (_log.shouldLog(Log.ERROR))     _log.error("Error starting SAM bridge on " + (_listenHost == null ? "0.0.0.0" : _listenHost) + ":"+ _listenPort,e);
    changeState(START_FAILED,e);
    throw e;
  }
  startThread();
}
