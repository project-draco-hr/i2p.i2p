{
  _log.debug("I2P message available (id: " + msgId + "; size: "+ size+ ")");
  try {
    byte msg[]=session.receiveMessage(msgId);
    if (_log.shouldLog(Log.DEBUG)) {
      _log.debug("Content of message " + msgId + ":\n"+ HexDump.dump(msg));
    }
    recv.receiveRawBytes(msg);
  }
 catch (  IOException e) {
    _log.error("Error forwarding message to receiver",e);
    stopRunning();
  }
catch (  I2PSessionException e) {
    _log.error("Error fetching I2P message",e);
    stopRunning();
  }
}
