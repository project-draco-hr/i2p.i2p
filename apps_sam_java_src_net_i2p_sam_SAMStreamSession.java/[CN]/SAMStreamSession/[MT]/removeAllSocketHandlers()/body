{
  Integer id;
  Set keySet;
  Iterator iter;
synchronized (handlersMapLock) {
    keySet=handlersMap.keySet();
    iter=keySet.iterator();
    while (iter.hasNext()) {
      id=(Integer)iter.next();
      ((SAMStreamSessionSocketHandler)handlersMap.get(id)).stopRunning();
    }
    handlersMap.clear();
  }
}
