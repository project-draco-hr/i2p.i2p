{
  SAMStreamSessionSocketHandler handler=getSocketHandler(id);
  if (handler == null) {
    if (_log.shouldLog(Log.WARN))     _log.error("Trying to send bytes through inexistent handler " + id);
    for (int i=0; i < size; i++) {
      int c=in.read();
      if (c == -1)       break;
    }
    return false;
  }
  return handler.sendBytes(in,size);
}
