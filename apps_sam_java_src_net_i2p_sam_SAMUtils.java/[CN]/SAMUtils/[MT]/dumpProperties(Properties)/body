{
  Enumeration enum=props.propertyNames();
  String msg="";
  String key, val;
  boolean firstIter=true;
  while (enum.hasMoreElements()) {
    key=(String)enum.nextElement();
    val=props.getProperty(key);
    if (!firstIter) {
      msg+=";";
    }
 else {
      firstIter=false;
    }
    msg+=" \"" + key + "\" -> \""+ val+ "\"";
  }
  return msg;
}
