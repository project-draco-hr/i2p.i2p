{
  NamingService ns=NamingService.getInstance();
  Destination dest=ns.lookup(name);
  if ((pubKey != null) && (dest != null)) {
    try {
      dest.writeBytes(pubKey);
    }
 catch (    IOException e) {
      e.printStackTrace();
      return null;
    }
catch (    DataFormatException e) {
      e.printStackTrace();
      return null;
    }
  }
  return dest;
}
