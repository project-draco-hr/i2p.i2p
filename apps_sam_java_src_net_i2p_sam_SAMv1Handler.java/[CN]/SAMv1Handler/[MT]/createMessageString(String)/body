{
  String rv;
  if (msg != null) {
    msg=msg.replace("\n"," ");
    msg=msg.replace("\r"," ");
    if (!msg.startsWith("\"")) {
      msg=msg.replace("\"","");
      if (msg.contains("\"") || msg.contains("\t"))       msg='"' + msg + '"';
    }
    rv=" MESSAGE=\"" + msg + "\"";
  }
 else {
    rv="";
  }
  return rv;
}
