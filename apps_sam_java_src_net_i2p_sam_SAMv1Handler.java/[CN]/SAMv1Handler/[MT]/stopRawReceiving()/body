{
  _log.debug("stopRawReceiving() invoked");
  if (rawSession == null) {
    _log.error("BUG! Got raw receiving stop, but session is null!");
    throw new NullPointerException("BUG! RAW session is null!");
  }
  try {
    this.socket.close();
  }
 catch (  IOException e) {
    _log.error("Error closing socket: " + e.getMessage());
  }
}
