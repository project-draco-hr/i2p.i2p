{
  _log.debug("stopRunning() invoked on socket sender " + _id);
synchronized (runningLock) {
    if (_stillRunning) {
      _stillRunning=false;
      try {
        i2pSocket.close();
      }
 catch (      IOException e) {
        _log.debug("Caught IOException",e);
      }
synchronized (_data) {
        _data.clear();
        _data.notifyAll();
      }
    }
  }
}
