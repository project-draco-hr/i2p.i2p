{
  try {
    try {
      streamSession.startForwardingIncoming(props);
      notifyStreamAccept("OK");
      return true;
    }
 catch (    SAMException e) {
      _log.debug("Forwarding STREAM connections failed: " + e.getMessage());
      notifyStreamAccept("FORWARDER_FAILED");
    }
  }
 catch (  IOException e) {
  }
  return false;
}
