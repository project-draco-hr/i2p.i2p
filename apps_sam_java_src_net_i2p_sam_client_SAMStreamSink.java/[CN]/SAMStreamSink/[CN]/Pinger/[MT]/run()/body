{
  while (true) {
    try {
      Thread.sleep(127 * 1000);
synchronized (_out) {
        _out.write(DataHelper.getASCII("PING " + System.currentTimeMillis() + '\n'));
        _out.flush();
      }
    }
 catch (    InterruptedException ie) {
      break;
    }
catch (    IOException ioe) {
      break;
    }
  }
}
