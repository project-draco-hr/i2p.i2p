{
  if (_closed)   return;
  try {
    _out.write(data,offset,len);
  }
 catch (  IOException ioe) {
    _log.error("Error writing received data");
    closed();
    return;
  }
  _log.debug("Received " + len + " on "+ _connectionId+ " after "+ (_context.clock().now() - _lastReceivedOn)+ "ms with "+ _remoteDestination.substring(0,6));
  _lastReceivedOn=_context.clock().now();
}
