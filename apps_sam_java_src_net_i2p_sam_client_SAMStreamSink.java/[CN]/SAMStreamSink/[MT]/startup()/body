{
  if (_log.shouldLog(Log.DEBUG))   _log.debug("Starting up");
  boolean ok=connect();
  if (_log.shouldLog(Log.DEBUG))   _log.debug("Connected: " + ok);
  if (ok) {
    _reader=new SAMReader(_context,_samIn,_eventHandler);
    _reader.startReading();
    if (_log.shouldLog(Log.DEBUG))     _log.debug("Reader created");
    String ourDest=handshake();
    if (_log.shouldLog(Log.DEBUG))     _log.debug("Handshake complete.  we are " + ourDest);
    if (ourDest != null) {
      writeDest(ourDest);
      if (_log.shouldLog(Log.DEBUG))       _log.debug("My destination written to " + _destFile);
    }
  }
}
