{
  FileOutputStream fos=null;
  try {
    fos=new FileOutputStream(_destFile);
    fos.write(dest.getBytes());
  }
 catch (  Exception e) {
    _log.error("Error writing to " + _destFile,e);
    return false;
  }
 finally {
    if (fos != null)     try {
      fos.close();
    }
 catch (    IOException ioe) {
    }
  }
  return true;
}
