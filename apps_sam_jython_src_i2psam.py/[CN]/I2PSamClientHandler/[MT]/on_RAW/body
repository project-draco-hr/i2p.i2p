def on_RAW(self, topic, subtopic, args):
    '\n        RAW SEND\n        DESTINATION=$base64key\n        SIZE=$numBytes\\n[$numBytes of data]\n        '
    log(4, 'entered')
    if (subtopic != 'SEND'):
        return
    peerdest = I2PDestination(base64=args['DESTINATION'])
    msg = args['DATA']
    self.samSession.sendMessage(peerdest, msg)
