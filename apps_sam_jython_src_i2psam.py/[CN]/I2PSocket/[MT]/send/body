def send(self, buf):
    '\n        Sends buf thru socket\n        '
    if (not self.outstream):
        raise I2PSocketError('Socket is not connected')
    log(4, ("send: writing '%s' to outstream..." % repr(buf)))
    outstream = self.outstream
    for c in buf:
        outstream.write(ord(c))
    log(4, 'send: flushing...')
    self.outstream.flush()
    log(4, 'send: done')
