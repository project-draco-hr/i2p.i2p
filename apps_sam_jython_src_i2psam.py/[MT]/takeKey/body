def takeKey(somedict, keyname, default=None):
    "\n    Utility function to destructively read a key from a given dict.\n    Same as the dict's 'takeKey' method, except that the key (if found)\n    sill be deleted from the dictionary.\n    "
    if somedict.has_key(keyname):
        val = somedict[keyname]
        del somedict[keyname]
    else:
        val = default
    return val
