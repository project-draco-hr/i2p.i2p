def makedoc():
    'Make all HTML documentation for Python I2P library.'
    modules = ['i2p', 'i2p.sam', 'i2p.eep', 'i2p.router', 'i2p.samclasses']
    origdir = os.getcwd()
    os.chdir('../..')
    for m in modules:
        pydoc(('-w ' + m))
    os.chdir(origdir)
    for m in modules:
        move((('../../' + m) + '.html'), (('./' + m) + '.html'))
