def __init__(self, addr=''):
    if (addr == ''):
        addr = i2p.sam.samaddr
    self.term = SAMTerminal(addr=addr, msgobj=self)
    self.lock = threading.Lock()
    self.closed = False
    self.qhello = Queue.Queue()
    self.qnaming = Queue.Queue()
    self.qsession = Queue.Queue()
    self._hello()
