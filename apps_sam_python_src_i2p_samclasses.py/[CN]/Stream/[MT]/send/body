def send(self, s):
    'Sends the string s, blocking if necessary.'
    id = self.id
    if (self.closed or (id == None)):
        if (self.err != None):
            raise self.err
        raise i2p.sam.ClosedError('stream closed')
    if (len(s) == 0):
        return
    nmax = 32768
    for block in [s[i:(i + nmax)] for i in range(0, len(s), nmax)]:
        self.parent._send_stream(id, block)
