def unsubscribe(self, node):
    '\n        Unsubscribes a node from the core\n        \n        If this was the last node, stops the handler thread\n        '
    try:
        nodes = self.nodes
        if (node not in nodes):
            self.log(4, ('duhhh! node %s was not subscribed' % repr(node)))
            return
        self.log(2, ('trying to unsubscribe node %s' % node.name))
        nodes.remove(node)
        if (len(nodes) == 0):
            self.isRunning = False
    except:
        traceback.print_exc()
        self.log(2, 'exception')
