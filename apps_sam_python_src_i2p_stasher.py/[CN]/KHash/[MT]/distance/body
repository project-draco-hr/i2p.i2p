def distance(self, other):
    "\n        calculates the 'distance' between this hash and another hash,\n        and returns it as i (where distance = 2^i, and 0 <= i < 160)\n        "
    rawdistance = (self.value ^ other.value)
    if (not rawdistance):
        return 0
    return int(math.log(rawdistance, 2))
