def _store(self, key, value, callback=None, **kw):
    '\n        Performs a STORE rpc\n        \n        Arguments:\n            - key - string - text name of key\n            - value - string - value to store\n    \n        Keywords:\n            - local - if given and true, only store value onto local store\n        '
    if (not kw.has_key('local')):
        kw = dict(kw)
        kw['local'] = False
    key = shahash(key)
    if callback:
        KRpcStore(self, callback, key=key, value=value, **kw)
        self.log(3, 'asynchronously invoked findnode, expecting callback')
    else:
        res = KRpcStore(self, key=key, value=value, **kw).execute()
        return res
