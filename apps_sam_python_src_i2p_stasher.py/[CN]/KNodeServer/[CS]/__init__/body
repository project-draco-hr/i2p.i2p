def __init__(self, node, addr=None):
    if (addr == None):
        addr = clientAddr
    self.isRunning = True
    self.node = node
    (listenHost, listenPort) = addr.split(':')
    listenPort = int(listenPort)
    self.listenPort = listenPort
    SocketServer.TCPServer.__init__(self, (listenHost, listenPort), KNodeReqHandler)
