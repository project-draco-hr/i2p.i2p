def isCloserThanAllOf(self, tab):
    "\n        returns True if this peerRec is closer to the desired hash\n        than all of the peerRecs in table 'tab'\n        "
    if (not isinstance(tab, KPeerQueryTable)):
        self.log(2, ('invalid qtable %s' % repr(tab)))
        raise Exception(('invalid qtable %s' % repr(tab)))
    for rec in tab:
        if (self > rec):
            return False
    return True
