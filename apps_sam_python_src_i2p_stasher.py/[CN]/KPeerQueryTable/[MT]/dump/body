def dump(self):
    c = self.count
    self.log(2, ('PeerQueryTable stats:\nstart:       %s\nrecommended: %s\nqueried:     %s\nreplied:     %s\ntimeout:     %s\nclosest:     %s\ntoofar:      %s\nTOTAL:       %s\n' % (c('start'), c('recommended'), c('queried'), c('replied'), c('timeout'), c('closest'), c('toofar'), len(self.peers))))
