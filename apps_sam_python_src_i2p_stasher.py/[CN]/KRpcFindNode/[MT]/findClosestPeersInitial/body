def findClosestPeersInitial(self):
    '\n        Searches our k-buckets, and returns a table of k of\n        peers closest to wanted hash into self.closestPeersInitial\n        '
    hashobj = self.hashWanted
    lst = []
    buckets = self.localNode.buckets
    for bucket in buckets:
        for peer in bucket:
            lst.append(peer)
    table = KPeerQueryTable(lst, self.hashWanted, 'start')
    table.sort()
    return table[:maxBucketSize]
