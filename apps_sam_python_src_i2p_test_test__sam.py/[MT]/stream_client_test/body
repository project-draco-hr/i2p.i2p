def stream_client_test():
    'Unit test for sam.socket in SOCK_STREAM mode.'
    url = 'morph.i2p'
    stream_client((('http://' + url) + '/'))
    stream_client(url)
    stream_client((url + '/'))
    stream_client(('http://' + url))
    stream_client(sam.resolve((('http://' + url) + '/')))
    test_passed('sam.socket stream client')
