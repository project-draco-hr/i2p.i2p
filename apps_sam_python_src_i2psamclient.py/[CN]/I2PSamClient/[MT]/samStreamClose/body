def samStreamClose(self, conn):
    "\n        DO NOT CALL DIRECTLY\n        \n        Invoked by I2PSAMStream to close stream\n        Use the object's .send() method instead.\n        \n        STREAM CLOSE\n        ID=$id\n        "
    self.samSend('STREAM', 'CLOSE', ID=conn.id)
    del self.streams[conn.id]
