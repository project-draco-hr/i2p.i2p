def justSeenPeer(self, peer):
    "\n        Tells the bucket that we've just seen a given node\n        "
    nodes = self.nodes
    if (not isinstance(peer, KPeer)):
        raise KBadNode
    try:
        idx = nodes.index(peer)
    except:
        idx = (-1)
    if (idx >= 0):
        del nodes[idx]
        nodes.append(peer)
    else:
        nodes.append(peer)
