def _doHousekeeping(self):
    '\n        Performs periodical housekeeping on this node.\n        \n        Activities include:\n            - checking pending records for timeouts\n        '
    now = time.time()
    for (msgId, (dest, q, pingDeadline)) in self.pendingPings.items():
        if (pingDeadline > now):
            continue
        del self.pendingPings[msgId]
        q.put(False)
    for rpc in self.rpcPending[:]:
        if (now >= rpc.nextTickTime):
            try:
                rpc.on_tick()
            except:
                traceback.print_exc()
                self.log(2, 'unhandled exception in RPC on_tick')
