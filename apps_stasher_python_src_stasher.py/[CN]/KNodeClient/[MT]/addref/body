def addref(self, ref):
    '\n        Passes a new noderef to node\n        '
    self.connect()
    self.write(('addref %s\n' % ref))
    self.flush()
    res = self.readline().strip()
    self.close()
    if (res == 'ok'):
        return True
    else:
        print repr(res)
        return False
