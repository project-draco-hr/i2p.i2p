def get(self, key):
    '\n        sends a get command to stasher socket, and retrieves\n        and interprets result\n        '
    self.connect()
    self.write(('get %s\n' % key))
    self.flush()
    res = self.readline().strip()
    if (res == 'ok'):
        size = int(self.readline())
        val = self.read(size)
        self.close()
        return val
    else:
        self.close()
        return None
