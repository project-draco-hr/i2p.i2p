def getref(self):
    "\n        Uplifts node's own ref\n        "
    self.connect()
    self.write('getref\n')
    self.flush()
    res = self.readline().strip()
    if (res == 'ok'):
        ref = self.readline().strip()
        self.close()
        return ref
    else:
        self.close()
        return 'failed'
