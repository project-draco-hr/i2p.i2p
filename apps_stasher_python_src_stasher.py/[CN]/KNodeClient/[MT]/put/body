def put(self, key, val):
    '\n        Tells remote stasher port to insert a file into the network\n        '
    self.connect()
    self.write(('put %s\n' % key))
    self.write(('%s\n' % len(val)))
    self.write(val)
    self.flush()
    res = self.readline().strip()
    self.close()
    if (res == 'ok'):
        return True
    else:
        print repr(res)
        return False
