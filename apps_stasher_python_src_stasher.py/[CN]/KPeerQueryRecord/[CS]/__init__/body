def __init__(self, peer, table, state=None, **kw):
    self.peer = peer
    self.dest = peer.dest
    self.deadline = (time.time() + timeout['findNode'])
    self.table = table
    if (state == None):
        state = 'start'
    if (not isinstance(state, str)):
        raise Exception(('Invalid state %s' % state))
    self.state = state
    self.__dict__.update(kw)
