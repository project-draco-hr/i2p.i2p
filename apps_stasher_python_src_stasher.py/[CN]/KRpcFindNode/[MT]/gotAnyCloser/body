def gotAnyCloser(self):
    "\n        Tests if any peer records in state 'recommended' or 'replied'\n        are nearer than the records in state 'closest'\n        "
    peerTab = self.peerTab
    closest = peerTab.select('closest')
    if (len(closest) == 0):
        return True
    candidates = peerTab.select('recommended')
    gotOneCloser = False
    for c in candidates:
        if c.isCloserThanAllOf(closest):
            return True
    return False
