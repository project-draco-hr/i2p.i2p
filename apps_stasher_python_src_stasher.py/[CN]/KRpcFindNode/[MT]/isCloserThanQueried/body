def isCloserThanQueried(self, peer):
    "\n        Test function which returns True if argument 'peer'\n        is closer than all the peers in self.peersAlreadyQueried,\n        or False if not\n        "
    for p in self.peersAlreadyQueried:
        if (p.id.rawdistance(self.hashWanted) < peer.id.rawdistance(self.hashWanted)):
            return False
    return True
