def on_tick(self):
    '\n        Callback for FIND_NODE reply timeout\n        '
    now = time.time()
    for peerRec in self.peerTab:
        if peerRec.hasTimedOut(now):
            peerRec.state = 'timeout'
            self.sendSomeQueries()
    self.checkEndOfRound()
    self.nextTickTime = (time.time() + 5)
