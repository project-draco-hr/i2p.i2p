def start(self):
    '\n        Kicks off this RPC\n        '
    if self.splitting:
        self.storeSplit()
        return
    self.value = ('chunks:0\n' + self.value)
    if (self.isLocalOnly or (len(self.localNode.peers) == 0)):
        result = self.localNode.storage.putKey(self.keyHashed, self.value, keyIsHashed=True)
        if result:
            result = 1
        else:
            result = 0
        self.returnValue(result)
        return
    KRpcFindNode(self.localNode, self.on_doneFindNode, hash=self.keyHashed, raw=True, local=False)
    return
