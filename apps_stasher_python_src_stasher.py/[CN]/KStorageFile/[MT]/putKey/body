def putKey(self, key, val, keyIsHashed=False):
    "\n        Stores a string into this storage under the key 'key'\n    \n        Returns True if key was saved successfully, False if not\n        "
    try:
        if keyIsHashed:
            keyHashed = key
        else:
            keyHashed = shahash(key)
        keyHashed = keyHashed.lower()
        keyPath = os.path.join(self.keysDir, keyHashed)
        file(keyPath, 'wb').write(val)
        self.log(4, ("stored key: '%s'\nunder hash '%s'\n(keyIsHashed=%s)" % (key, keyHashed, keyIsHashed)))
        return True
    except:
        traceback.print_exc()
        self.log(3, 'failed to store key')
        return False
