{
  int max=_defaultOptions.getMaxConns();
  if (max <= 0)   return false;
  if (_connectionByInboundId.size() < max)   return false;
  int active=0;
  for (  Connection con : _connectionByInboundId.values()) {
    if (con.getIsConnected())     active++;
  }
  if ((_connectionByInboundId.size() > 100) && (_log.shouldLog(Log.INFO)))   _log.info("More than 100 connections!  " + active + " total: "+ _connectionByInboundId.size());
  return (active >= max);
}
