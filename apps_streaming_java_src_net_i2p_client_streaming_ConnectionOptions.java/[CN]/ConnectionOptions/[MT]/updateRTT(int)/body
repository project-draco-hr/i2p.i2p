{
switch (_initState) {
case INIT:
    _initState=AckInit.FIRST;
  setRTT(measuredValue);
break;
case FIRST:
_initState=AckInit.STEADY;
case STEADY:
_rttDev=(int)((1 - _beta) * _rttDev + _beta * Math.abs(measuredValue - _rtt));
int smoothed=(int)((1 - _alpha) * _rtt + _alpha * measuredValue);
setRTT(smoothed);
}
computeRTO();
}
