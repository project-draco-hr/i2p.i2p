{
  long timeout=this.getSoTimeout();
  try {
    if (blocking) {
      this.setSoTimeout(-1);
    }
 else {
      this.setSoTimeout(0);
    }
    try {
      return this.accept();
    }
 catch (    SocketTimeoutException e) {
      if (blocking)       throw e;
 else       return null;
    }
  }
  finally {
    this.setSoTimeout(timeout);
  }
}
