{
  Connection c=_connection;
  if (c == null) {
    return;
  }
  if (c.getIsConnected()) {
    OutputStream out=c.getOutputStream();
    if (out != null) {
      try {
        out.close();
      }
 catch (      IOException ioe) {
      }
    }
    c.disconnect(true);
  }
 else {
  }
  destroy();
}
