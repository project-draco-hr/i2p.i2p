{
  verifySession();
  Connection con=_connectionManager.getConnectionHandler().accept(-1);
  if (_log.shouldLog(Log.DEBUG))   _log.debug("receiveSocket() called: " + con);
  if (con != null) {
    I2PSocketFull sock=new I2PSocketFull(con);
    con.setSocket(sock);
    return sock;
  }
 else {
    return null;
  }
}
