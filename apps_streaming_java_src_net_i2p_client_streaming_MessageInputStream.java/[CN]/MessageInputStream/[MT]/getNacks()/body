{
  List ids=null;
synchronized (_dataLock) {
    for (long i=_highestReadyBlockId + 1; i < _highestBlockId; i++) {
      Long l=new Long(i);
      if (_notYetReadyBlocks.containsKey(l)) {
      }
 else {
        if (ids == null)         ids=new ArrayList(4);
        ids.add(l);
      }
    }
  }
  if (ids != null) {
    long rv[]=new long[ids.size()];
    for (int i=0; i < rv.length; i++)     rv[i]=((Long)ids.get(i)).longValue();
    return rv;
  }
 else {
    return null;
  }
}
