{
  long blocks[]=null;
synchronized (_dataLock) {
    int num=_notYetReadyBlocks.size();
    if (num <= 0)     return null;
    blocks=new long[num];
    int i=0;
    for (Iterator iter=_notYetReadyBlocks.keySet().iterator(); iter.hasNext(); ) {
      Long id=(Long)iter.next();
      blocks[i]=id.longValue();
      i++;
    }
  }
  Arrays.sort(blocks);
  return blocks;
}
