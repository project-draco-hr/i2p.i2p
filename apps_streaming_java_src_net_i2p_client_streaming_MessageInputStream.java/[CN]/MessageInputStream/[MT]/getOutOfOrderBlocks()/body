{
  long blocks[]=null;
synchronized (_dataLock) {
    int num=_notYetReadyBlocks.size();
    if (num <= 0)     return null;
    blocks=new long[num];
    int i=0;
    for (    Long id : _notYetReadyBlocks.keySet()) {
      blocks[i++]=id.longValue();
    }
  }
  Arrays.sort(blocks);
  return blocks;
}
