{
  if (false) {
    if (_context.random().nextInt(100) >= 98) {
      _log.error("DROP: " + packet);
      return false;
    }
 else {
      int delay=_context.random().nextInt(1000);
      try {
        Thread.sleep(delay);
      }
 catch (      InterruptedException ie) {
      }
      _log.debug("OK  : " + packet + " delay = "+ delay);
      return true;
    }
  }
 else {
    return true;
  }
}
