{
  if (false) {
    if (_context.random().nextInt(100) >= 95) {
      displayPacket(packet,"DROP");
      return false;
    }
 else {
      int delay=_context.random().nextInt(6 * 1000);
      int delayFactor=_context.random().nextInt(100);
      if (delayFactor > 80) {
        if (delayFactor > 98)         delay*=5;
 else         if (delayFactor > 95)         delay*=4;
 else         if (delayFactor > 90)         delay*=3;
 else         delay*=2;
      }
      if (_context.random().nextInt(100) >= 20)       delay=_lastDelay;
      _lastDelay=delay;
      SimpleTimer.getInstance().addEvent(new Reinject(packet,delay),delay);
      return false;
    }
  }
 else {
    return true;
  }
}
