{
  _context=ctx;
  _createdOn=ctx.clock().now();
  _log=ctx.logManager().getLog(PacketLocal.class);
  _to=to;
  _connection=con;
  _lastSend=-1;
  _cancelledOn=-1;
  _nackCount=0;
  _retransmitted=false;
synchronized (initLock) {
    if (!_initialized) {
      initPcap();
      _initialized=true;
    }
  }
}
