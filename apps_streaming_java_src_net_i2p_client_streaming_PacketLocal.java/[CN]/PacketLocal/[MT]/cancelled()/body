{
  ByteArray ba=null;
synchronized (this) {
    _cancelledOn=_context.clock().now();
    ba=getPayload();
    setPayload(null);
    notifyAll();
  }
  SimpleTimer.getInstance().removeEvent(_resendEvent);
  if (_log.shouldLog(Log.DEBUG))   _log.debug("Cancelled! " + toString(),new Exception("cancelled"));
  if (ba != null)   _cache.release(ba);
}
