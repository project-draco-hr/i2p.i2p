{
  if (tags != null && tags.size() > 0)   _log.error("Who is sending tags thru the streaming lib? " + tags.size());
  if ((_tagsSent != null) && (_tagsSent.size() > 0) && (tags.size() > 0)) {
    if (!_tagsSent.equals(tags))     System.out.println("ERROR: dup tags: old=" + _tagsSent.size() + " new="+ tags.size()+ " packet: "+ toString());
  }
 else {
    _tagsSent=tags;
  }
}
