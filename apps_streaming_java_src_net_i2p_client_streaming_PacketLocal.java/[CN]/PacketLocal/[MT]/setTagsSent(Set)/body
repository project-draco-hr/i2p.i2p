{
  if (tags != null && !tags.isEmpty())   _log.error("Who is sending tags thru the streaming lib? " + tags.size());
  if ((_tagsSent != null) && (!_tagsSent.isEmpty()) && (!tags.isEmpty())) {
    if (!_tagsSent.equals(tags))     System.out.println("ERROR: dup tags: old=" + _tagsSent.size() + " new="+ tags.size()+ " packet: "+ toString());
  }
 else {
    _tagsSent=tags;
  }
}
