{
  boolean ok=(con != null) && (con.getAckedPackets() > 0) && (con.getOptions().getProfile() == ConnectionOptions.PROFILE_BULK)&& (!con.getResetReceived())&& ((con.getCloseSentOn() <= 0) || (con.getCloseReceivedOn() <= 0));
  if (!ok) {
    if (_log.shouldLog(Log.DEBUG))     _log.debug("con: " + con + " closeSentOn: "+ con.getCloseSentOn()+ " closeReceivedOn: "+ con.getCloseReceivedOn());
  }
  return ok;
}
