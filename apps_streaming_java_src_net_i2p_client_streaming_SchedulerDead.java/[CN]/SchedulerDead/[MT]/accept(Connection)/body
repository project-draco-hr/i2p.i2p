{
  boolean ok=(con != null) && (con.getResetReceived()) || ((con.getCloseSentOn() > 0) && (con.getCloseReceivedOn() > 0) && (con.getUnackedPacketsReceived() <= 0)&& (con.getUnackedPacketsSent() <= 0)&& (con.getCloseSentOn() + SchedulerClosed.CLOSE_TIMEOUT <= _context.clock().now()));
  return ok;
}
