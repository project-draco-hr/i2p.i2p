{
  String cert=destination.substring(512);
  if (cert.equals("AAAA"))   return _("DSA 1024 bit");
  byte[] enc=Base64.decode(cert);
  if (enc == null)   return "invalid";
  int type=enc[0] & 0xff;
  if (type != Certificate.CERTIFICATE_TYPE_KEY)   return _("DSA 1024 bit");
  int st=((enc[3] & 0xff) << 8) | (enc[4] & 0xff);
  if (st == 0)   return _("DSA 1024 bit");
  SigType stype=SigType.getByCode(st);
  if (stype == null)   return _("Type {0}",st);
  return stype.toString();
}
