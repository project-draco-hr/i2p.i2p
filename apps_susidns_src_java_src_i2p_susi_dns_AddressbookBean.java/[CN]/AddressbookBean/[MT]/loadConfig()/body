{
  long currentTime=System.currentTimeMillis();
  if (properties.size() > 0 && currentTime - configLastLoaded < 10000)   return;
  try {
    properties.clear();
    properties.load(new FileInputStream(ConfigBean.configFileName));
    if (properties.getProperty(PRIVATE_BOOK) == null)     properties.setProperty(PRIVATE_BOOK,DEFAULT_PRIVATE_BOOK);
    configLastLoaded=currentTime;
  }
 catch (  Exception e) {
    Debug.debug(e.getClass().getName() + ": " + e.getMessage());
  }
}
