{
  String filename=properties.getProperty(getBook() + "_addressbook");
  FileOutputStream fos=null;
  try {
    fos=new SecureFileOutputStream(ConfigBean.addressbookPrefix + filename);
    addressbook.store(fos,null);
  }
  finally {
    if (fos != null) {
      try {
        fos.close();
      }
 catch (      IOException ioe) {
      }
    }
  }
}
