{
  if (text != null && text.length() > 0) {
    String[] ccs=text.split(",");
    for (int i=0; i < ccs.length; i++) {
      String recipient=ccs[i].trim();
      if (validateAddress(recipient)) {
        String str=getAddress(recipient);
        if (str != null && str.length() > 0) {
          recipients.add(str);
        }
 else {
          ok=false;
        }
      }
 else {
        ok=false;
      }
    }
  }
  return ok;
}
