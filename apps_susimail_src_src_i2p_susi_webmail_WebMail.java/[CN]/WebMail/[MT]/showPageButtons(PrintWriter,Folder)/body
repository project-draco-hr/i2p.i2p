{
  out.println("<br>" + (folder.isFirstPage() ? button2(FIRSTPAGE,_t("First")) + "&nbsp;" + button2(PREVPAGE,_t("Previous")) : button(FIRSTPAGE,_t("First")) + "&nbsp;" + button(PREVPAGE,_t("Previous"))) + " &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;"+ _t("Page {0} of {1}",folder.getCurrentPage(),folder.getPages())+ "&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "+ (folder.isLastPage() ? button2(NEXTPAGE,_t("Next")) + "&nbsp;" + button2(LASTPAGE,_t("Last")) : button(NEXTPAGE,_t("Next")) + "&nbsp;" + button(LASTPAGE,_t("Last"))));
}
