{
  out.println("<br>" + (folder.isFirstPage() ? button2(FIRSTPAGE,_("First")) + "&nbsp;" + button2(PREVPAGE,_("Previous")) : button(FIRSTPAGE,_("First")) + "&nbsp;" + button(PREVPAGE,_("Previous"))) + " &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;"+ _("Page {0} of {1}",folder.getCurrentPage(),folder.getPages())+ "&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; "+ (folder.isLastPage() ? button2(NEXTPAGE,_("Next")) + "&nbsp;" + button2(LASTPAGE,_("Last")) : button(NEXTPAGE,_("Next")) + "&nbsp;" + button(LASTPAGE,_("Last"))));
}
