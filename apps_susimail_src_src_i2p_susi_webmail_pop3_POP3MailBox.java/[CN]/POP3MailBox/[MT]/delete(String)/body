{
  Debug.debug(Debug.DEBUG,"delete(" + uidl + ")");
synchronized (synchronizer) {
    try {
      checkConnection();
    }
 catch (    IOException ioe) {
      Debug.debug(Debug.DEBUG,"Error deleting: " + ioe);
      return false;
    }
    int id=getIDfromUIDL(uidl);
    if (id < 0)     return false;
    return delete(id);
  }
}
