{
  if (entryIndexName == null)   return -1;
  if (entryIndexName.endsWith(".snd"))   entryIndexName=entryIndexName.substring(0,entryIndexName.length() - 4);
  int beginSize=entryIndexName.lastIndexOf('_');
  if ((beginSize <= 0) || (beginSize >= entryIndexName.length() - 3))   return -1;
  try {
    String sz=entryIndexName.substring(beginSize + 1,entryIndexName.length() - 2);
    return Integer.parseInt(sz);
  }
 catch (  NumberFormatException nfe) {
    nfe.printStackTrace();
  }
  return -1;
}
