{
  Properties rv=new Properties();
  if (filename.exists()) {
    FileInputStream in=null;
    try {
      in=new FileInputStream(filename);
      rv.load(in);
    }
  finally {
      if (in != null)       try {
        in.close();
      }
 catch (      IOException ioe) {
      }
    }
  }
  return rv;
}
