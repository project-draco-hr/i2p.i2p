{
  if ((location == null) || (location.trim().length() <= 0))   return false;
  if (false) {
    try {
      Destination d=new Destination(location);
      return (d.getPublicKey() != null);
    }
 catch (    DataFormatException dfe) {
      _log.error("Error validating address location",dfe);
      return false;
    }
  }
 else {
    return true;
  }
}
