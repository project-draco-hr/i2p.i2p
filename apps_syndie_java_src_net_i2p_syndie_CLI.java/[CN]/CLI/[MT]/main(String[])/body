{
  if (args.length < 2) {
    System.err.print(USAGE);
    return;
  }
  String command=args[1];
  if ("createBlog".equals(command))   createBlog(args);
 else   if ("listMyBlogs".equals(command))   listMyBlogs(args);
 else   if ("createEntry".equals(command))   createEntry(args);
 else   if ("listTags".equals(command))   listPaths(args);
 else   if ("listEntries".equals(command))   listEntries(args);
 else   if ("regenerateIndex".equals(command))   regenerateIndex(args);
 else   if ("renderEntry".equals(command))   renderEntry(args);
 else   System.out.print(USAGE);
}
