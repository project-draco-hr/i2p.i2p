{
  long end=max + System.currentTimeMillis();
  while (!_complete) {
    long now=System.currentTimeMillis();
    if (now >= end)     return false;
    try {
synchronized (OnCompletion.this) {
        OnCompletion.this.wait(end - now);
      }
    }
 catch (    InterruptedException ie) {
    }
  }
  return true;
}
