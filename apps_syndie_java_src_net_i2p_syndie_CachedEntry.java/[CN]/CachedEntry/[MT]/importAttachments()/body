{
  if (_attachments == null) {
    List attachments=new ArrayList();
    int i=0;
    while (true) {
      File meta=new File(_entryDir,EntryExtractor.ATTACHMENT_PREFIX + i + EntryExtractor.ATTACHMENT_META_SUFFIX);
      if (meta.exists())       attachments.add(new CachedAttachment(i,meta));
 else       break;
      i++;
    }
    Attachment a[]=new Attachment[attachments.size()];
    for (i=0; i < a.length; i++)     a[i]=(Attachment)attachments.get(i);
    _attachments=a;
  }
  return;
}
