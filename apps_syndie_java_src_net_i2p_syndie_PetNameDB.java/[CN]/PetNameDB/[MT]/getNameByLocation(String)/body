{
  if (location == null)   return null;
synchronized (_names) {
    for (Iterator iter=_names.values().iterator(); iter.hasNext(); ) {
      PetName name=(PetName)iter.next();
      if ((name.getLocation() != null) && (name.getLocation().trim().equals(location.trim())))       return name.getName();
    }
  }
  return null;
}
