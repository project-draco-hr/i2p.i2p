{
  Sucker sucker=new Sucker();
  boolean ok=sucker.parseArgs(args);
  if (!ok) {
    System.out.println("sucker --load $urlToFeed \n" + "--proxyhost <host> \n" + "--proxyport <port> \n"+ "--importenclosures true \n"+ "--importrefs true \n"+ "--tag feed \n"+ "--outputdir ./sucker_out \n"+ "--exec pushscript.sh OUTPUTDIR UNIQUEID ENTRYTIMESTAMP \n"+ "--history ./sucker.history");
    System.exit(1);
  }
  sucker.suck();
}
