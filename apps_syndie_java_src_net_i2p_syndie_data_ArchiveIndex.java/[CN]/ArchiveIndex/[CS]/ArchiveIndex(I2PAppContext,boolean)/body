{
  _context=ctx;
  _log=ctx.logManager().getLog(ArchiveIndex.class);
  _blogs=new ArrayList();
  _newestBlogs=new ArrayList();
  _newestEntries=new ArrayList();
  _headers=new Properties();
  _replies=Collections.synchronizedMap(new HashMap());
