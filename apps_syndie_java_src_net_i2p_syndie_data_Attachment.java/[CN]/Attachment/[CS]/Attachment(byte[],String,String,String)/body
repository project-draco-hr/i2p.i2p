{
  _data=data;
  _keys=new ArrayList();
  _values=new ArrayList();
  _keys.add(NAME);
  _values.add(name);
  if ((description != null) && (description.trim().length() > 0)) {
    _keys.add(DESCRIPTION);
    _values.add(description);
  }
  if ((mimeType != null) && (mimeType.trim().length() > 0)) {
    _keys.add(MIMETYPE);
    _values.add(mimeType);
  }
  createMeta();
}
