{
  StringBuffer buf=new StringBuffer(512);
  for (int i=0; i < _optionNames.length; i++) {
    if ((includeRealSignature) || (!SIGNATURE.equals(_optionNames[i])))     buf.append(_optionNames[i]).append(':').append(_optionValues[i]).append('\n');
  }
  String s=buf.toString();
  out.write(s.getBytes());
}
