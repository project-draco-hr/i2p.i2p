{
  this();
  _entryURI=uri;
  if ((smlData == null) || (smlData.length <= 0))   _entryData=new Entry(null);
 else   _entryData=new Entry(DataHelper.getUTF8(smlData));
  setHeader(HEADER_BLOGKEY,Base64.encode(uri.getKeyHash().getData()));
  StringBuffer buf=new StringBuffer();
  for (int i=0; tags != null && i < tags.length; i++)   buf.append(tags[i]).append('\t');
  setHeader(HEADER_BLOGTAGS,buf.toString());
  if (uri.getEntryId() < 0)   uri.setEntryId(System.currentTimeMillis());
  setHeader(HEADER_ENTRYID,Long.toString(uri.getEntryId()));
}
