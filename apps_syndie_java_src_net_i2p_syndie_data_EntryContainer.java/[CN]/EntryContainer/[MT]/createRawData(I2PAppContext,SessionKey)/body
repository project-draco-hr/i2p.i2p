{
  byte raw[]=createRawData();
  if (entryKey != null) {
    byte iv[]=new byte[16];
    ctx.random().nextBytes(iv);
    byte rv[]=new byte[raw.length + iv.length];
    ctx.aes().encrypt(raw,0,rv,iv.length,entryKey,iv,raw.length);
    System.arraycopy(iv,0,rv,0,iv.length);
    return rv;
  }
 else {
    return raw;
  }
}
