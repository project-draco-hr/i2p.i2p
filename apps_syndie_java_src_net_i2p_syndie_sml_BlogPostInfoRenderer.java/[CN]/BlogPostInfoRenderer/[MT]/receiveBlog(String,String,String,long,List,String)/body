{
  if (_log.shouldLog(Log.DEBUG))   _log.debug("Receiving the blog: " + name + "/"+ hash+ "/"+ tag+ "/"+ entryId+ "/"+ locations+ ": "+ description);
  byte blogData[]=Base64.decode(hash);
  if ((blogData == null) || (blogData.length != Hash.HASH_LENGTH))   return;
  Blog b=new Blog();
  b.name=name;
  b.hash=hash;
  b.tag=tag;
  b.entryId=entryId;
  b.locations=locations;
  if (!_blogs.contains(b))   _blogs.add(b);
}
