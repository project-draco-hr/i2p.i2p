{
  BlogURI postURI=null;
  Attachment attachments[]=null;
  if (_entry != null) {
    attachments=_entry.getAttachments();
    postURI=_entry.getURI();
  }
  renderAttachments(postURI,attachments);
  renderBlogs(postURI);
  renderLinks(postURI);
  renderAddresses(postURI);
  renderArchives(postURI);
}
