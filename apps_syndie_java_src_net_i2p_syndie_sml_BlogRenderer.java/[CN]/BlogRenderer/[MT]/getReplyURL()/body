{
  String subject=(String)_headers.get(HEADER_SUBJECT);
  if (subject != null) {
    if (!subject.startsWith("re:"))     subject="re: " + subject;
  }
 else {
    subject="re: ";
  }
  return "post.jsp?" + PostServlet.PARAM_PARENT + "="+ Base64.encode(_entry.getURI().getKeyHash().toBase64() + "/" + _entry.getURI().getEntryId())+ "&amp;"+ PostServlet.PARAM_SUBJECT+ "="+ sanitizeTagParam(subject)+ "&amp;";
}
