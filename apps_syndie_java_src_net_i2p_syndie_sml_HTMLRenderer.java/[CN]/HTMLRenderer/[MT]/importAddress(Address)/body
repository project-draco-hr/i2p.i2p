{
  if (I2PAppContext.getGlobalContext().getProperty("syndie.addressExport","false").equalsIgnoreCase("true") && I2PAppContext.getGlobalContext().namingService().lookup(a.name) == null && a.schema.equalsIgnoreCase("i2p")) {
    PetName pn=new PetName(a.name,a.schema,a.protocol,a.location);
    I2PAppContext.getGlobalContext().petnameDb().add(pn);
    try {
      I2PAppContext.getGlobalContext().petnameDb().store();
    }
 catch (    IOException ioe) {
    }
  }
}
