{
  Address a=new Address();
  a.name=name;
  a.schema=schema;
  a.location=location;
  a.protocol=protocol;
  if (!_addresses.contains(a))   _addresses.add(a);
  if (!continueBody()) {
    return;
  }
  if ((schema == null) || (location == null))   return;
  String knownName=null;
  if (_user != null)   knownName=_user.getPetNameDB().getNameByLocation(location);
  if (knownName != null) {
    _bodyBuffer.append(sanitizeString(anchorText));
  }
 else {
    _bodyBuffer.append(sanitizeString(anchorText));
  }
}
