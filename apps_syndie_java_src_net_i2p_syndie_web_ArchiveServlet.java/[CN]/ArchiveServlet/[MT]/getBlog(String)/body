{
  int start=0;
  int end=-1;
  int len=path.length();
  for (int i=0; i < len; i++) {
    if (path.charAt(i) != '/') {
      start=i;
      break;
    }
  }
  for (int j=start + 1; j < len; j++) {
    if (path.charAt(j) == '/') {
      end=j;
      break;
    }
  }
  if (end < 0)   end=len;
  String rv=path.substring(start,end);
  return rv;
}
