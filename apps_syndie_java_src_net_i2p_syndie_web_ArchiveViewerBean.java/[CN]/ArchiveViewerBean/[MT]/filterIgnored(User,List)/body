{
  for (int i=0; i < uris.size(); i++) {
    BlogURI uri=(BlogURI)uris.get(i);
    Hash k=uri.getKeyHash();
    if (k == null)     continue;
    PetName pn=user.getPetNameDB().getByLocation(k.toBase64());
    if ((pn != null) && (pn.isMember("Ignore"))) {
      uris.remove(i);
      i--;
    }
  }
}
