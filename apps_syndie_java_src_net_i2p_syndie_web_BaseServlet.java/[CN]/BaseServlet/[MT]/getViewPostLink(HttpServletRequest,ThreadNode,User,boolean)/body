{
  return ThreadedHTMLRenderer.getViewPostLink(req.getRequestURI(),node,user,isPermalink,req.getParameter(ThreadedHTMLRenderer.PARAM_OFFSET),req.getParameter(ThreadedHTMLRenderer.PARAM_TAGS),req.getParameter(ThreadedHTMLRenderer.PARAM_AUTHOR),Boolean.valueOf(req.getParameter(ThreadedHTMLRenderer.PARAM_THREAD_AUTHOR)).booleanValue());
}
