{
  for (int i=0; i < _groups.size(); i++) {
    List grp=(List)_groups.get(i);
    if (grp.size() > 0) {
      PetName pn=(PetName)grp.get(0);
      if ((pn.getGroupCount() == 0) && ((name == null) || (name.length() <= 0)))       return grp;
      String curGroup=pn.getGroup(0);
      if (curGroup.equals(name))       return grp;
    }
  }
  return null;
}
