{
  for (int i=0; i < getGroupCount(); i++) {
    List group=getGroup(i);
    for (int j=0; j < group.size(); j++) {
      PetName pn=(PetName)group.get(j);
      if (pn.getName().equals(name)) {
        group.remove(j);
        if (group.size() <= 0)         _groups.remove(group);
        _updated=true;
        return;
      }
    }
  }
}
