{
  return getAddToGroupLink(user,author,group,req.getRequestURI(),req.getParameter(ThreadedHTMLRenderer.PARAM_VISIBLE),req.getParameter(ThreadedHTMLRenderer.PARAM_VIEW_POST),req.getParameter(ThreadedHTMLRenderer.PARAM_VIEW_THREAD),req.getParameter(ThreadedHTMLRenderer.PARAM_OFFSET),req.getParameter(ThreadedHTMLRenderer.PARAM_TAGS));
}
