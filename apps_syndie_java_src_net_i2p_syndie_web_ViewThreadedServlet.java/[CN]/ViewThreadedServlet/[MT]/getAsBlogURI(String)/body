{
  if (uri != null) {
    int split=uri.indexOf('/');
    if ((split <= 0) || (split + 1 >= uri.length()))     return null;
    String blog=uri.substring(0,split);
    String id=uri.substring(split + 1);
    try {
      Hash hash=new Hash();
      hash.fromBase64(blog);
      long msgId=Long.parseLong(id);
      if (msgId > 0)       return new BlogURI(hash,msgId);
    }
 catch (    DataFormatException dfe) {
      return null;
    }
catch (    NumberFormatException nfe) {
      return null;
    }
  }
  return null;
}
