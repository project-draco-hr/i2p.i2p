{
  FileInputStream fileInputStream=null;
  boolean rv=true;
  try {
    fileInputStream=new FileInputStream(_configFile);
    _properties.load(fileInputStream);
  }
 catch (  Exception e) {
    rv=false;
  }
 finally {
    if (fileInputStream != null) {
      try {
        fileInputStream.close();
      }
 catch (      IOException e) {
      }
    }
  }
  return rv;
}
