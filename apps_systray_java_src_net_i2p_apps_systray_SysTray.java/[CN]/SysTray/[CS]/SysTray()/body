{
  if (!_configFile.init("systray.config"))   _configFile.setProperty("browser","default");
  _browserString=_configFile.getProperty("browser","default");
  if (!(new File("router.config")).exists())   openRouterConsole();
  if (!System.getProperty("os.name").startsWith("Windows")) {
    _instance=null;
    return;
  }
  _sysTrayMenuIcon.addSysTrayMenuListener(this);
  createSysTrayMenu();
}
