{
  SysTray impl=getImpl();
  if (impl == null) {
    System.out.println("No systray implementation found");
    System.exit(1);
  }
  System.err.println("Hit ^C to exit");
  Thread t=Thread.currentThread();
synchronized (t) {
    try {
      t.wait();
    }
 catch (    InterruptedException ie) {
    }
  }
}
