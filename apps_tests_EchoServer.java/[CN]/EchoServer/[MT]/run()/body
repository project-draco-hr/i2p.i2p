{
  try {
    InputStream in=s.getInputStream();
    OutputStream out=s.getOutputStream();
    byte[] b=new byte[4096];
    int len;
    while ((len=in.read(b)) != -1) {
      out.write(b,0,len);
    }
  }
 catch (  SocketException ex) {
  }
catch (  IOException ex) {
    ex.printStackTrace();
  }
}
