{
  if (getCPUFamily() >= 15) {
    return true;
  }
 else   if (getCPUExtendedModel() == 2 && (getCPUFamily() == 6)) {
  }
 else   if (getCPUExtendedModel() == 1 && (getCPUFamily() == 6 && (getCPUModel() == 10 || getCPUModel() == 13 || getCPUModel() == 14))) {
    return true;
  }
 else   if (getCPUExtendedModel() == 0 && getCPUFamily() == 6 && getCPUModel() == 15) {
    return true;
  }
 else {
    return false;
  }
}
