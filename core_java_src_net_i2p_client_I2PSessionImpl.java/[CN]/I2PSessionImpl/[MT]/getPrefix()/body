{
  StringBuilder buf=new StringBuilder();
  buf.append('[');
  String s=_options.getProperty("inbound.nickname");
  if (s != null)   buf.append(s);
 else   buf.append(getClass().getSimpleName());
  buf.append(" #");
  if (_sessionId != null)   buf.append(_sessionId.getSessionId());
 else   buf.append("n/a");
  buf.append("]: ");
  return buf.toString();
}
