{
  Properties options=new Properties();
  options.putAll(filter(opts));
  if ((!_context.isRouterContext()) && _context.getBooleanProperty("i2cp.auth") && ((!opts.containsKey("i2cp.username")) || (!opts.containsKey("i2cp.password")))) {
    String configUser=_context.getProperty("i2cp.username");
    String configPW=_context.getProperty("i2cp.password");
    if (configUser != null && configPW != null) {
      options.setProperty("i2cp.username",configUser);
      options.setProperty("i2cp.password",configPW);
    }
  }
  if (options.getProperty(I2PClient.PROP_FAST_RECEIVE) == null)   options.setProperty(I2PClient.PROP_FAST_RECEIVE,"true");
  if (options.getProperty(I2PClient.PROP_RELIABILITY) == null)   options.setProperty(I2PClient.PROP_RELIABILITY,"none");
  return options;
}
