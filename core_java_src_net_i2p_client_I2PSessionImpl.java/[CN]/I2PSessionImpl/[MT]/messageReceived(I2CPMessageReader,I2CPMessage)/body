{
  I2CPMessageHandler handler=_handlerMap.getHandler(message.getType());
  if (handler == null) {
    if (_log.shouldLog(Log.WARN))     _log.warn(getPrefix() + "Unknown message or unhandleable message received: type = " + message.getType());
  }
 else {
    if (_log.shouldLog(Log.DEBUG))     _log.debug(getPrefix() + "Message received of type " + message.getType()+ " to be handled by "+ handler.getClass().getSimpleName());
    handler.handleMessage(message,this);
  }
}
