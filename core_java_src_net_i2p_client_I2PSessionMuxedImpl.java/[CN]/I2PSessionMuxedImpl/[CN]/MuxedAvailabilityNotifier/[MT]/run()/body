{
  _alive=true;
  while (true) {
    MsgData msg;
    try {
      msg=_msgs.take();
    }
 catch (    InterruptedException ie) {
      continue;
    }
    if (msg.size == POISON_SIZE)     break;
    try {
      _demultiplexer.messageAvailable(I2PSessionMuxedImpl.this,msg.id,msg.size,msg.proto,msg.fromPort,msg.toPort);
    }
 catch (    Exception e) {
      _log.error("Error notifying app of message availability");
    }
  }
}
