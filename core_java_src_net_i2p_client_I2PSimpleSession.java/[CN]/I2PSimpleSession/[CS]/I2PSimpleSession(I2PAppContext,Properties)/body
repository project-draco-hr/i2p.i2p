{
  _context=context;
  _log=context.logManager().getLog(I2PSimpleSession.class);
  _handlerMap=new SimpleMessageHandlerMap(context);
  _closed=true;
  _closing=false;
  _availabilityNotifier=new AvailabilityNotifier();
  if (options == null)   options=System.getProperties();
  loadConfig(options);
}
