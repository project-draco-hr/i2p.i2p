{
synchronized (_stateLock) {
    if (_state != State.OPEN) {
      _state=State.OPENING;
    }
  }
  _primary.connect();
synchronized (_stateLock) {
    if (_state != State.OPEN) {
      Thread notifier=new I2PAppThread(_availabilityNotifier,"ClientNotifier " + getPrefix(),true);
      notifier.start();
      _state=State.OPEN;
    }
  }
}
