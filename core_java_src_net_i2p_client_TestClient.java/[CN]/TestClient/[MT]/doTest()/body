{
  Thread test1=new I2PThread(new Runnable(){
    public void run(){
      (new TestClient()).runTest("test1.keyfile",true);
    }
  }
);
  Thread test2=new I2PThread(new Runnable(){
    public void run(){
      (new TestClient()).runTest("test2.keyfile",false);
    }
  }
);
  test1.start();
  test2.start();
  _log.debug("Test threads started");
}
