{
  try {
    _socket=new ServerSocket(LISTEN_PORT);
  }
 catch (  IOException ioe) {
    _log.error("Error listening",ioe);
    return;
  }
  while (true) {
    try {
      Socket socket=_socket.accept();
      runConnection(socket);
    }
 catch (    IOException ioe) {
      _log.error("Server error accepting",ioe);
    }
  }
}
