{
  if (this.valid)   return;
  if (rxSign == null || rxSign.getData() == null || rxDest == null || rxDest.getSigningPublicKey() == null)   throw new I2PInvalidDatagramException("Datagram not yet read");
  if (!this.dsaEng.verifySignature(rxSign,rxHash.getData(),rxDest.getSigningPublicKey()))   throw new I2PInvalidDatagramException("Incorrect I2P repliable datagram signature");
  this.valid=true;
}
