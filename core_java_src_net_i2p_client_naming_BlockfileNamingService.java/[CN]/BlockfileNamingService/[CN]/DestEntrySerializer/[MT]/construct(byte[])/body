{
  DestEntry rv=new DestEntry();
  Destination dest=new Destination();
  rv.dest=dest;
  ByteArrayInputStream bais=new ByteArrayInputStream(b);
  try {
    rv.props=DataHelper.readProperties(bais);
    dest.readBytes(bais);
  }
 catch (  IOException ioe) {
    return null;
  }
catch (  DataFormatException dfe) {
    return null;
  }
  return rv;
}
