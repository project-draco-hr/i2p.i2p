{
  if (VERSION.equals(version))   return false;
  if (!OLD_VERSION.equals(version))   throw new IOException("Bad db version: " + version);
  if (!bf.file.canWrite()) {
    if (_log.shouldLog(Log.WARN))     _log.warn("Not upgrading read-only database version " + version);
    return false;
  }
  return true;
}
