{
  if (hostname.length() >= DEST_SIZE)   return lookupBase64(hostname);
  hostname=hostname.toLowerCase();
  if (hostname.length() == 60 && hostname.endsWith(".b32.i2p"))   return null;
  Destination d=getCache(hostname);
  if (d != null)   return d;
  String key=fetchAddr(hostname);
  if (key != null) {
    _log.error("Success: " + hostname);
    d=lookupBase64(key);
    putCache(hostname,d);
    return d;
  }
  return null;
}
