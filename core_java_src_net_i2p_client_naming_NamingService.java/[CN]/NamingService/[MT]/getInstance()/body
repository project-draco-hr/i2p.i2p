{
  if (instance == null) {
    String impl=System.getProperty(PROP_IMPL,DEFAULT_IMPL);
    try {
      instance=(NamingService)Class.forName(impl).newInstance();
    }
 catch (    Exception ex) {
      _log.error("Cannot loadNaming service implementation",ex);
      instance=new DummyNamingService();
    }
  }
  return instance;
}
