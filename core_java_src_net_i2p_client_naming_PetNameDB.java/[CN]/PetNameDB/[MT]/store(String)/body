{
  Writer out=null;
  try {
    out=new OutputStreamWriter(new FileOutputStream(location),"UTF-8");
    for (Iterator names=getNames().iterator(); names.hasNext(); ) {
      PetName name=get((String)names.next());
      if (name != null)       out.write(name.toString() + "\n");
    }
  }
  finally {
    out.close();
  }
}
