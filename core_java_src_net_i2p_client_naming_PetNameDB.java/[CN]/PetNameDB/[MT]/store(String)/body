{
  Writer out=null;
  try {
    out=new OutputStreamWriter(new FileOutputStream(location),"UTF-8");
    for (Iterator iter=iterator(); iter.hasNext(); ) {
      PetName name=(PetName)iter.next();
      if (name != null)       out.write(name.toString() + "\n");
    }
  }
  finally {
    out.close();
  }
}
