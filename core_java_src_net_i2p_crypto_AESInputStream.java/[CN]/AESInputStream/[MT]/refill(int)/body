{
  if (!_eofFound) {
    byte buf[]=new byte[max];
    int read=in.read(buf);
    if (read == -1) {
      _eofFound=true;
    }
 else     if (read > 0) {
      _cumulativeRead+=read;
      _encryptedBuf.write(buf,0,read);
    }
  }
  if (false)   return;
  if (_encryptedBuf.size() > 0) {
    if (_encryptedBuf.size() >= DECRYPT_SIZE) {
      decrypt();
    }
 else {
      if (_eofFound) {
      }
 else {
      }
    }
  }
}
