{
  SessionKey key=ctx.keyGenerator().generateSessionKey();
  byte iv[]=new byte[16];
  byte orig[]=new byte[16];
  byte data[]=new byte[16];
  ctx.random().nextBytes(iv);
  ctx.random().nextBytes(orig);
  CryptixAESEngine aes=new CryptixAESEngine(ctx);
  aes.encryptBlock(orig,0,key,data,0);
  aes.decryptBlock(data,0,key,data,0);
  if (!DataHelper.eq(data,orig))   throw new RuntimeException("block D(E(orig)) != orig");
 else   System.out.println("block D(E(orig)) == orig");
}
