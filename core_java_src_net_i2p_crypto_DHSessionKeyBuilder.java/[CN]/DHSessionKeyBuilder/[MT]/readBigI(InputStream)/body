{
  byte Y[]=new byte[256];
  int read=DataHelper.read(in,Y);
  if (read != 256) {
    return null;
  }
  if (1 == (Y[0] & 0x80)) {
    if (_log.shouldLog(Log.DEBUG))     _log.debug("High bit set");
    byte Y2[]=new byte[257];
    System.arraycopy(Y,0,Y2,1,256);
    Y=Y2;
  }
  return new NativeBigInteger(Y);
}
