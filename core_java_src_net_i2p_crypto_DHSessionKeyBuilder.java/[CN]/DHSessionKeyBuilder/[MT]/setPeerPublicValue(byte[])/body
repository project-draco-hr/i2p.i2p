{
  if (val.length != 256)   throw new IllegalArgumentException("Peer public value must be exactly 256 bytes");
  if (1 == (val[0] & 0x80)) {
    if (_log.shouldLog(Log.DEBUG))     _log.debug("High bit set");
    byte val2[]=new byte[257];
    System.arraycopy(val,0,val2,1,256);
    val=val2;
  }
  setPeerPublicValue(new NativeBigInteger(1,val));
}
