{
  SigType type=signingKey.getType();
  if (type != SigType.DSA_SHA1) {
    try {
      return altSign(data,signingKey);
    }
 catch (    GeneralSecurityException gse) {
      if (_log.shouldLog(Log.WARN))       _log.warn(type + " Sign Fail",gse);
      return null;
    }
  }
  if (_useJavaLibs) {
    try {
      return altSignSHA1(data,signingKey);
    }
 catch (    GeneralSecurityException gse) {
      if (_log.shouldLog(Log.WARN))       _log.warn("Lib Sign Fail, privkey = " + signingKey,gse);
    }
  }
  return sign(data,0,data.length,signingKey);
}
