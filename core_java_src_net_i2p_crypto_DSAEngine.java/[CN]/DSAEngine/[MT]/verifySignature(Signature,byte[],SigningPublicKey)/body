{
  boolean rv;
  if (_useJavaLibs) {
    try {
      rv=altVerifySigSHA1(signature,signedData,verifyingKey);
      if ((!rv) && _log.shouldLog(Log.WARN))       _log.warn("Lib Verify Fail, sig =\n" + signature + "\npubkey =\n"+ verifyingKey);
      return rv;
    }
 catch (    GeneralSecurityException gse) {
      if (_log.shouldLog(Log.WARN))       _log.warn("Lib Verify Fail, sig =\n" + signature + "\npubkey =\n"+ verifyingKey,gse);
    }
  }
  rv=verifySignature(signature,signedData,0,signedData.length,verifyingKey);
  if ((!rv) && _log.shouldLog(Log.WARN))   _log.warn("TheCrypto Verify Fail, sig =\n" + signature + "\npubkey =\n"+ verifyingKey);
  return rv;
}
