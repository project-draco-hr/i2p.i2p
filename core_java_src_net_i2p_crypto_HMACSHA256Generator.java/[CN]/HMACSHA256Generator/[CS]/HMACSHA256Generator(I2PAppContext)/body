{
  _context=context;
  _available=new ArrayList(32);
  _availableTmp=new ArrayList(32);
  if ("true".equals(context.getProperty("i2p.HMACMD5",Boolean.toString(DEFAULT_USE_MD5).toLowerCase())))   _useMD5=true;
 else   _useMD5=false;
  if ("true".equals(context.getProperty("i2p.HMACBrokenSize","true")))   _macSize=32;
 else   _macSize=(_useMD5 ? 16 : 32);
}
