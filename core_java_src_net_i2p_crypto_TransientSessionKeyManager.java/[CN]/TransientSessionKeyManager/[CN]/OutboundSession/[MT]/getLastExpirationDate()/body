{
  long last=0;
synchronized (_tagSets) {
    for (Iterator<TagSet> iter=_tagSets.iterator(); iter.hasNext(); ) {
      TagSet set=iter.next();
      if ((set.getDate() > last) && (!set.getTags().isEmpty()))       last=set.getDate();
    }
  }
  if (last > 0)   return last + SESSION_TAG_DURATION_MS;
 else   return -1;
}
