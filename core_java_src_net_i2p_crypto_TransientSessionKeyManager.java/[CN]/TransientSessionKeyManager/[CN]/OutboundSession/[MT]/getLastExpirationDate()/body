{
  long last=0;
synchronized (_tagSets) {
    for (Iterator iter=_tagSets.iterator(); iter.hasNext(); ) {
      TagSet set=(TagSet)iter.next();
      if ((set.getDate() > last) && (set.getTagCount() > 0))       last=set.getDate();
    }
  }
  if (last > 0)   return last + SESSION_TAG_DURATION_MS;
 else   return -1;
}
