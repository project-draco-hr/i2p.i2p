{
  _lastUsed=_context.clock().now();
  if (_currentKey != null) {
    if (!_currentKey.equals(key)) {
      int dropped=0;
      List sets=_tagSets;
      _tagSets=new ArrayList();
      for (int i=0; i < sets.size(); i++) {
        TagSet set=(TagSet)sets.get(i);
        dropped+=set.getTags().size();
      }
      if (_log.shouldLog(Log.INFO))       _log.info("Rekeyed from " + _currentKey + " to "+ key+ ": dropping "+ dropped+ " session tags");
    }
  }
  _currentKey=key;
}
