{
  if (key == null)   throw new IllegalArgumentException("Missing key");
  if (tags == null)   throw new IllegalArgumentException("Missing tags");
  _sessionTags=tags;
  _key=key;
  _date=date;
  if (true)   _createdBy=new Exception("Created by: key=" + _key.toBase64() + " on "+ new Date(I2PAppContext.getGlobalContext().clock().now())+ " via "+ Thread.currentThread().getName());
}
