{
  OutboundSession sess=getSession(target);
  if (sess == null) {
    if (_log.shouldLog(Log.DEBUG))     _log.debug("No session for " + target);
    return null;
  }
  if (sess.getCurrentKey().equals(key)) {
    SessionTag nxt=sess.consumeNext();
    if (_log.shouldLog(Log.DEBUG))     _log.debug("Tag consumed: " + nxt + " with key: "+ key.toBase64());
    return nxt;
  }
  if (_log.shouldLog(Log.DEBUG))   _log.debug("Key does not match existing key, no tag");
  return null;
}
