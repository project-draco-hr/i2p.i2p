{
  FileInputStream in=null;
  try {
    in=new FileInputStream(inputFile);
    byte data[]=new byte[VERSION_BYTES];
    int read=DataHelper.read(in,data);
    if (read != VERSION_BYTES)     return null;
    for (int i=0; i < VERSION_BYTES; i++)     if (data[i] == 0x00)     return new String(data,0,i,"UTF-8");
    return new String(data,"UTF-8");
  }
 catch (  UnsupportedEncodingException uee) {
    throw new RuntimeException("wtf, your JVM doesnt support utf-8? " + uee.getMessage());
  }
catch (  IOException ioe) {
    return "";
  }
 finally {
    if (in != null)     try {
      in.close();
    }
 catch (    IOException ioe) {
    }
  }
}
