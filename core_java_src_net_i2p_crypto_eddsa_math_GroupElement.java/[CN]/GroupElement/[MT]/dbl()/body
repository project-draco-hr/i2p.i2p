{
switch (repr) {
case P2:
case P3:
    FieldElement XX, YY, B, A, AA, Yn, Zn;
  XX=X.square();
YY=Y.square();
B=Z.squareAndDouble();
A=X.add(Y);
AA=A.square();
Yn=YY.add(XX);
Zn=YY.subtract(XX);
return p1p1(curve,AA.subtract(Yn),Yn,Zn,B.subtract(Zn));
default :
throw new UnsupportedOperationException();
}
}
