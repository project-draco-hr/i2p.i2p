{
  int bnegative=Utils.negative(b);
  int babs=b - (((-bnegative) & b) << 1);
  GroupElement t=curve.getZero(Representation.PRECOMP).cmov(precmp[pos][0],Utils.equal(babs,1)).cmov(precmp[pos][1],Utils.equal(babs,2)).cmov(precmp[pos][2],Utils.equal(babs,3)).cmov(precmp[pos][3],Utils.equal(babs,4)).cmov(precmp[pos][4],Utils.equal(babs,5)).cmov(precmp[pos][5],Utils.equal(babs,6)).cmov(precmp[pos][6],Utils.equal(babs,7)).cmov(precmp[pos][7],Utils.equal(babs,8));
  GroupElement tminus=precomp(curve,t.Y,t.X,t.Z.negate());
  return t.cmov(tminus,bnegative);
}
