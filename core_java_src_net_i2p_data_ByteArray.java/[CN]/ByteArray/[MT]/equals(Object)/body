{
  if (o == null)   return false;
  if (o instanceof ByteArray) {
    ByteArray ba=(ByteArray)o;
    return compare(getData(),_offset,_valid,ba.getData(),ba.getOffset(),ba.getValid());
  }
  try {
    byte val[]=(byte[])o;
    return compare(getData(),_offset,_valid,val,0,val.length);
  }
 catch (  Throwable t) {
    return false;
  }
}
