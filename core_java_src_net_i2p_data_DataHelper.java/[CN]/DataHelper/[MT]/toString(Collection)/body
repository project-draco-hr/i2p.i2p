{
  StringBuffer buf=new StringBuffer();
  if (col != null) {
    for (Iterator iter=col.iterator(); iter.hasNext(); ) {
      Object o=iter.next();
      buf.append("[").append(o).append("]");
      if (iter.hasNext())       buf.append(", ");
    }
  }
 else {
    buf.append("null");
  }
  return buf.toString();
}
