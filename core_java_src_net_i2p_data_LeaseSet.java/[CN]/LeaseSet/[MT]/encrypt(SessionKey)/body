{
  if (_log.shouldLog(Log.WARN))   _log.warn("encrypting lease: " + _destination.calculateHash());
  try {
    encryp(key);
  }
 catch (  DataFormatException dfe) {
    _log.error("Error encrypting lease: " + _destination.calculateHash());
  }
catch (  IOException ioe) {
    _log.error("Error encrypting lease: " + _destination.calculateHash());
  }
}
