{
  StringBuffer buf=new StringBuffer(128);
  buf.append("[Payload: ");
  if (getUnencryptedData() != null)   buf.append("\n\tData: ").append(DataHelper.toString(getUnencryptedData(),16));
 else   buf.append("\n\tData: *encrypted* = [").append(DataHelper.toString(getEncryptedData(),16)).append("]");
  buf.append("]");
  return buf.toString();
}
