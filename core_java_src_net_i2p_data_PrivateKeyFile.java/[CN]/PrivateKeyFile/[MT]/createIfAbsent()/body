{
  if (!this.file.exists()) {
    OutputStream out=null;
    try {
      out=new SecureFileOutputStream(this.file);
      if (this.client != null)       this.client.createDestination(out);
 else       write();
    }
  finally {
      if (out != null) {
        try {
          out.close();
        }
 catch (        IOException ioe) {
        }
      }
    }
  }
  return getDestination();
}
