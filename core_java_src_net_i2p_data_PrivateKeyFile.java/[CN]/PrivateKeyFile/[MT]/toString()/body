{
  StringBuffer s=new StringBuffer(128);
  s.append("Dest: ");
  s.append(this.dest.toBase64());
  s.append("\nContains: ");
  s.append(this.dest);
  s.append("\nPrivate Key: ");
  s.append(this.privKey);
  s.append("\nSigining Private Key: ");
  s.append(this.signingPrivKey);
  s.append("\n");
  return s.toString();
}
