{
  StringBuilder s=new StringBuilder(128);
  s.append("Dest: ");
  s.append(this.dest != null ? this.dest.toBase64() : "null");
  s.append("\nContains: ");
  s.append(this.dest);
  s.append("\nPrivate Key: ");
  s.append(this.privKey);
  s.append("\nSigining Private Key: ");
  s.append(this.signingPrivKey);
  s.append("\n");
  return s.toString();
}
