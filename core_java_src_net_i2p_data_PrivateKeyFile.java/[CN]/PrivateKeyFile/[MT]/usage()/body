{
  System.err.println("Usage: PrivateKeyFile [-c sigtype] filename (generates if nonexistent, then prints)\n" + "       PrivateKeyFile -h filename (generates if nonexistent, adds hashcash cert)\n" + "       PrivateKeyFile -h -e effort filename (specify HashCash effort instead of default " + HASH_EFFORT + ")\n"+ "       PrivateKeyFile -n filename (changes to null cert)\n"+ "       PrivateKeyFile -s filename signwithdestfile (generates if nonexistent, adds cert signed by 2nd dest)\n"+ "       PrivateKeyFile -t sigtype filename (changes to KeyCertificate of the given sig type)\n"+ "       PrivateKeyFile -u filename (changes to unknown cert)\n"+ "       PrivateKeyFile -x filename (changes to hidden cert)\n");
}
