{
  FileOutputStream out=null;
  try {
    out=new FileOutputStream(this.file);
    this.dest.writeBytes(out);
    this.privKey.writeBytes(out);
    this.signingPrivKey.writeBytes(out);
    out.flush();
  }
  finally {
    if (out != null) {
      try {
        out.close();
      }
 catch (      IOException ioe) {
      }
    }
  }
}
