{
  if (_ip != null)   return _ip;
  byte[] rv=null;
  String host=_options.getProperty(PROP_HOST);
  if (host != null) {
    rv=Addresses.getIP(host);
    if (rv != null && (host.replaceAll("[0-9\\.]","").length() == 0 || host.replaceAll("[0-9a-fA-F:]","").length() == 0)) {
      _ip=rv;
    }
  }
  return rv;
}
