{
  if (_options == null)   _options=new OrderedProperties();
synchronized (_options) {
    String caps=_options.getProperty(PROP_CAPABILITIES);
    if (caps == null)     _options.setProperty(PROP_CAPABILITIES,"" + cap);
 else     if (caps.indexOf(cap) == -1)     _options.setProperty(PROP_CAPABILITIES,caps + cap);
  }
}
