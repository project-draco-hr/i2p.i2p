{
  _validated=true;
  _isValid=super.verifySignature();
  if (!_isValid) {
    byte data[]=null;
    try {
      data=getBytes();
    }
 catch (    DataFormatException dfe) {
      _log.error("Error validating",dfe);
      return;
    }
    if (_log.shouldLog(Log.ERROR))     _log.error("Invalid [" + SHA256Generator.getInstance().calculateHash(data).toBase64() + (_log.shouldLog(Log.WARN) ? ("]\n" + toString()) : ""),new Exception("Signature failed"));
  }
}
