{
  StringBuilder buf=new StringBuilder(64);
  buf.append('[').append(getClass().getSimpleName()).append(' ').append(_type).append(": ");
  if (_data == null) {
    buf.append("null");
  }
 else {
    buf.append("size: ").append(Integer.toString(length()));
  }
  buf.append(']');
  return buf.toString();
}
