{
  StringBuilder buf=new StringBuilder(64);
  buf.append('[').append(getClass().getSimpleName()).append(": ");
  if (_data == null) {
    buf.append("null");
  }
 else   if (_length <= 32) {
    buf.append(toBase64());
  }
 else {
    buf.append("size: ").append(Integer.toString(_length));
  }
  buf.append(']');
  return buf.toString();
}
