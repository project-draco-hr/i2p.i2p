{
  String val=props.getProperty(prefix + name);
  if (val != null) {
    try {
      return Double.parseDouble(val);
    }
 catch (    NumberFormatException nfe) {
      _log.error("Error formatting " + val + " into a double",nfe);
    }
  }
 else {
    _log.error("Key " + prefix + name+ " does not exist");
  }
  return 0;
}
