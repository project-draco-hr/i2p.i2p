{
  if ((obj == null) || !(obj instanceof Rate))   return false;
  if (obj == this)   return true;
  Rate r=(Rate)obj;
  return _period == r.getPeriod() && _creationDate == r.getCreationDate() && _currentTotalValue == r.getCurrentTotalValue() && _currentEventCount == r.getCurrentEventCount() && _currentTotalEventTime == r.getCurrentTotalEventTime() && _lastTotalValue == r.getLastTotalValue() && _lastEventCount == r.getLastEventCount() && _lastTotalEventTime == r.getLastTotalEventTime() && _extremeTotalValue == r.getExtremeTotalValue() && _extremeEventCount == r.getExtremeEventCount() && _extremeTotalEventTime == r.getExtremeTotalEventTime() && _lifetimeTotalValue == r.getLifetimeTotalValue() && _lifetimeEventCount == r.getLifetimeEventCount() && _lifetimeTotalEventTime == r.getLifetimeTotalEventTime();
}
