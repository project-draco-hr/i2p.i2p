{
  if (++coalesceCounter % FREQ_COALESCE_RATE == 0) {
    for (    FrequencyStat stat : _frequencyStats.values()) {
      if (stat != null) {
        stat.coalesceStats();
      }
    }
  }
  for (Iterator<RateStat> iter=_rateStats.values().iterator(); iter.hasNext(); ) {
    RateStat stat=iter.next();
    if (stat != null) {
      stat.coalesceStats();
    }
  }
}
