{
  for (Iterator iter=getFrequencyNames().iterator(); iter.hasNext(); ) {
    String name=(String)iter.next();
    FrequencyStat stat=getFrequency(name);
    if (stat != null) {
      stat.coallesceStats();
    }
  }
  for (Iterator iter=getRateNames().iterator(); iter.hasNext(); ) {
    String name=(String)iter.next();
    RateStat stat=getRate(name);
    if (stat != null) {
      stat.coallesceStats();
    }
  }
}
