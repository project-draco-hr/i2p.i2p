{
  if (_context.getBooleanProperty(PROP_STAT_FULL))   return false;
  String required=_context.getProperty(PROP_STAT_REQUIRED,DEFAULT_STAT_REQUIRED);
  String req[]=required.split(",");
  for (int i=0; i < req.length; i++) {
    if (req[i].equals(statName))     return false;
    if (req[i].endsWith("*") && statName.startsWith(req[i].substring(0,req[i].length() - 2)))     return false;
  }
  return true;
}
