{
  if (_cache) {
synchronized (_available) {
      if (_available.size() > 0)       return (ByteArray)_available.remove(0);
    }
  }
  _lastOverflow=System.currentTimeMillis();
  byte data[]=new byte[_entrySize];
  ByteArray rv=new ByteArray(data);
  rv.setValid(0);
  rv.setOffset(0);
  return rv;
}
