{
  if (_cache) {
    if ((entry == null) || (entry.getData() == null))     return;
    entry.setValid(0);
    entry.setOffset(0);
    if (shouldZero)     Arrays.fill(entry.getData(),(byte)0x0);
synchronized (_available) {
      if (_available.size() < _maxCached)       _available.add(entry);
    }
  }
}
