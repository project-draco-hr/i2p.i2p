{
synchronized (this) {
    if (_entryBytes <= 7)     entry&=_longToEntryMask;
    if (entry < 0) {
      DataHelper.toLong(_longToEntry,0,_entryBytes,0 - entry);
      _longToEntry[0]|=(1 << 7);
    }
 else {
      DataHelper.toLong(_longToEntry,0,_entryBytes,entry);
    }
    return locked_add(_longToEntry,false);
  }
}
