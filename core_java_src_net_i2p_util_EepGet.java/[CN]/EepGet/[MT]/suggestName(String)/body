{
  int last=url.lastIndexOf('/');
  if ((last < 0) || (url.lastIndexOf('#') > last))   last=url.lastIndexOf('#');
  if ((last < 0) || (url.lastIndexOf('?') > last))   last=url.lastIndexOf('?');
  if ((last < 0) || (url.lastIndexOf('=') > last))   last=url.lastIndexOf('=');
  String name=null;
  if (last >= 0)   name=sanitize(url.substring(last + 1));
  if ((name != null) && (name.length() > 0))   return name;
 else   return sanitize(url);
}
