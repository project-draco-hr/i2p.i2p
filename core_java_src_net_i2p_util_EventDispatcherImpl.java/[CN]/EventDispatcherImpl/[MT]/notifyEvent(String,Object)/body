{
  if (_ignore)   return;
  if (args == null) {
    args="[null value]";
  }
synchronized (_events) {
    _events.put(eventName,args);
    _events.notifyAll();
synchronized (_attached) {
      Iterator it=_attached.iterator();
      EventDispatcher e;
      while (it.hasNext()) {
        e=(EventDispatcher)it.next();
        e.notifyEvent(eventName,args);
      }
    }
  }
}
