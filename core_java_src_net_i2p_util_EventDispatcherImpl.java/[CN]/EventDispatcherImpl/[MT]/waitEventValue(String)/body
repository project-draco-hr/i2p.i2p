{
  if (_ignore)   return null;
  Object val;
  _log.debug(this.hashCode() + ": waiting for [" + name+ "]");
  do {
synchronized (_events) {
      if (_events.containsKey(name)) {
        val=_events.get(name);
        break;
      }
      try {
        _events.wait(1 * 1000);
      }
 catch (      InterruptedException e) {
      }
    }
  }
 while (true);
  return val;
}
