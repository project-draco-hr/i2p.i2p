{
  try {
    java.util.Properties props=new java.util.Properties();
    props.setProperty("prng.buffers","12");
    I2PAppContext ctx=new I2PAppContext(props);
    RandomSource rand=ctx.random();
    byte[] buf=new byte[65536];
    while (true) {
      rand.nextBytes(buf);
      System.out.write(buf);
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
