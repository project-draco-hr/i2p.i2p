{
  byte data[]=new byte[4096];
  for (int i=0; i < data.length; i++)   data[i]=(byte)((i % 26) + 'a');
  boolean sent=HTTPSendData.postData("http://dev.i2p.net/cgi-bin/submitMessageHistory",data.length,new ByteArrayInputStream(data));
  _log.debug("Sent? " + sent);
  try {
    Thread.sleep(2000);
  }
 catch (  InterruptedException ie) {
