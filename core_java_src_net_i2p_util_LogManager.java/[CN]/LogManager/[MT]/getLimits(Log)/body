{
  ArrayList limits=null;
synchronized (_limits) {
    for (int i=0; i < _limits.size(); i++) {
      LogLimit limit=(LogLimit)_limits.get(i);
      if (limit.matches(log)) {
        if (limits == null)         limits=new ArrayList(4);
        limits.add(limit);
      }
    }
  }
  return limits;
}
