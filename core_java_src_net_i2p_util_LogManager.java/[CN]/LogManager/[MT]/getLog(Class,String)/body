{
  Log rv=null;
synchronized (_logs) {
    Log newLog=new Log(this,cls,name);
    if (_logs.containsKey(newLog.getScope())) {
      Log oldLog=(Log)_logs.get(newLog.getScope());
      rv=oldLog;
    }
 else {
      _logs.put(newLog.getScope(),newLog);
      rv=newLog;
    }
  }
  updateLimit(rv);
  return rv;
}
