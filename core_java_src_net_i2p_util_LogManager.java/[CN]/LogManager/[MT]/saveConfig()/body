{
  String config=createConfig();
  FileOutputStream fos=null;
  try {
    fos=new FileOutputStream(_locationFile);
    fos.write(config.getBytes());
    return true;
  }
 catch (  IOException ioe) {
    getLog(LogManager.class).error("Error saving the config",ioe);
    return false;
  }
 finally {
    if (fos != null)     try {
      fos.close();
    }
 catch (    IOException ioe) {
    }
  }
}
