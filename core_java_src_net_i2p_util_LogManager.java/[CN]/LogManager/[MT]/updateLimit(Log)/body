{
  List<LogLimit> limits=getLimits(log);
  LogLimit max=null;
  LogLimit notMax=null;
  if (limits != null) {
    for (    LogLimit cur : limits) {
      if (max == null)       max=cur;
 else {
        if (cur.getRootName().length() > max.getRootName().length()) {
          notMax=max;
          max=cur;
        }
      }
    }
  }
  if (max != null) {
    log.setMinimumPriority(max.getLimit());
  }
 else {
    log.setMinimumPriority(_defaultLimit);
  }
}
