{
  File f=null;
  if (pattern.indexOf('#') < 0) {
    return new File(pattern);
  }
 else {
    int max=_manager.getRotationLimit();
    if (_rotationNum == -1) {
      return getFirstFile(pattern,max);
    }
 else {
      _rotationNum++;
      if (_rotationNum > max)       _rotationNum=0;
      return new File(replace(pattern,_rotationNum));
    }
  }
}
