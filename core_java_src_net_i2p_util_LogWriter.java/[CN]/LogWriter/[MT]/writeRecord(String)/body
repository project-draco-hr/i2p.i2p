{
  if (val == null)   return;
  if (_currentOut == null)   rotateFile();
  byte b[]=val.getBytes();
  try {
    _currentOut.write(b);
    _numBytesInCurrentFile+=b.length;
  }
 catch (  Throwable t) {
    System.err.println("Error writing record, disk full?");
    t.printStackTrace();
  }
  if (_numBytesInCurrentFile >= _manager._getFileSize()) {
    rotateFile();
  }
}
