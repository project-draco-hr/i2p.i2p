{
  String sAppend;
  if (optimized) {
    if (sCPUType == null)     return null;
    if (sCPUType.equals(JBIGI_OPTIMIZATION_K6_3) && !_isWin)     sAppend="-" + JBIGI_OPTIMIZATION_K6_2;
 else     if (sCPUType.equals(JBIGI_OPTIMIZATION_VIAC32))     sAppend="-" + JBIGI_OPTIMIZATION_PENTIUM3;
 else     sAppend="-" + sCPUType;
  }
 else {
    sAppend="-none";
  }
  if (_isWin)   return "jbigi-windows" + sAppend;
  if (_isFreebsd)   return "jbigi-freebsd" + sAppend;
  if (_isMac)   return "jbigi-osx" + sAppend;
  if (_isOS2)   return "jbigi-os2" + sAppend;
  return "jbigi-linux" + sAppend;
}
