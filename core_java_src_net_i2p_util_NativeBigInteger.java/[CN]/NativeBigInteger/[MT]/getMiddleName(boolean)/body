{
  String sAppend;
  if (optimized) {
    if (sCPUType == null)     return null;
 else     sAppend="-" + sCPUType;
  }
 else   sAppend="-none";
  if (_isWin)   return "jbigi-windows" + sAppend;
  if (_isLinux)   return "jbigi-linux" + sAppend;
  if (_isFreebsd)   return "jbigi-freebsd" + sAppend;
  if (_isMac)   return "jbigi-osx" + sAppend;
  throw new RuntimeException("Dont know jbigi library name for os type '" + System.getProperty("os.name") + "'");
}
