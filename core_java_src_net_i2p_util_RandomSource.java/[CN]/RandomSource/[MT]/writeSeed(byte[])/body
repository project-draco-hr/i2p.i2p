{
  File f=new File(I2PAppContext.getGlobalContext().getConfigDir(),SEEDFILE);
  FileOutputStream fos=null;
  try {
    fos=new FileOutputStream(f);
    fos.write(buf);
  }
 catch (  IOException ioe) {
  }
 finally {
    if (fos != null)     try {
      fos.close();
    }
 catch (    IOException ioe) {
    }
  }
}
