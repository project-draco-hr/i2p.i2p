{
  try {
    for (int i=0; i < 2; i++)     test();
    for (int i=500; i < 64 * 1024; i++) {
      if (!test(i))       break;
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  try {
    Thread.sleep(10 * 1000);
  }
 catch (  InterruptedException ie) {
  }
  System.out.println("After all tests are complete...");
}
