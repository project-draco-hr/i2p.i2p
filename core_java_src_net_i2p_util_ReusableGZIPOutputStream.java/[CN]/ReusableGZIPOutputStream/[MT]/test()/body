{
  byte b[]="hi, how are you today?".getBytes();
  try {
    ReusableGZIPOutputStream o=ReusableGZIPOutputStream.acquire();
    o.write(b);
    o.finish();
    o.flush();
    byte compressed[]=o.getData();
    ReusableGZIPOutputStream.release(o);
    GZIPInputStream in=new GZIPInputStream(new ByteArrayInputStream(compressed));
    byte rv[]=new byte[128];
    int read=in.read(rv);
    if (!DataHelper.eq(rv,0,b,0,b.length))     throw new RuntimeException("foo, read=" + read);
 else     System.out.println("match, w00t");
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
