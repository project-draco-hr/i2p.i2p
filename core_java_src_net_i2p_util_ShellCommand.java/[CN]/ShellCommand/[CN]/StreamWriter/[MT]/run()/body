{
  String input;
  in=new BufferedReader(new InputStreamReader(System.in));
  try {
    while (true) {
      input=in.readLine() + "\r\n";
      bufferedWriter.write(input,0,input.length());
      bufferedWriter.flush();
    }
  }
 catch (  IOException e) {
    try {
      bufferedWriter.flush();
    }
 catch (    IOException e1) {
    }
  }
}
