{
  BufferedReader in=new BufferedReader(new InputStreamReader(System.in));
  try {
    while (true) {
      bufferedWriter.write(in.readLine());
      bufferedWriter.write("\r\n");
      bufferedWriter.flush();
    }
  }
 catch (  IOException e) {
    try {
      bufferedWriter.flush();
    }
 catch (    IOException e1) {
    }
  }
}
