{
  _rescheduleAfterRun=false;
switch (_state) {
case CANCELLED:
case IDLE:
    break;
case RUNNING:
  _cancelAfterRun=true;
return true;
case SCHEDULED:
boolean cancelled=_future.cancel(false);
if (cancelled) _state=TimedEventState.CANCELLED;
 else _log.warn("could not cancel " + this);
return cancelled;
}
return false;
}
