{
  long left, right;
  try {
    left=Long.parseLong(lop);
  }
 catch (  NumberFormatException nfe) {
    return -1;
  }
  try {
    right=Long.parseLong(rop);
  }
 catch (  NumberFormatException nfe) {
    return 1;
  }
  long diff=left - right;
  if (diff < 0)   return -1;
  if (diff > 0)   return 1;
  return 0;
}
