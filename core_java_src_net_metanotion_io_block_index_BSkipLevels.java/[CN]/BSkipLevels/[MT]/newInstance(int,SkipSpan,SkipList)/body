{
  try {
    BSkipSpan bss=(BSkipSpan)ss;
    BSkipList bsl=(BSkipList)sl;
    int page=bf.allocPage();
    BSkipLevels.init(bf,page,bss.page,levels);
    if (BlockFile.log.shouldLog(Log.DEBUG))     BlockFile.log.debug("New BSkipLevels height " + levels + " page "+ page);
    return new BSkipLevels(bf,page,bsl);
  }
 catch (  IOException ioe) {
    throw new RuntimeException("Error creating database page",ioe);
  }
}
