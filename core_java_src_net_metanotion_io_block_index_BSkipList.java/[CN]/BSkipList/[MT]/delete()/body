{
  SkipLevels curLevel=stack, nextLevel;
  while (curLevel != null) {
    nextLevel=curLevel.levels[0];
    curLevel.killInstance();
    curLevel=nextLevel;
  }
  SkipSpan curSpan=first, nextSpan;
  while (curSpan != null) {
    nextSpan=curSpan.next;
    curSpan.killInstance();
    curSpan=nextSpan;
  }
  bf.freePage(skipPage);
}
