{
  super.flush();
  if (nKeys <= 0)   this.firstKey=null;
  if (keys != null) {
    if (nKeys > 0)     this.firstKey=keys[0];
    this.keys=null;
    this.vals=null;
    if (BlockFile.log.shouldLog(Log.DEBUG))     BlockFile.log.debug("Flushed data for page " + this.page + " containing "+ this.nKeys+ '/'+ this.spanSize);
  }
 else   if (BlockFile.log.shouldLog(Log.DEBUG)) {
    BlockFile.log.debug("Flushed pointers for for unloaded page " + this.page + " containing "+ this.nKeys+ '/'+ this.spanSize);
  }
}
