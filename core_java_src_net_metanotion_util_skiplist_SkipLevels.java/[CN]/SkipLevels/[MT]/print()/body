{
  StringBuilder buf=new StringBuilder(128);
  buf.append("SL: ").append(key()).append(" :: ");
  for (int i=0; i < levels.length; i++) {
    buf.append(i);
    if (levels[i] != null) {
      buf.append("->").append(levels[i].key()).append(' ');
    }
 else {
      buf.append("->() ");
    }
  }
  buf.append('\n');
  return buf.toString();
}
