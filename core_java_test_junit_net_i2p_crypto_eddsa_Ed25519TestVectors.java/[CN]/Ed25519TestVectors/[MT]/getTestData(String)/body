{
  List<TestTuple> testCases=new ArrayList<TestTuple>();
  BufferedReader file=null;
  try {
    file=new BufferedReader(new InputStreamReader(Ed25519TestVectors.class.getResourceAsStream(fileName)));
    String line;
    while ((line=file.readLine()) != null) {
      testCases.add(new TestTuple(line));
    }
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
 finally {
    if (file != null)     try {
      file.close();
    }
 catch (    IOException e) {
    }
  }
  return testCases;
}
