{
  PrivateKey privateKey=new PrivateKey();
  byte data[]=new byte[56];
  for (int i=0; i < data.length; i++)   data[i]=(byte)(i);
  privateKey.setData(data);
  boolean error=false;
  try {
    privateKey.writeBytes(new ByteArrayOutputStream());
  }
 catch (  DataFormatException dfe) {
    error=true;
  }
  assertTrue(error);
}
