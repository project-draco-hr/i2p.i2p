{
  askQuestions();
  detectOS();
  configureAll();
  createConfigFile();
  createLoggerConfig();
  createStartScript();
  createReseedScript();
  createSeedNodes();
  copyLibraries();
  if (_isWindows) {
    showStatus("To run the router, please run startRouter.bat in " + _installDir.getAbsolutePath());
  }
 else {
    showStatus("To run the router, please run startRouter.sh in " + _installDir.getAbsolutePath());
  }
  showStatus("");
}
