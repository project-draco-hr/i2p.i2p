{
  String ii=numberTo4Digits(number);
  String param=_p.getProperty("qs." + ii + ".param"), type=_p.getProperty("qs." + ii + ".type");
  Object value=getOptionValue(answer,type);
  if (value == null) {
    return false;
  }
 else {
    if (param == null || value == null)     throw new NullPointerException();
    _answers.put(param,value);
    return true;
  }
}
