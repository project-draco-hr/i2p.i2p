{
  try {
    String cmd[]=new String[args.length - 1];
    System.arraycopy(args,1,cmd,0,cmd.length);
    Process proc=Runtime.getRuntime().exec(cmd,(String[])null,new File(args[0]));
    try {
      proc.exitValue();
    }
 catch (    Throwable t) {
    }
    Runtime.getRuntime().halt(0);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
