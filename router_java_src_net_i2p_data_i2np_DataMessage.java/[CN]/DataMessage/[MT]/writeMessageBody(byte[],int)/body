{
  if (_data == null) {
    out[curIndex++]=0x0;
    out[curIndex++]=0x0;
    out[curIndex++]=0x0;
    out[curIndex++]=0x0;
  }
 else {
    DataHelper.toLong(out,curIndex,4,_data.length);
    curIndex+=4;
    System.arraycopy(_data,0,out,curIndex,_data.length);
    curIndex+=_data.length;
  }
  return curIndex;
}
