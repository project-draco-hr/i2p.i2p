{
  byte data[]=new byte[getMessageSize()];
  int written=toByteArray(data);
  if (written != data.length) {
    _log.log(Log.CRIT,"Error writing out " + data.length + " (written: "+ written+ ", msgSize: "+ getMessageSize()+ ", writtenLen: "+ calculateWrittenLength()+ ") for "+ getClass().getName());
    return null;
  }
  return data;
}
