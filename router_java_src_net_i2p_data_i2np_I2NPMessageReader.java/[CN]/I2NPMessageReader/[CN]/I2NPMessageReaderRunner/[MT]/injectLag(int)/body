{
  if (true) {
    return 0;
  }
 else {
    boolean shouldLag=_context.random().nextInt(1000) > size;
    if (!shouldLag)     return 0;
    long readLag=getReadLag();
    if (readLag > 0) {
      long lag=_context.random().nextLong(readLag);
      if (lag > 0) {
        try {
          Thread.sleep(lag);
        }
 catch (        InterruptedException ie) {
        }
        return lag;
      }
 else {
        return 0;
      }
    }
 else {
      return 0;
    }
  }
}
