{
  _router=new Hash();
  _router.readBytes(in);
  int size=(int)DataHelper.readLong(in,2);
  _data=new byte[size];
  int read=read(in,_data);
  if (read != _data.length)   throw new DataFormatException("Incorrect # of bytes read for source route block: " + read);
  _key=new SessionKey();
  _key.readBytes(in);
  _tag=new byte[32];
  read=read(in,_tag);
  if (read != _tag.length)   throw new DataFormatException("Incorrect # of bytes read for session tag: " + read);
}
