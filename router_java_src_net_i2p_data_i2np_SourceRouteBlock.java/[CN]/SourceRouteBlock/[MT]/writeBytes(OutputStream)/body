{
  if ((_router == null) || (_data == null) || (_key == null)|| (_tag == null)|| (_tag.length != 32))   throw new DataFormatException("Insufficient data to write");
  _router.writeBytes(out);
  DataHelper.writeLong(out,2,_data.length);
  out.write(_data);
  _key.writeBytes(out);
  out.write(_tag);
}
