{
  int length=0;
  length+=1;
  if (_participantType != PARTICIPANT_TYPE_ENDPOINT) {
    length+=Hash.HASH_LENGTH;
    length+=4;
  }
  length+=4;
  length+=4;
  length+=SessionKey.KEYSIZE_BYTES;
  length+=4 * 4;
  length+=1;
  length+=SigningPublicKey.KEYSIZE_BYTES;
  if (_participantType == PARTICIPANT_TYPE_GATEWAY)   length+=SigningPrivateKey.KEYSIZE_BYTES;
  if ((_participantType == PARTICIPANT_TYPE_ENDPOINT) || (_participantType == PARTICIPANT_TYPE_GATEWAY))   length+=SessionKey.KEYSIZE_BYTES;
  _certificateCache=_certificate.toByteArray();
  length+=_certificateCache.length;
  length+=SessionTag.BYTE_LENGTH;
  length+=SessionKey.KEYSIZE_BYTES;
  length+=4;
  length+=Hash.HASH_LENGTH;
  return length;
}
