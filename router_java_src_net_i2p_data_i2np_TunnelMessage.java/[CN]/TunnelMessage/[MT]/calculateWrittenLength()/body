{
  int length=0;
  length+=4;
  length+=4;
  length+=_data.length;
  if ((_verification == null) || (_encryptedInstructions == null)) {
    length+=1;
  }
 else {
    length+=1;
    length+=Hash.HASH_LENGTH + Signature.SIGNATURE_BYTES;
    length+=2;
    length+=_encryptedInstructions.length;
  }
  return length;
}
