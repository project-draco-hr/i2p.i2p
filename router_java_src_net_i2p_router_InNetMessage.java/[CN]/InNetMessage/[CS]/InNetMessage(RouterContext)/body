{
  _context=context;
  setMessage(null);
  setFromRouter(null);
  setFromRouterHash(null);
  context.messageStateMonitor().inboundMessageAdded();
  _created=context.clock().now();
  _context.statManager().createRateStat("inNetMessage.timeToDiscard","How long until we discard an inbound msg?","InNetMessage",new long[]{5 * 60 * 1000,30 * 60 * 1000,60 * 60 * 1000});
}
