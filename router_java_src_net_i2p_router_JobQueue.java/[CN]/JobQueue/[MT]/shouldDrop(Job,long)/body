{
  if (_maxWaitingJobs <= 0)   return false;
  if (!_allowParallelOperation)   return false;
  Class cls=job.getClass();
  if (numReady > _maxWaitingJobs) {
    if (cls == HandleSourceRouteReplyMessageJob.class)     return true;
    if (cls == HandleDatabaseLookupMessageJob.class)     return true;
    if (cls == RequestTunnelJob.class)     return true;
    if (cls == HandleTunnelCreateMessageJob.class)     return true;
  }
  return false;
}
