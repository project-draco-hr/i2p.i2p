{
  FileOutputStream out=null;
  FileInputStream in=null;
  try {
    if (exists) {
      out=FileStreamFactory.getFileOutputStream(keyFile);
      structure.writeBytes(out);
      return structure;
    }
 else {
      if (keyFile.exists()) {
        in=FileStreamFactory.getFileInputStream(keyFile);
        structure.readBytes(in);
        return structure;
      }
 else {
        return null;
      }
    }
  }
 catch (  IOException ioe) {
    _log.error("Error syncing the structure to " + keyFile.getAbsolutePath(),ioe);
  }
catch (  DataFormatException dfe) {
    _log.error("Error syncing the structure with " + keyFile.getAbsolutePath(),dfe);
  }
 finally {
    if (out != null)     try {
      out.close();
    }
 catch (    IOException ioe) {
    }
    if (in != null)     try {
      in.close();
    }
 catch (    IOException ioe) {
    }
  }
  if (exists)   return structure;
 else   return null;
}
