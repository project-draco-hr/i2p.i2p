{
  LoadTestTunnelConfig ltCfg=new LoadTestTunnelConfig(cfg);
  if (wantToTest(ltCfg)) {
    long delay=_context.random().nextInt(30 * 1000) + 30 * 1000;
    SimpleTimer.getInstance().addEvent(new BeginTest(ltCfg),delay);
    if (_log.shouldLog(Log.INFO))     _log.info("Testing " + cfg + ", with "+ _active.size()+ " active");
  }
 else {
    if (_log.shouldLog(Log.INFO))     _log.info("Not testing " + cfg + " because we have "+ _active.size()+ " active: "+ _active);
  }
}
