{
  if (!isEnabled(_context))   return 0;
  int rv=CONCURRENT_PEERS;
  try {
    rv=Integer.parseInt(_context.getProperty("router.loadTestConcurrency",CONCURRENT_PEERS + ""));
  }
 catch (  NumberFormatException nfe) {
    rv=CONCURRENT_PEERS;
  }
  if (rv < 0)   rv=0;
  if (rv > 50)   rv=50;
  return rv;
}
