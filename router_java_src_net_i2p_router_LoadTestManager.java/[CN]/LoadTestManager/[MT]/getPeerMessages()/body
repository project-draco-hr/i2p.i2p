{
  String msgsPerPeer=_context.getProperty("router.loadTestMessagesPerPeer");
  int rv=CONCURRENT_MESSAGES;
  if (msgsPerPeer != null) {
    try {
      rv=Integer.parseInt(msgsPerPeer);
    }
 catch (    NumberFormatException nfe) {
      rv=CONCURRENT_MESSAGES;
    }
  }
  if (rv < 1)   rv=1;
  if (rv > 50)   rv=50;
  return rv;
}
