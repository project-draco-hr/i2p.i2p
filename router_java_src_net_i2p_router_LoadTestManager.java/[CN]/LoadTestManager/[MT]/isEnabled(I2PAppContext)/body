{
  if (FORCE_DISABLE)   return false;
  String enable=ctx.getProperty("router.enableLoadTesting");
  if ((DEFAULT_ENABLE) && (enable != null) && (!Boolean.valueOf(enable).booleanValue()))   return false;
 else   if ((!DEFAULT_ENABLE) && ((enable == null) || (!Boolean.valueOf(enable).booleanValue())))   return false;
  return true;
}
