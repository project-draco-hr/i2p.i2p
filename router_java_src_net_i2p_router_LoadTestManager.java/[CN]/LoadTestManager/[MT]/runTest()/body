{
  if ((_untestedPeers == null) || (_untestedPeers.size() <= 0)) {
    UDPTransport t=UDPTransport._instance();
    if (t != null)     _untestedPeers=t._getActivePeers();
  }
  int peers=getConcurrency();
  for (int i=0; i < peers && _untestedPeers.size() > 0; i++)   buildTestTunnel((Hash)_untestedPeers.remove(0));
}
