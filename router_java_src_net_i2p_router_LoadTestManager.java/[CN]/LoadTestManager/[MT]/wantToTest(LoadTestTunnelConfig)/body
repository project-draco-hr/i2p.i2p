{
  if (_context.router().getUptime() <= 10 * 60 * 1000)   return false;
  if (TEST_LIVE_TUNNELS && _active.size() < getConcurrency()) {
    if (cfg.getLength() < 2)     return false;
    if (cfg.getTunnel().getDestination() == null)     return false;
    _active.add(cfg);
    return true;
  }
 else {
    return false;
  }
}
