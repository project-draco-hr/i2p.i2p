{
  RouterContext ctx=new RouterContext(null);
  MessageHistory hist=new MessageHistory(ctx);
  hist.setDoLog(false);
  hist.addEntry("you smell before");
  hist.setDoLog(true);
  hist.addEntry("you smell after");
  hist.setDoLog(false);
  hist.addEntry("you smell finished");
  hist.flushEntries();
}
