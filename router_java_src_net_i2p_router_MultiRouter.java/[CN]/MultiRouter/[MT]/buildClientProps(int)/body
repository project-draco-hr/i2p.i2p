{
  Properties rProps=getRouterProps(id);
  Properties props=getClientProps();
  File f=new File(rProps.getProperty("router.clientConfigFile"));
  f.getParentFile().mkdirs();
  if (!f.exists()) {
    try {
      DataHelper.storeProps(props,f);
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
  }
  return props;
}
