{
  Properties props=new Properties();
  FileInputStream in=null;
  try {
    in=new FileInputStream(filename);
    props.load(in);
    props.setProperty("time.disabled","true");
    return props;
  }
 catch (  IOException ioe) {
    ioe.printStackTrace();
    return null;
  }
 finally {
    if (in != null)     try {
      in.close();
    }
 catch (    IOException ioe) {
    }
  }
}
