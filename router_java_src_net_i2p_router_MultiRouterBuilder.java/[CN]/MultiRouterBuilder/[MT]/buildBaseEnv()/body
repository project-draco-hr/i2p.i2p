{
  File envFile=new File("baseEnv.txt");
  try {
    FileOutputStream fos=new FileOutputStream(envFile);
    fos.write(("loggerFilenameOverride=logs/log-base-#.txt\n").getBytes());
    fos.close();
  }
 catch (  IOException ioe) {
    ioe.printStackTrace();
  }
  File f=new File("logs");
  f.mkdirs();
}
