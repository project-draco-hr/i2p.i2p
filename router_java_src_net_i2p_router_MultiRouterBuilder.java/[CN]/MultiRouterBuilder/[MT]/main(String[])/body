{
  if (args.length <= 2) {
    usage();
    return;
  }
  for (int i=0; i < args.length; i+=2) {
    String dir=args[i];
    try {
      int basePortNum=Integer.parseInt(args[i + 1]);
      buildConfig(dir,basePortNum);
    }
 catch (    NumberFormatException nfe) {
      nfe.printStackTrace();
    }
  }
  buildBaseEnv();
  buildStartupScript(args);
}
