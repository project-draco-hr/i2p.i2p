{
  long now=_context.clock().now();
  if (_log.shouldLog(Log.INFO)) {
synchronized (this) {
      locked_initTimestamps();
      while (_timestamps.containsKey(eventName)) {
        eventName=eventName + '.';
      }
      _timestamps.put(eventName,new Long(now));
      _timestampOrder.add(eventName);
    }
  }
  return now - _created;
}
