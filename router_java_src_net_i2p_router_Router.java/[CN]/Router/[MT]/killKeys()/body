{
  for (int i=0; i < _rebuildFiles.length; i++) {
    File f=new File(_rebuildFiles[i]);
    if (f.exists()) {
      boolean removed=f.delete();
      if (removed)       System.out.println("INFO:  Removing old identity file: " + _rebuildFiles[i]);
 else       System.out.println("ERROR: Could not remove old identity file: " + _rebuildFiles[i]);
    }
  }
}
