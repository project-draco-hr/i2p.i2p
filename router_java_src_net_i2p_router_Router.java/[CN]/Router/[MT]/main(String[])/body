{
  System.out.println("Starting I2P " + RouterVersion.VERSION + "-"+ RouterVersion.BUILD);
  System.out.println(RouterVersion.ID);
  installUpdates();
  verifyWrapperConfig();
  Router r=new Router();
  if ((args != null) && (args.length == 1) && ("rebuild".equals(args[0]))) {
    r.rebuildNewIdentity();
  }
 else {
    r.runRouter();
  }
}
