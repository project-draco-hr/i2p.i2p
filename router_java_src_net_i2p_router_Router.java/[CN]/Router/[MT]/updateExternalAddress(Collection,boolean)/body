{
  if ("false".equalsIgnoreCase(_context.getProperty(Router.PROP_DYNAMIC_KEYS,"false")))   return false;
  boolean ret=false;
  for (Iterator i=addrs.iterator(); i.hasNext(); ) {
    RouterAddress addr=(RouterAddress)i.next();
    if (UDPTransport.STYLE.equalsIgnoreCase(addr.getTransportStyle()))     ret=updateExternalAddress(addr,reboot);
  }
  return ret;
}
