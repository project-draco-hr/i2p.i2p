{
  String newExternal=null;
  if (!UDPTransport.STYLE.equalsIgnoreCase(addr.getTransportStyle()))   return false;
  if ("false".equalsIgnoreCase(_context.getProperty(Router.PROP_DYNAMIC_KEYS,"false")))   return false;
  if (_log.shouldLog(Log.WARN))   _log.warn("Rekeying and restarting due to " + addr.getTransportStyle() + " address update. new address: "+ addr);
  if (rebootRouter) {
    _context.router().rebuildNewIdentity();
  }
 else {
    _context.router().killKeys();
  }
  return true;
}
