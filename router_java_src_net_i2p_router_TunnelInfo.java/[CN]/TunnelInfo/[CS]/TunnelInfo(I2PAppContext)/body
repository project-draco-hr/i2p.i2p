{
  _context=context;
  if (_log == null)   _log=context.logManager().getLog(TunnelInfo.class);
  setTunnelId(null);
  setThisHop(null);
  setNextHop(null);
  setNextHopId(null);
  setNextHopInfo(null);
  _configurationKey=null;
  _verificationKey=null;
  _signingKey=null;
  _encryptionKey=null;
  setDestination(null);
  setSettings(null);
  _options=new Properties();
  _ready=false;
  _wasEverReady=false;
  _created=_context.clock().now();
  _lastTested=-1;
  _messagesProcessed=0;
  _tunnelFailures=0;
  _bytesProcessed=0;
}
