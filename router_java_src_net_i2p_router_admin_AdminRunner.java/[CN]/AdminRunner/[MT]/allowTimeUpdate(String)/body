{
  String pass=_context.getProperty("adminTimePassphrase");
  if ((pass == null) || (pass.trim().length() <= 0)) {
    if (_log.shouldLog(Log.ERROR))     _log.error("No passphrase for update time from " + _socket.getInetAddress() + ":"+ _socket.getPort());
    return false;
  }
  if (command.indexOf(pass) != -1) {
    return true;
  }
 else {
    if (_log.shouldLog(Log.ERROR))     _log.error("Invalid passphrase for update time from " + _socket.getInetAddress() + ":"+ _socket.getPort());
    return false;
  }
}
