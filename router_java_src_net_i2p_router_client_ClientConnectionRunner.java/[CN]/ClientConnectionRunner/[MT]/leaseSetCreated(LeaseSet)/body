{
  LeaseRequestState state=null;
synchronized (this) {
    state=_leaseRequest;
    if (state == null) {
      if (_log.shouldLog(Log.WARN))       _log.warn("LeaseRequest is null and we've received a new lease?! perhaps this is odd... " + ls);
      return;
    }
 else {
      state.setIsSuccessful(true);
      _currentLeaseSet=ls;
      if (_log.shouldLog(Log.DEBUG))       _log.debug("LeaseSet created fully: " + state + " / "+ ls);
      _leaseRequest=null;
      _consecutiveLeaseRequestFails=0;
    }
  }
  if ((state != null) && (state.getOnGranted() != null))   _context.jobQueue().addJob(state.getOnGranted());
}
