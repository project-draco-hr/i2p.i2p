{
  if (_leaseRequest == null) {
    _log.error("LeaseRequest is null and we've received a new lease?! WTF");
    return;
  }
 else {
    _leaseRequest.setIsSuccessful(true);
    if (_leaseRequest.getOnGranted() != null)     _context.jobQueue().addJob(_leaseRequest.getOnGranted());
    _leaseRequest=null;
    _currentLeaseSet=ls;
  }
}
