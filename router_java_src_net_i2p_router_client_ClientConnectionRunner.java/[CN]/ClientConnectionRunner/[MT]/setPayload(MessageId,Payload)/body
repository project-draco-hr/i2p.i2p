{
  long beforeLock=_context.clock().now();
  long inLock=0;
synchronized (_messages) {
    inLock=_context.clock().now();
    _messages.put(id,payload);
  }
  long afterLock=_context.clock().now();
  if (afterLock - beforeLock > 50) {
    _log.warn("setPayload.locking took too long: " + (afterLock - beforeLock) + " overall, synchronized took "+ (inLock - beforeLock));
  }
}
