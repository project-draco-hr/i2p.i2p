{
  if (_dead)   return;
  if (_context.router().isAlive() && _log.shouldLog(Log.WARN))   _log.warn("Stop the I2CP connection!  current leaseSet: " + _currentLeaseSet,new Exception("Stop client connection"));
  _dead=true;
  if (_reader != null)   _reader.stopReading();
  if (_writer != null)   _writer.stopWriting();
  if (_socket != null)   try {
    _socket.close();
  }
 catch (  IOException ioe) {
  }
synchronized (_messages) {
    _messages.clear();
  }
  if (_manager != null)   _manager.unregisterConnection(this);
  if (_currentLeaseSet != null)   _context.netDb().unpublish(_currentLeaseSet);
  _leaseRequest=null;
synchronized (_alreadyProcessed) {
    _alreadyProcessed.clear();
  }
  _config=null;
}
