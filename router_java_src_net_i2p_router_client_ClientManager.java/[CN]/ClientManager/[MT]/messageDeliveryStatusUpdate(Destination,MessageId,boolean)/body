{
  ClientConnectionRunner runner=getRunner(fromDest);
  if (runner != null) {
    if (_log.shouldLog(Log.DEBUG))     _log.debug("Delivering status [" + (delivered ? "success" : "failure") + "] to "+ fromDest.calculateHash().toBase64()+ " for message "+ id);
    runner.updateMessageDeliveryStatus(id,delivered);
  }
 else {
    if (_log.shouldLog(Log.WARN))     _log.warn("Cannot deliver status [" + (delivered ? "success" : "failure") + "] to "+ fromDest.calculateHash().toBase64()+ " for message "+ id);
  }
}
