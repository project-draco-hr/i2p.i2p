{
  if (!_ctx.getBooleanProperty(PROP_DISABLE_EXTERNAL)) {
    if (_ctx.getBooleanProperty(PROP_ENABLE_SSL))     _listener=new SSLClientListenerRunner(_ctx,this,_port);
 else     _listener=new ClientListenerRunner(_ctx,this,_port);
    Thread t=new I2PThread(_listener,"ClientListener:" + _port,true);
    t.start();
  }
  _isStarted=true;
}
