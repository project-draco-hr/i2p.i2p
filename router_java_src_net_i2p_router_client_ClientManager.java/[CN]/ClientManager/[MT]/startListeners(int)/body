{
  if (!_ctx.getBooleanProperty(PROP_DISABLE_EXTERNAL)) {
    if (_ctx.getBooleanProperty(PROP_ENABLE_SSL))     _listener=new SSLClientListenerRunner(_ctx,this,port);
 else     _listener=new ClientListenerRunner(_ctx,this,port);
    Thread t=new I2PThread(_listener,"ClientListener:" + port,true);
    t.start();
  }
  _isStarted=true;
}
