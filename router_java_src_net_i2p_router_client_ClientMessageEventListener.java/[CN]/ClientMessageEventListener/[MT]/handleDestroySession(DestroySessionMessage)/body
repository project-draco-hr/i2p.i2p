{
  SessionId id=message.getSessionId();
  SessionConfig cfg=_runner.getConfig(id);
  _runner.removeSession(id);
  int left=_runner.getSessionIds().size();
  if (left <= 0) {
    _runner.stopRunning();
  }
 else {
    if (_log.shouldLog(Log.INFO))     _log.info("Still " + left + " sessions left");
  }
}
