{
  if (!isValid(clove)) {
    if (_log.shouldLog(Log.DEBUG))     _log.warn("Invalid clove " + clove);
    return;
  }
  boolean requestAck=(clove.getSourceRouteBlockAction() == GarlicClove.ACTION_STATUS);
  long sendExpiration=clove.getExpiration().getTime();
  MessageHandler.getInstance().handleMessage(clove.getInstructions(),clove.getData(),requestAck,clove.getSourceRouteBlock(),clove.getCloveId(),_from,_fromHash,sendExpiration,FORWARD_PRIORITY);
}
