{
  if (!isValid(clove)) {
    if (_log.shouldLog(Log.DEBUG))     _log.warn("Invalid clove " + clove);
    return;
  }
  long sendExpiration=clove.getExpiration().getTime();
  boolean sendDirect=false;
  _handler.handleMessage(clove.getInstructions(),clove.getData(),clove.getCloveId(),_from,_fromHash,sendExpiration,FORWARD_PRIORITY,sendDirect);
}
