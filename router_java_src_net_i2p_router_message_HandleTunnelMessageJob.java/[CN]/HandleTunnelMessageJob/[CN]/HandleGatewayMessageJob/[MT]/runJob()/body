{
  RouterContext ctx=HandleTunnelMessageJob.this.getContext();
  if (_body != null) {
    ctx.statManager().addRateData("tunnel.gatewayMessageSize",_length,0);
    if (_log.shouldLog(Log.INFO))     _log.info("Message for tunnel " + _info.getTunnelId() + " received at the gateway (us), and since its > 0 length, forward the "+ _body.getClass().getName()+ " message on to "+ _info.getNextHop().toBase64()+ " via SendTunnelMessageJob");
    ctx.jobQueue().addJob(new SendTunnelMessageJob(ctx,_body,_info.getTunnelId(),null,null,null,null,FORWARD_TIMEOUT,FORWARD_PRIORITY));
  }
 else {
    if (_log.shouldLog(Log.WARN))     _log.warn("Body of the message for the tunnel could not be parsed");
    if (_log.shouldLog(Log.DEBUG))     _log.debug("Message that failed: " + _message);
  }
}
