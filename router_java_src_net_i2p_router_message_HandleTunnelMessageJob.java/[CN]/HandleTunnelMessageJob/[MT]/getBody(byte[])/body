{
  try {
    return _handler.readMessage(new ByteArrayInputStream(body));
  }
 catch (  I2NPMessageException ime) {
    if (_log.shouldLog(Log.ERROR))     _log.error("Error parsing the message body",ime);
  }
catch (  IOException ioe) {
    if (_log.shouldLog(Log.ERROR))     _log.error("Error reading the message body",ioe);
  }
  return null;
}
