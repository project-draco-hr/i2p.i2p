{
  if (body instanceof DataMessage) {
    boolean isLocal=getContext().clientManager().isLocal(dest);
    if (isLocal) {
      deliverMessage(null,dest,(DataMessage)body);
      return;
    }
 else {
      if (_log.shouldLog(Log.ERROR))       _log.error("Delivery to remote destinations is not yet supported",getAddedBy());
      return;
    }
  }
 else {
    if (_log.shouldLog(Log.ERROR))     _log.error("Deliver something other than a DataMessage to a Destination?  I don't think so.");
    return;
  }
}
