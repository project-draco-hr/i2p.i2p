{
  if (_log.shouldLog(Log.DEBUG))   _log.debug("Send outbound client message job beginning");
  buildClove();
  if (_log.shouldLog(Log.DEBUG))   _log.debug("Clove built");
  Hash to=_status.getTo().calculateHash();
  long timeoutMs=_overallExpiration - _context.clock().now();
  if (_log.shouldLog(Log.DEBUG))   _log.debug("Send outbound client message - sending off leaseSet lookup job");
  _status.incrementLookups();
  _context.netDb().lookupLeaseSet(to,_nextStep,_lookupLeaseSetFailed,timeoutMs);
}
