{
  GarlicConfig config=createGarlicConfig(ctx,replyToken,expiration,recipientPK,dataClove,from,dest,replyTunnel,requireAck,bundledReplyLeaseSet);
  if (config == null)   return null;
  GarlicMessage msg=GarlicMessageBuilder.buildMessage(ctx,config,wrappedKey,wrappedTags,ctx.clientManager().getClientSessionKeyManager(from));
  return msg;
}
