{
  if (inMsg.getType() == DeliveryStatusMessage.MESSAGE_TYPE) {
    if (_log.shouldLog(Log.INFO))     _log.info(OutboundClientMessageOneShotJob.this.getJobId() + "delivery status message received: " + inMsg+ " our token: "+ _pendingToken);
    return _pendingToken == ((DeliveryStatusMessage)inMsg).getMessageId();
  }
 else {
    return false;
  }
}
