{
  if (_inTunnel != null) {
    _leaseSetCache.remove(_hashPair);
  }
  if (_lease != null) {
    _leaseCache.remove(_hashPair,_lease);
  }
  if (_outTunnel != null) {
synchronized (_tunnelCache) {
      TunnelInfo t=_backloggedTunnelCache.get(_hashPair);
      if (t != null && t.equals(_outTunnel))       _backloggedTunnelCache.remove(_hashPair);
      t=_tunnelCache.get(_hashPair);
      if (t != null && t.equals(_outTunnel))       _tunnelCache.remove(_hashPair);
    }
  }
}
