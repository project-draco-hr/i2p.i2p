{
  if (_log.shouldLog(Log.DEBUG))   _log.debug(getJobId() + ": Send outbound client message job beginning");
  buildClove();
  if (_log.shouldLog(Log.DEBUG))   _log.debug(getJobId() + ": Clove built");
  long timeoutMs=_overallExpiration - getContext().clock().now();
  if (_log.shouldLog(Log.DEBUG))   _log.debug(getJobId() + ": Send outbound client message - sending off leaseSet lookup job");
  getContext().netDb().lookupLeaseSet(_to.calculateHash(),new SendJob(getContext()),new LookupLeaseSetFailedJob(getContext()),timeoutMs);
}
