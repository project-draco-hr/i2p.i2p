{
  _state=46;
  if (_selector != null)   createFakeOutNetMessage();
  if (_onSend != null) {
    if (_log.shouldLog(Log.DEBUG))     _log.debug("Firing onSend as we're honoring the instructions");
    getContext().jobQueue().addJob(_onSend);
    _state=47;
  }
  RouterIdentity ident=getContext().router().getRouterInfo().getIdentity();
  if (_destRouter != null) {
    _state=48;
    honorSendRemote(info,ident);
    _state=49;
  }
 else {
    _state=50;
    honorSendLocal(info,ident);
    _state=51;
  }
}
