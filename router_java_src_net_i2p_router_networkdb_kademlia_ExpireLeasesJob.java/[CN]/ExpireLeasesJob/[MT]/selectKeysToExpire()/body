{
  Set keys=_facade.getDataStore().getKeys();
  Set toExpire=new HashSet(128);
  for (Iterator iter=keys.iterator(); iter.hasNext(); ) {
    Hash key=(Hash)iter.next();
    Object obj=_facade.getDataStore().get(key);
    if (obj instanceof LeaseSet) {
      LeaseSet ls=(LeaseSet)obj;
      if (!ls.isCurrent(Router.CLOCK_FUDGE_FACTOR))       toExpire.add(key);
 else       _log.debug("Lease " + ls.getDestination().calculateHash() + " is current, no need to expire");
    }
  }
  return toExpire;
}
