{
  if (getContext().netDb().lookupLocally(_search.getKey()) != null) {
    _search.success();
  }
 else {
    int remaining=_search.getLookupsRemaining();
    if (_log.shouldLog(Log.INFO))     _log.info(getJobId() + "/" + _search.getJobId()+ ": got a reply looking for "+ _search.getKey().toBase64()+ ", with "+ remaining+ " outstanding searches");
    if (remaining <= 0)     _search.failed();
  }
}
