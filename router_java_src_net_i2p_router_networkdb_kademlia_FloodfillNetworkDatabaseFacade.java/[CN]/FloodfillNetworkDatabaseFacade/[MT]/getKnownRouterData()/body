{
  List<RouterInfo> rv=new ArrayList();
  DataStore ds=getDataStore();
  if (ds != null) {
    Set keys=ds.getKeys();
    if (keys != null) {
      for (Iterator iter=keys.iterator(); iter.hasNext(); ) {
        Object o=ds.get((Hash)iter.next());
        if (o instanceof RouterInfo)         rv.add((RouterInfo)o);
      }
    }
  }
  return rv;
}
