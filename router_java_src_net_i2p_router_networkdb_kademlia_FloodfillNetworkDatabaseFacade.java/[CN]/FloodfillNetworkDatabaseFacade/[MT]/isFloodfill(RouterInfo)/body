{
  if (peer == null)   return false;
  if (_alwaysQuery != null) {
    Hash aq=new Hash();
    try {
      aq.fromBase64(_alwaysQuery);
      if (aq.equals(peer.getIdentity().getHash()))       return true;
    }
 catch (    DataFormatException dfe) {
    }
  }
  String caps=peer.getCapabilities();
  if ((caps != null) && (caps.indexOf(FloodfillNetworkDatabaseFacade.CAPACITY_FLOODFILL) != -1))   return true;
 else   return false;
}
