{
  if ((_toIgnore != null) && (_toIgnore.contains(entry)))   return;
  if (entry.equals(_context.routerHash()))   return;
  if (_context.shitlist().isShitlisted(entry))   return;
  RouterInfo info=_context.netDb().lookupRouterInfoLocally(entry);
  if (info == null)   return;
  if (FloodfillNetworkDatabaseFacade.isFloodfill(info)) {
    _floodfillMatches.add(entry);
  }
 else {
    if ((_wanted > _matches) && (_key != null)) {
      BigInteger diff=getDistance(_key,entry);
      _sorted.put(diff,entry);
    }
  }
  _matches++;
}
