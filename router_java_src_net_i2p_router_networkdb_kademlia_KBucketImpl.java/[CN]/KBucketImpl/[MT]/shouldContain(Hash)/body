{
  byte distance[]=distanceFromLocal(key);
  boolean tooLarge=distanceIsTooLarge(distance);
  if (tooLarge) {
    if (false && _log.shouldLog(Log.DEBUG))     _log.debug("too large [" + _begin + "-->"+ _end+ "] "+ "\nLow:  "+ BigInteger.ZERO.setBit(_begin).toString(16)+ "\nCur:  "+ DataHelper.toHexString(distance)+ "\nHigh: "+ BigInteger.ZERO.setBit(_end).toString(16));
    return false;
  }
  boolean tooSmall=distanceIsTooSmall(distance);
  if (tooSmall) {
    if (_log.shouldLog(Log.DEBUG))     _log.debug("too small [" + _begin + "-->"+ _end+ "] distance: "+ DataHelper.toHexString(distance));
    return false;
  }
  return true;
}
