{
  for (int i=0; i < NUM_BUCKETS; i++) {
    if (_buckets[i].shouldContain(key))     return i;
  }
  _log.error("Key does not fit in any bucket?! WTF!\nKey  : [" + DataHelper.toHexString(key.getData()) + "]"+ "\nUs   : ["+ toString(_us.getData())+ "]"+ "\nDelta: ["+ DataHelper.toHexString(DataHelper.xor(_us.getData(),key.getData()))+ "]",new Exception("WTF"));
  displayBuckets();
  return -1;
}
