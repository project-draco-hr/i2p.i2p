{
  if (!_initialized)   return null;
  Set rv=new HashSet(hashes.size());
  for (Iterator iter=hashes.iterator(); iter.hasNext(); ) {
    Hash rhash=(Hash)iter.next();
    DataStructure ds=_ds.get(rhash);
    if (ds == null) {
      if (_log.shouldLog(Log.INFO))       _log.info("Selected hash " + rhash.toBase64() + " is not stored locally");
    }
 else     if (!(ds instanceof RouterInfo)) {
      if (_log.shouldLog(Log.WARN))       _log.warn("Selected router hash " + rhash.toBase64() + " is NOT a routerInfo!");
    }
 else {
      rv.add(ds);
    }
  }
  return rv;
}
