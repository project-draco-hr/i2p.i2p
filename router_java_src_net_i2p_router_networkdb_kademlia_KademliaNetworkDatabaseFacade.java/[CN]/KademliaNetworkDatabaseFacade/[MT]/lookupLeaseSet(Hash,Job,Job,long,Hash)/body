{
  if (!_initialized)   return;
  LeaseSet ls=lookupLeaseSetLocally(key);
  if (ls != null) {
    if (_log.shouldLog(Log.DEBUG))     _log.debug("leaseSet found locally, firing " + onFindJob);
    if (onFindJob != null)     _context.jobQueue().addJob(onFindJob);
  }
 else {
    if (_log.shouldLog(Log.DEBUG))     _log.debug("leaseSet not found locally, running search");
    search(key,onFindJob,onFailedLookupJob,timeoutMs,true,fromLocalDest);
  }
  if (_log.shouldLog(Log.DEBUG))   _log.debug("after lookupLeaseSet");
}
