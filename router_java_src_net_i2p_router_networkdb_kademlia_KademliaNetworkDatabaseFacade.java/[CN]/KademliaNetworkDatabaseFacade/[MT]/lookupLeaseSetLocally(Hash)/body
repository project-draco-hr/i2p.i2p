{
  if (!_initialized)   return null;
  if (_ds.isKnown(key)) {
    DataStructure ds=_ds.get(key);
    if (ds instanceof LeaseSet) {
      LeaseSet ls=(LeaseSet)ds;
      if (ls.isCurrent(Router.CLOCK_FUDGE_FACTOR)) {
        return ls;
      }
 else {
        fail(key);
synchronized (_exploreKeys) {
          _exploreKeys.add(key);
        }
        return null;
      }
    }
 else {
      return null;
    }
  }
 else {
    return null;
  }
}
