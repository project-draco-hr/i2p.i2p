{
  if (!_initialized)   return null;
  DataStructure ds=_ds.get(key);
  if (ds != null) {
    if (ds instanceof RouterInfo) {
      boolean valid=true;
      try {
        valid=(null == validate(key,(RouterInfo)ds));
      }
 catch (      IllegalArgumentException iae) {
        valid=false;
      }
      if (!valid) {
        fail(key);
        return null;
      }
      return (RouterInfo)ds;
    }
 else {
      return null;
    }
  }
 else {
    return null;
  }
}
