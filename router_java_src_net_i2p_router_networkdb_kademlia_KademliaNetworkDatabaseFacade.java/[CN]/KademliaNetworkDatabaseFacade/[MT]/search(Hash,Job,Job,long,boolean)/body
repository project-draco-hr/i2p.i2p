{
  if (!_initialized)   return;
  if (isShitlisted(key)) {
    if (_log.shouldLog(Log.WARN))     _log.warn("Not searching for a shitlisted key [" + key.toBase64() + "]");
    if (onFailedLookupJob != null)     _context.jobQueue().addJob(onFailedLookupJob);
    return;
  }
  _context.jobQueue().addJob(new SearchJob(_context,this,key,onFindJob,onFailedLookupJob,timeoutMs,true,isLease));
}
