{
  if (_context.profileOrganizer().isFailing(entry))   return;
  if (_toIgnore.contains(entry))   return;
  RouterInfo info=_context.netDb().lookupRouterInfoLocally(entry);
  if (info == null)   return;
  if (info.getIdentity().isHidden())   return;
  BigInteger diff=getDistance(_key,entry);
  _sorted.put(diff,entry);
  _matches++;
}
