{
  DataStructure data=get(_key);
  if (data == null)   return true;
  if (data instanceof LeaseSet) {
    long knownDate=((LeaseSet)data).getEarliestLeaseDate();
    long fileDate=_leaseFile.lastModified();
    if (fileDate > knownDate)     return true;
 else     return false;
  }
 else {
    return true;
  }
}
