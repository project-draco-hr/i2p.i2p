{
  boolean exists=false;
  int pending=0;
synchronized (_keys) {
    pending=_keys.size();
    exists=(null != _keys.put(key,data));
    if (!exists)     _keyOrder.add(key);
    _keys.notifyAll();
  }
  if (exists)   _context.statManager().addRateData("netDb.writeClobber",pending,0);
  _context.statManager().addRateData("netDb.writePending",pending,0);
}
