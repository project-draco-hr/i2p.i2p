{
  String riName=getRouterInfoName(key);
  File f=new File(dir,riName);
  if (f.exists()) {
    boolean removed=f.delete();
    if (!removed)     _log.warn("Unable to remove router info at " + f.getAbsolutePath());
 else     _log.info("Removed router info at " + f.getAbsolutePath());
    return;
  }
}
