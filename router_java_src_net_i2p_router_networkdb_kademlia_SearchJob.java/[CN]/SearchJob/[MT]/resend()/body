{
  DataStructure ds=_facade.lookupLeaseSetLocally(_state.getTarget());
  if (ds == null)   ds=_facade.lookupRouterInfoLocally(_state.getTarget());
  if (ds != null)   _context.jobQueue().addJob(new StoreJob(_context,_facade,_state.getTarget(),ds,null,null,RESEND_TIMEOUT,_state.getSuccessful()));
}
