{
  if (router.getIdentity().equals(Router.getInstance().getRouterInfo().getIdentity())) {
    if (_log.shouldLog(Log.ERROR))     _log.error(getJobId() + ": Dont send search to ourselves - why did we try?");
    return;
  }
 else {
    if (_log.shouldLog(Log.DEBUG))     _log.debug(getJobId() + ": Send search to " + router);
  }
  if (_isLease || false)   sendLeaseSearch(router);
 else   sendRouterSearch(router);
}
