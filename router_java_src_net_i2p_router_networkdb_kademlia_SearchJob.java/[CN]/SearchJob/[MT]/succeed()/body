{
  if (_log.shouldLog(Log.INFO))   _log.info(getJobId() + ": Succeeded search for key " + _state.getTarget());
  if (_log.shouldLog(Log.DEBUG))   _log.debug(getJobId() + ": State of successful search: " + _state);
  if (_keepStats) {
    long time=_context.clock().now() - _state.getWhenStarted();
    _context.statManager().addRateData("netDb.successTime",time,0);
    _context.statManager().addRateData("netDb.successPeers",_state.getAttempted().size(),time);
  }
  if (_onSuccess != null)   _context.jobQueue().addJob(_onSuccess);
}
