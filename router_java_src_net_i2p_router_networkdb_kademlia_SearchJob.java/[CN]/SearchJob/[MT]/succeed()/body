{
  if (_log.shouldLog(Log.INFO))   _log.info(getJobId() + ": Succeeded search for key " + _state.getTarget());
  if (_log.shouldLog(Log.DEBUG))   _log.debug(getJobId() + ": State of successful search: " + _state);
  if (_keepStats) {
    long time=Clock.getInstance().now() - _state.getWhenStarted();
    StatManager.getInstance().addRateData("netDb.successTime",time,0);
    StatManager.getInstance().addRateData("netDb.successPeers",_state.getAttempted().size(),time);
  }
  if (_onSuccess != null)   JobQueue.getInstance().addJob(_onSuccess);
}
