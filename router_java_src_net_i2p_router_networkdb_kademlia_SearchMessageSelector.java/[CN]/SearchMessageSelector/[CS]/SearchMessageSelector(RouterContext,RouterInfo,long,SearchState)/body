{
  _context=context;
  _log=context.logManager().getLog(SearchMessageSelector.class);
  _peer=peer.getIdentity().getHash();
  _found=false;
  _exp=expiration;
  _state=state;
  _id=__searchSelectorId.incrementAndGet();
  if (_log.shouldLog(Log.DEBUG))   _log.debug("[" + _id + "] Created: "+ toString());
}
