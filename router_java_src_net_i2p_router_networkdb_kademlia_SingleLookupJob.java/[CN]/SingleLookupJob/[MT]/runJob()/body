{
  Hash from=_dsrm.getFromHash();
  for (int i=0; i < _dsrm.getNumReplies(); i++) {
    Hash peer=_dsrm.getReply(i);
    if (peer.equals(getContext().routerHash()))     continue;
    if (getContext().netDb().lookupRouterInfoLocally(peer) == null)     getContext().jobQueue().addJob(new SingleSearchJob(getContext(),peer,from));
 else     if (!getContext().commSystem().isEstablished(peer))     getContext().jobQueue().addJob(new SingleSearchJob(getContext(),peer,peer));
  }
}
