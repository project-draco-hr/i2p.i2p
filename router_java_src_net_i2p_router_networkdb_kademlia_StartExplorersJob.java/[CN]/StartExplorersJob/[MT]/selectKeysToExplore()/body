{
  Set queued=_facade.getExploreKeys();
  if (_log.shouldLog(Log.DEBUG))   _log.debug("Keys waiting for exploration: " + queued.size());
  if (queued.size() <= MAX_PER_RUN)   return queued;
  Set rv=new HashSet(MAX_PER_RUN);
  for (Iterator iter=queued.iterator(); iter.hasNext(); ) {
    if (rv.size() >= MAX_PER_RUN)     break;
    rv.add(iter.next());
  }
  return rv;
}
