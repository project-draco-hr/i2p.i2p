{
  long howLong=_state.confirmed(_peer.getIdentity().getHash());
  if (_log.shouldLog(Log.INFO))   _log.info(StoreJob.this.getJobId() + ": Marking store of " + _state.getTarget()+ " to "+ _peer.getIdentity().getHash().toBase64()+ " successful after "+ howLong);
  getContext().profileManager().dbStoreSent(_peer.getIdentity().getHash(),howLong);
  getContext().statManager().addRateData("netDb.ackTime",howLong,howLong);
  if ((_sendThrough != null) && (_msgSize > 0)) {
    if (_log.shouldLog(Log.WARN))     _log.warn("sent a " + _msgSize + "byte netDb message through tunnel "+ _sendThrough+ " after "+ howLong);
    for (int i=0; i < _sendThrough.getLength(); i++)     getContext().profileManager().tunnelDataPushed(_sendThrough.getPeer(i),howLong,_msgSize);
  }
  if (_state.getCompleteCount() >= getRedundancy()) {
    succeed();
  }
 else {
    sendNext();
  }
}
