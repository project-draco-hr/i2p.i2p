{
  long rv=-1;
synchronized (_pendingPeers) {
    _pendingPeers.remove(peer);
    Long when=(Long)_pendingPeerTimes.remove(peer);
    if (when != null)     rv=_context.clock().now() - when.longValue();
  }
synchronized (_successfulPeers) {
    _successfulPeers.add(peer);
  }
  _completeCount++;
  return rv;
}
