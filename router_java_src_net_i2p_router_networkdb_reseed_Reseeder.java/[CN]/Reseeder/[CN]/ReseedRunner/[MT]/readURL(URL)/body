{
  ByteArrayOutputStream baos=new ByteArrayOutputStream(4 * 1024);
  EepGet get=new EepGet(I2PAppContext.getGlobalContext(),false,null,-1,0,0,MAX_RESEED_RESPONSE_SIZE,null,baos,url.toString(),false,null,null);
  get.addStatusListener(ReseedRunner.this);
  if (get.fetch())   return baos.toByteArray();
 else   return null;
}
