{
  List URLList=new ArrayList();
  String URLs=_context.getProperty("i2p.reseedURL",DEFAULT_SEED_URL);
  StringTokenizer tok=new StringTokenizer(URLs," ,");
  while (tok.hasMoreTokens())   URLList.add(tok.nextToken().trim());
  Collections.shuffle(URLList);
  for (int i=0; i < URLList.size() && _isRunning; i++)   reseedOne((String)URLList.get(i),echoStatus);
}
