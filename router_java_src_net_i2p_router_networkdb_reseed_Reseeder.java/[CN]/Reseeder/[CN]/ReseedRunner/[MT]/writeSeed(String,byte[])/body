{
  String dirName="netDb";
  File netDbDir=new SecureDirectory(_context.getRouterDir(),dirName);
  if (!netDbDir.exists()) {
    boolean ok=netDbDir.mkdirs();
  }
  FileOutputStream fos=new SecureFileOutputStream(new File(netDbDir,"routerInfo-" + name + ".dat"));
  fos.write(data);
  fos.close();
}
