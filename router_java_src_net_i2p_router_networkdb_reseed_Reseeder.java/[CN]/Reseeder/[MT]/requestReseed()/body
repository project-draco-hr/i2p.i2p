{
synchronized (Reseeder.class) {
    if (_reseedRunner == null)     _reseedRunner=new ReseedRunner();
    if (_reseedRunner.isRunning()) {
      return;
    }
 else {
      System.setProperty(PROP_INPROGRESS,"true");
      I2PThread reseed=new I2PThread(_reseedRunner,"Reseed");
      reseed.start();
    }
  }
}
