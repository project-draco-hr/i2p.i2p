{
  try {
    getContext().profileOrganizer().reorganize(true);
  }
 catch (  Throwable t) {
    _log.log(Log.CRIT,"Error evaluating profiles",t);
  }
 finally {
    requeue(30 * 1000);
    if (_log.shouldLog(Log.DEBUG))     _log.debug("Requeued for " + new Date(getTiming().getStartAfter()));
  }
}
