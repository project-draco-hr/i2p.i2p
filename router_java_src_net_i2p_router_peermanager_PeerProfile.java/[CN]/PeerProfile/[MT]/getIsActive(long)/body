{
  if ((getSendSuccessSize().getRate(period).getCurrentEventCount() > 0) || (getSendSuccessSize().getRate(period).getLastEventCount() > 0) || (getReceiveSize().getRate(period).getCurrentEventCount() > 0)|| (getReceiveSize().getRate(period).getLastEventCount() > 0))   return true;
 else   return false;
}
