{
  RouterContext ctx=new RouterContext(new net.i2p.router.Router());
  ProfilePersistenceHelper helper=new ProfilePersistenceHelper(ctx);
  try {
    Thread.sleep(5 * 1000);
  }
 catch (  InterruptedException e) {
  }
  StringBuffer buf=new StringBuffer(1024);
  for (int i=0; i < args.length; i++) {
    PeerProfile profile=helper.readProfile(new File(args[i]));
    if (profile == null) {
      buf.append("Could not load profile ").append(args[i]).append('\n');
      continue;
    }
    buf.append("Peer " + profile.getPeer().toBase64() + ":\t Speed:\t"+ profile.calculateSpeed()+ " Reliability:\t"+ profile.calculateReliability()+ " Integration:\t"+ profile.calculateIntegration()+ " Active?\t"+ profile.getIsActive()+ " Failing?\t"+ profile.calculateIsFailing()+ '\n');
  }
  try {
    Thread.sleep(5 * 1000);
  }
 catch (  InterruptedException e) {
  }
  System.out.println(buf.toString());
}
