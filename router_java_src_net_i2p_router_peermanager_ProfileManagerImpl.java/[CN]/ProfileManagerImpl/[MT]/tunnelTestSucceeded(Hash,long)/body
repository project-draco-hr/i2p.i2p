{
  PeerProfile data=getProfile(peer);
  if (data == null)   return;
  data.updateTunnelTestTimeAverage(responseTimeMs);
  data.getTunnelTestResponseTime().addData(responseTimeMs,responseTimeMs);
  if (responseTimeMs > getSlowThreshold())   data.getTunnelTestResponseTimeSlow().addData(responseTimeMs,responseTimeMs);
}
