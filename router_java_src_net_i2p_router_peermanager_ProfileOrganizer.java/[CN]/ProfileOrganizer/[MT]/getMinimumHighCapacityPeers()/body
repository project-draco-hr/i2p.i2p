{
  String val=_context.getProperty(PROP_MINIMUM_HIGH_CAPACITY_PEERS,"" + DEFAULT_MINIMUM_HIGH_CAPACITY_PEERS);
  if (val != null) {
    try {
      int rv=Integer.parseInt(val);
      if (_log.shouldLog(Log.DEBUG))       _log.debug("router context said " + PROP_MINIMUM_HIGH_CAPACITY_PEERS + '='+ val);
      return rv;
    }
 catch (    NumberFormatException nfe) {
      if (_log.shouldLog(Log.WARN))       _log.warn("Minimum high capacity peers improperly set in the router environment [" + val + "]",nfe);
    }
  }
  if (_log.shouldLog(Log.DEBUG))   _log.debug("no config for " + PROP_MINIMUM_HIGH_CAPACITY_PEERS + ", using "+ DEFAULT_MINIMUM_HIGH_CAPACITY_PEERS);
  return DEFAULT_MINIMUM_HIGH_CAPACITY_PEERS;
}
