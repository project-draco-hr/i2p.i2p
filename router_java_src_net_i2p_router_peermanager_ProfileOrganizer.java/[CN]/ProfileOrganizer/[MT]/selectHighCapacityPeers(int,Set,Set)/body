{
synchronized (_reorganizeLock) {
    if (exclude == null)     exclude=new HashSet(_fastPeers.keySet());
 else     exclude.addAll(_fastPeers.keySet());
    locked_selectPeers(_highCapacityPeers,howMany,exclude,matches);
  }
  if (matches.size() < howMany) {
    if (_log.shouldLog(Log.INFO))     _log.info("selectHighCap(" + howMany + "), not enough fast ("+ matches.size()+ ") going on to notFailing");
    selectNotFailingPeers(howMany,exclude,matches);
  }
 else {
    if (_log.shouldLog(Log.INFO))     _log.info("selectHighCap(" + howMany + "), found enough highCap ("+ matches.size()+ ")");
  }
  return;
}
