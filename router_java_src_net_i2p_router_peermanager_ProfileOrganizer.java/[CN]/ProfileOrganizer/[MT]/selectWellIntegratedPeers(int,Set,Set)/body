{
synchronized (_reorganizeLock) {
    locked_selectPeers(_wellIntegratedPeers,howMany,exclude,matches);
  }
  if (matches.size() < howMany) {
    if (_log.shouldLog(Log.DEBUG))     _log.debug("selectWellIntegrated(" + howMany + "), not enough integrated ("+ matches.size()+ ") going on to notFailing");
    selectNotFailingPeers(howMany,exclude,matches);
  }
 else {
    if (_log.shouldLog(Log.DEBUG))     _log.debug("selectWellIntegrated(" + howMany + "), found enough well integrated ("+ matches.size()+ ")");
  }
  return;
}
