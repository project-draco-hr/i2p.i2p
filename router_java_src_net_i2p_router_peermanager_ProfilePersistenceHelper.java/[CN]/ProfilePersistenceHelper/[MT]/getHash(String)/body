{
  String key=name.substring("profile-".length());
  key=key.substring(0,key.length() - ".dat".length());
  try {
    byte[] b=Base64.decode(key);
    if (b == null)     return null;
    Hash h=Hash.create(b);
    return h;
  }
 catch (  Exception dfe) {
    _log.warn("Invalid base64 [" + key + "]",dfe);
    return null;
  }
}
