{
  loadRouterInfo();
  if (_us == null) {
    RebuildRouterInfoJob.rebuildRouterInfo(false);
    JobQueue.getInstance().addJob(this);
    return;
  }
 else {
    Router.getInstance().setRouterInfo(_us);
    MessageHistory.initialize();
    JobQueue.getInstance().addJob(new BootCommSystemJob());
  }
}
