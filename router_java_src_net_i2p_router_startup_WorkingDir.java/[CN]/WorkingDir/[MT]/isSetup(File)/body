{
  if (dir.isDirectory()) {
    String[] files=dir.list();
    if (files == null)     return false;
    String migrated[]=MIGRATE_BASE.split(",");
    for (    String file : files) {
      for (int i=0; i < migrated.length; i++) {
        if (file.equals(migrated[i]))         return true;
      }
    }
  }
  return false;
}
