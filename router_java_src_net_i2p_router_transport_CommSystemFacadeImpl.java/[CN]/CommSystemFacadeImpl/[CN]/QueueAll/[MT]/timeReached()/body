{
  for (  Hash h : _context.netDb().getAllRouters()) {
    RouterInfo ri=_context.netDb().lookupRouterInfoLocally(h);
    if (ri == null)     continue;
    byte[] ip=getIP(ri);
    if (ip == null)     continue;
    _geoIP.add(ip);
  }
  _context.simpleScheduler().addPeriodicEvent(new Lookup(),5000,LOOKUP_TIME);
}
