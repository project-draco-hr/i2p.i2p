{
  Map addresses=null;
  if (_manager != null)   addresses=_manager.getAddresses();
 else   addresses=new HashMap(1);
  if (!addresses.containsKey(TCPTransport.STYLE)) {
    RouterAddress addr=createTCPAddress();
    if (addr != null)     addresses.put(TCPTransport.STYLE,addr);
  }
  if (_log.shouldLog(Log.INFO))   _log.info("Creating addresses: " + addresses);
  return new HashSet(addresses.values());
}
