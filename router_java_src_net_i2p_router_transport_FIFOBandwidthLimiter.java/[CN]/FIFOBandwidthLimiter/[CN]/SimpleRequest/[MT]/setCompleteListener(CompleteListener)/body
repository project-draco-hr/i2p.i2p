{
  boolean complete=false;
synchronized (SimpleRequest.this) {
    _lsnr=lsnr;
    if (isComplete()) {
      complete=true;
    }
  }
  if (complete && lsnr != null) {
    if (_log.shouldLog(Log.INFO))     _log.info("complete listener set AND completed: " + lsnr);
    lsnr.complete(SimpleRequest.this);
  }
}
