{
  if (addr[0] == (int)127)   return false;
  if (addr[0] == (int)10)   return false;
  if ((addr[0] == (int)172) && (addr[1] >= (int)16) && (addr[1] <= (int)31))   return false;
  if ((addr[0] == (int)192) && (addr[1] == (int)168))   return false;
  if (addr[0] >= (int)224)   return false;
  return true;
}
