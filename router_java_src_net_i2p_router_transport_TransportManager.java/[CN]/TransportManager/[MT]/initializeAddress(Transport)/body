{
  String ips=Addresses.getAnyAddress();
  if (ips == null)   return;
  InetAddress ia=null;
  try {
    ia=InetAddress.getByName(ips);
  }
 catch (  UnknownHostException e) {
    return;
  }
  if (ia == null)   return;
  byte[] ip=ia.getAddress();
  t.externalAddressReceived(Transport.SOURCE_INTERFACE,ip,0);
}
