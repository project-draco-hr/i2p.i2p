{
  if (_log.shouldLog(Log.DEBUG))   _log.debug("I2NPMessage received: " + message.getClass().getName(),new Exception("Where did I come from again?"));
  int num=_context.inNetMessagePool().add(message,fromRouter,fromRouterHash);
  if (_log.shouldLog(Log.DEBUG))   _log.debug("Added to in pool: " + num);
}
