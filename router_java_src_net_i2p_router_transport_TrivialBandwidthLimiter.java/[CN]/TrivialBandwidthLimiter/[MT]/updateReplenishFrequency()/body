{
  String freqMs=_context.getProperty(PROP_REPLENISH_FREQUENCY);
  if ((freqMs != null) && (freqMs.trim().length() > 0) && (!(freqMs.equals(String.valueOf(_replenishFrequency))))) {
    try {
      long ms=Long.parseLong(freqMs);
      if (ms >= 0) {
        _replenishFrequency=ms;
      }
    }
 catch (    NumberFormatException nfe) {
      if (_log.shouldLog(Log.WARN))       _log.warn("Invalid replenish frequency [" + freqMs + "], keeping as "+ _replenishFrequency);
    }
  }
 else {
    if (_log.shouldLog(Log.DEBUG))     _log.debug("Replenish frequency not specified in the config via " + PROP_REPLENISH_FREQUENCY);
    _replenishFrequency=DEFAULT_REPLENISH_FREQUENCY;
  }
}
