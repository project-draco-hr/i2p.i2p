{
  ActionList al=service.getActionList();
  sb.append("<ul>");
  for (int i=0; i < al.size(); i++) {
    Action action=al.getAction(i);
    if (action == null)     continue;
    sb.append("<li>").append(action.getName());
    listActionsArguments(action,sb);
    sb.append("</li>");
  }
  sb.append("</ul>");
}
