{
  ArgumentList ar=action.getArgumentList();
  sb.append("<ol>");
  for (int i=0; i < ar.size(); i++) {
    Argument argument=ar.getArgument(i);
    if (argument == null)     continue;
    sb.append("<li><small>argument : ").append(argument.getName()).append("</small></li>");
  }
  sb.append("</ol>");
}
