{
  ServiceStateTable table;
  try {
    table=serv.getServiceStateTable();
  }
 catch (  Exception e) {
    sb.append(" : no state");
    return;
  }
  sb.append("<ul><small>");
  for (int i=0; i < table.size(); i++) {
    StateVariable current=table.getStateVariable(i);
    sb.append("<li>" + current.getName() + " : \""+ current.getValue()+ "\"</li>");
  }
  sb.append("</small></ul>");
}
