{
  if (_log.shouldLog(Log.INFO))   _log.info("Before adding wants to write on " + con);
synchronized (_wantsWrite) {
    if (!_wantsWrite.contains(con))     _wantsWrite.add(con);
  }
  if (_log.shouldLog(Log.INFO))   _log.info("Wants to write on " + con);
  _selector.wakeup();
  if (_log.shouldLog(Log.DEBUG))   _log.debug("selector awoken for write");
}
