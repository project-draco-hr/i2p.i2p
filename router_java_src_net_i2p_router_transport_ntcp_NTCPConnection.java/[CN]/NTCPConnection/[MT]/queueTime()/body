{
  long queueTime=0;
  int size=0;
synchronized (_outbound) {
    OutNetMessage msg=_currentOutbound;
    size=_outbound.size();
    if ((msg == null) && (size > 0))     msg=(OutNetMessage)_outbound.get(0);
    if (msg == null)     return 0;
    queueTime=msg.getSendTime();
  }
  return queueTime;
}
