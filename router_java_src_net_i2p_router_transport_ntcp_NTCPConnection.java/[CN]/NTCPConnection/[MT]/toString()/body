{
  return "NTCP conn " + (_isInbound ? "from " : "to ") + (_remotePeer == null ? "unknown" : _remotePeer.calculateHash().toBase64().substring(0,6))+ (isEstablished() ? "" : " not established")+ " created "+ DataHelper.formatDuration(getTimeSinceCreated())+ " ago,"+ " last send "+ DataHelper.formatDuration(getTimeSinceSend())+ " ago,"+ " last recv "+ DataHelper.formatDuration(getTimeSinceReceive())+ " ago,"+ " sent "+ _messagesWritten+ ","+ " rcvd "+ _messagesRead;
}
