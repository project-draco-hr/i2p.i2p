{
  long queueTime=queueTime();
  if (queueTime <= 0)   return false;
  int size=0;
synchronized (_outbound) {
    size=_outbound.size();
  }
  if (getUptime() < 10 * 1000)   return false;
  if (queueTime > 5 * 1000) {
    if (_log.shouldLog(Log.WARN))     _log.warn("Too backlogged: queue time " + queueTime + " and the size is "+ size);
    _context.statManager().addRateData("ntcp.sendBacklogTime",queueTime,size);
    return true;
  }
 else {
    return false;
  }
}
