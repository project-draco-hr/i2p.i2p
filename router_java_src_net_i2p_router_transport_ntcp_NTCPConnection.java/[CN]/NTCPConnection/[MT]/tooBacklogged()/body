{
  long queueTime=queueTime();
  if (queueTime <= 0)   return false;
  boolean currentOutboundSet=_currentOutbound != null;
  if (getUptime() < 10 * 1000)   return false;
  if (queueTime > 5 * 1000) {
    int size=_outbound.size();
    if (_log.shouldLog(Log.WARN)) {
      int writeBufs=_writeBufs.size();
      try {
        _log.warn("Too backlogged: queue time " + queueTime + " and the size is "+ size+ ", wantsWrite? "+ (0 != (_conKey.interestOps() & SelectionKey.OP_WRITE))+ ", currentOut set? "+ currentOutboundSet+ ", writeBufs: "+ writeBufs+ " on "+ toString());
      }
 catch (      Exception e) {
      }
    }
    _context.statManager().addRateData("ntcp.sendBacklogTime",queueTime,size);
    return true;
  }
 else {
    return false;
  }
}
