{
  int pending=0;
  OutNetMessage msg=null;
synchronized (_sent) {
    pending=_sent.size() - 1;
    if (pending >= 0)     msg=(OutNetMessage)_sent.remove(0);
  }
  if (msg != null)   afterSend(msg,true,false,msg.getSendTime());
  if (pending > 0)   SimpleTimer.getInstance().addEvent(SendFinisher.this,0);
}
