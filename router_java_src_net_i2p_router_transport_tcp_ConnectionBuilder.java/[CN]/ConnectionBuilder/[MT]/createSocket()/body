{
  CreateSocketRunner r=new CreateSocketRunner();
  I2PThread t=new I2PThread(r);
  t.start();
  try {
    t.join(CONNECTION_TIMEOUT);
  }
 catch (  InterruptedException ie) {
  }
  if (!r.getCreated()) {
    fail("Unable to establish a socket in time to " + _target.getIdentity().calculateHash().toBase64().substring(0,6));
  }
}
