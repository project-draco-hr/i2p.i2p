{
  _keepRunning=true;
  _writeBuffer=new byte[38 * 1024];
  _lastTimeSend=-1;
  String name="TCP " + _context.routerHash().toBase64().substring(0,6) + " to "+ _con.getRemoteRouterIdentity().calculateHash().toBase64().substring(0,6);
  I2PThread t=new I2PThread(this,name);
  t.start();
  long delay=TIME_SEND_FREQUENCY + _context.random().nextInt(60 * 1000);
  SimpleTimer.getInstance().addEvent(new KeepaliveEvent(),delay);
}
