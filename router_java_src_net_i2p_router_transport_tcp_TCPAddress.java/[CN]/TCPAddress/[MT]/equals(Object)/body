{
  if ((val != null) && (val instanceof TCPAddress)) {
    TCPAddress addr=(TCPAddress)val;
    if (getAddress().getHostAddress() != null)     return DataHelper.eq(getAddress().getHostAddress(),addr.getAddress().getHostAddress()) && (getPort() == addr.getPort());
 else     return DataHelper.eq(getHost(),addr.getHost()) && (getPort() == addr.getPort());
  }
  return false;
}
