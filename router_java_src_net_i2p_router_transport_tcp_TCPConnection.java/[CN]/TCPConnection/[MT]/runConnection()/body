{
  if (_log.shouldLog(Log.DEBUG))   log.debug("Run connection");
  _runner=new ConnectionRunner();
  Thread t=new I2PThread(_runner);
  t.setName("Run Conn [" + _id + "]");
  t.setDaemon(true);
  t.start();
  _reader=new I2NPMessageReader(_in,this,"TCP Read [" + _id + "]");
  _reader.startReading();
}
