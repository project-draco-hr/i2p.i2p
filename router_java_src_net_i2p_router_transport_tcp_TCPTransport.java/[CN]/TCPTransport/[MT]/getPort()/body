{
  if ((_myAddress != null) && (_myAddress.getPort() > 0))   return _myAddress.getPort();
  String port=_context.getProperty(LISTEN_PORT,DEFAULT_LISTEN_PORT + "");
  if (port != null) {
    try {
      int portNum=Integer.parseInt(port);
      if ((portNum >= 1) && (portNum < 65535))       return portNum;
    }
 catch (    NumberFormatException nfe) {
    }
  }
  return -1;
}
