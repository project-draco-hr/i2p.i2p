{
  JobQueue.getInstance().addJob(new JobImpl(){
    public void runJob(){
      OutNetMessage msg=getNextMessage();
      if (msg != null) {
        handleOutbound(msg);
      }
 else {
        if (_log.shouldLog(Log.DEBUG))         _log.debug("OutboundMessageReady called, but none were available");
      }
    }
    public String getName(){
      return "TCP Message Ready to send";
    }
  }
);
}
