{
  RemoteHostId id=new RemoteHostId(from.getAddress(),fromPort);
  OutboundEstablishState state=_outboundStates.get(id);
  if (state != null) {
    boolean sendNow=state.receiveHolePunch();
    if (sendNow) {
      if (_log.shouldLog(Log.WARN))       _log.warn("Hole punch from " + state + ", sending SessionRequest now");
      notifyActivity();
    }
 else {
      if (_log.shouldLog(Log.WARN))       _log.warn("Hole punch from " + state + ", already sent SessionRequest");
    }
  }
 else {
    if (_log.shouldLog(Log.WARN))     _log.warn("No state found for hole punch from " + from + " port "+ fromPort);
  }
}
