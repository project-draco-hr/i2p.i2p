{
  if (_log.shouldLog(Log.DEBUG))   _log.debug("Send request to: " + state.getRemoteHostId().toString());
  UDPPacket packet=_builder.buildSessionRequestPacket(state);
  if (packet != null) {
    _transport.send(packet);
  }
 else {
    if (_log.shouldLog(Log.WARN))     _log.warn("Unable to build a session request packet for " + state.getRemoteHostId());
  }
  state.requestSent();
}
