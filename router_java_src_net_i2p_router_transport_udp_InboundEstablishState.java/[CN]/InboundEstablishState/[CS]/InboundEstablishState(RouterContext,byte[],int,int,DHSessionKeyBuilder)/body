{
  _context=ctx;
  _log=ctx.logManager().getLog(InboundEstablishState.class);
  _aliceIP=remoteIP;
  _alicePort=remotePort;
  _remoteHostId=new RemoteHostId(_aliceIP,_alicePort);
  _bobPort=localPort;
  _currentState=InboundState.IB_STATE_UNKNOWN;
  _establishBegin=ctx.clock().now();
  _keyBuilder=dh;
