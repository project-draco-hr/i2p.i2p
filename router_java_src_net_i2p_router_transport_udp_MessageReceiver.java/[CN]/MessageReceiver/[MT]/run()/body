{
  while (_fragments.isAlive()) {
    InboundMessageState message=_fragments.receiveNextMessage();
    if (message == null)     continue;
    int size=message.getCompleteSize();
    if (_log.shouldLog(Log.INFO))     _log.info("Full message received (" + message.getMessageId() + ") after "+ message.getLifetime()+ "... todo: parse and plop it onto InNetMessagePool");
    I2NPMessage msg=readMessage(message);
    if (msg != null)     _transport.messageReceived(msg,null,message.getFrom(),message.getLifetime(),size);
  }
}
