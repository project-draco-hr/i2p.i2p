{
  _lastSend=_context.clock().now();
  long delay=Math.min(RETRANSMIT_DELAY << (_confirmedSentCount++),MAX_DELAY);
  _nextSend=_lastSend + delay;
  if (_log.shouldLog(Log.DEBUG))   _log.debug("Send confirm packets, nextSend in " + delay);
  if ((_currentState == STATE_UNKNOWN) || (_currentState == STATE_REQUEST_SENT) || (_currentState == STATE_CREATED_RECEIVED))   _currentState=STATE_CONFIRMED_PARTIALLY;
}
