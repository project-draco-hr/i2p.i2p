{
  PeerState peer=_transport.getPeerState(ackedBy);
  if (peer != null) {
    if (_log.shouldLog(Log.DEBUG))     _log.debug("partial acked [" + bitfield + "] by "+ ackedBy.toBase64());
    peer.acked(bitfield);
  }
 else {
    if (_log.shouldLog(Log.DEBUG))     _log.debug("partial acked [" + bitfield + "] by an unknown remote peer?  "+ ackedBy.toBase64());
  }
}
