{
  short sends[]=_fragmentSends;
  if (sends != null)   for (int i=0; i < bitfield.fragmentCount() && i < sends.length; i++)   if (bitfield.received(i))   sends[i]=(short)-1;
  boolean rv=isComplete();
  if (!rv && false) {
    long nextTime=_context.clock().now() + Math.max(_peer.getRTT(),ACKSender.ACK_FREQUENCY);
    if (_nextSendTime <= 0)     _nextSendTime=nextTime;
 else     _nextSendTime=Math.min(_nextSendTime,nextTime);
  }
  return rv;
}
