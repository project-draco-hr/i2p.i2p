{
  if (_messageBuf == null)   return;
  int size=_messageBuf.getData().length;
  if (size == CACHE1_BYTES)   _cache1.release(_messageBuf);
 else   if (size == CACHE2_BYTES)   _cache2.release(_messageBuf);
 else   if (size == CACHE3_BYTES)   _cache3.release(_messageBuf);
 else   if (size == CACHE4_BYTES)   _cache4.release(_messageBuf);
  _messageBuf=null;
  _released=true;
}
