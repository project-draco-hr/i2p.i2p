{
  while (_alive) {
    try {
      UDPPacket packets[]=_fragments.getNextVolley();
      if (packets != null) {
        for (int i=0; i < packets.length; i++) {
          if (packets[i] != null)           send(packets[i]);
        }
      }
    }
 catch (    Exception e) {
      _log.error("SSU Output Queue Error",e);
    }
  }
}
