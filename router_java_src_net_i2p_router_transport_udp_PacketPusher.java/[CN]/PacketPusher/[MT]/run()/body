{
  while (_alive) {
    try {
      List<UDPPacket> packets=_fragments.getNextVolley();
      if (packets != null) {
        for (int i=0; i < packets.size(); i++) {
          send(packets.get(i));
        }
      }
    }
 catch (    Exception e) {
      _log.error("SSU Output Queue Error",e);
    }
  }
}
