{
  if (_dead) {
    _transport.failed(state,false);
    return 0;
  }
  state.setPeer(this);
  if (_log.shouldLog(Log.DEBUG))   _log.debug("Adding to " + _remotePeer.toBase64() + ": "+ state.getMessageId());
  List msgs=_outboundMessages;
  if (msgs == null)   return 0;
synchronized (msgs) {
    msgs.add(state);
    return msgs.size();
  }
}
