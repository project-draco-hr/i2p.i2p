{
  _dead=true;
  List msgs=_outboundMessages;
  _retransmitter=null;
  if (msgs != null) {
    int sz=0;
    List tempList=null;
synchronized (msgs) {
      sz=msgs.size();
      if (sz > 0) {
        tempList=new ArrayList(msgs);
        msgs.clear();
      }
    }
    for (int i=0; i < sz; i++)     _transport.failed((OutboundMessageState)tempList.get(i),false);
  }
}
