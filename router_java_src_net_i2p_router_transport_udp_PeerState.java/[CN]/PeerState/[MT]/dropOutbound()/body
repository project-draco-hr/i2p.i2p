{
  if (_dead)   return;
  _dead=true;
  List msgs=_outboundMessages;
  _retransmitter=null;
  if (msgs != null) {
    List tempList=null;
synchronized (msgs) {
      tempList=new ArrayList(msgs);
      msgs.clear();
    }
    int sz=tempList.size();
    for (int i=0; i < sz; i++)     _transport.failed((OutboundMessageState)tempList.get(i));
  }
}
