{
  _dead=true;
  List<OutboundMessageState> msgs=_outboundMessages;
  _retransmitter=null;
  int sz=0;
  List<OutboundMessageState> tempList=null;
synchronized (msgs) {
    sz=msgs.size();
    if (sz > 0) {
      tempList=new ArrayList(msgs);
      msgs.clear();
    }
  }
  for (int i=0; i < sz; i++)   _transport.failed(tempList.get(i),false);
  _wantACKSendSince=-1;
}
