{
  if (_currentTestNonce < 0) {
    return;
  }
 else   if (expired()) {
    testComplete();
  }
 else {
    if (_receiveBobReplyTime < 0) {
      sendTestToBob();
    }
 else     if (_receiveCharlieReplyTime < 0) {
      sendTestToBob();
    }
 else {
      sendTestToCharlie();
    }
    SimpleTimer.getInstance().addEvent(ContinueTest.this,RESEND_TIMEOUT);
  }
}
