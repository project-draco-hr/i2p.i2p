  _receiveCharlieReplyPort=-1;
  if (_log.shouldLog(Log.DEBUG))   _log.debug("Running test with bob = " + bobIP + ":"+ bobPort);
  sendTestToBob();
  SimpleTimer.getInstance().addEvent(new ContinueTest(),RESEND_TIMEOUT);
}
