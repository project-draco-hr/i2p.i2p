{
  if (obj == null)   throw new NullPointerException("obj is null");
  if (!(obj instanceof RemoteHostId))   throw new ClassCastException("obj is a " + obj.getClass().getName());
  RemoteHostId id=(RemoteHostId)obj;
  return (_port == id.getPort()) && DataHelper.eq(_ip,id.getIP()) && DataHelper.eq(_peerHash,id.getPeerHash());
}
