{
  if (CACHE) {
synchronized (_packetCache) {
      if (_packetCache.size() > 0) {
        UDPPacket rv=(UDPPacket)_packetCache.remove(0);
        rv._context=ctx;
        rv._log=ctx.logManager().getLog(UDPPacket.class);
        rv.resetBegin();
        Arrays.fill(rv._data,(byte)0x00);
        return rv;
      }
    }
  }
  return new UDPPacket(ctx);
}
