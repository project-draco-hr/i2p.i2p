{
  while (_keepRunning) {
synchronized (_inboundQueue) {
      if (_inboundQueue.size() <= 0) {
        try {
          _inboundQueue.wait();
        }
 catch (        InterruptedException ie) {
        }
      }
      if (_inboundQueue.size() > 0)       return (UDPPacket)_inboundQueue.remove(0);
    }
  }
  return null;
}
