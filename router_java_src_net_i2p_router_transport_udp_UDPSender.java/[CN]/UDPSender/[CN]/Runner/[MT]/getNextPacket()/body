{
  UDPPacket packet=null;
  while ((_keepRunning) && (packet == null)) {
    try {
synchronized (_outboundQueue) {
        if (_outboundQueue.size() <= 0) {
          _outboundQueue.wait();
        }
 else {
          packet=(UDPPacket)_outboundQueue.remove(0);
          _outboundQueue.notifyAll();
        }
      }
    }
 catch (    InterruptedException ie) {
    }
  }
  return packet;
}
