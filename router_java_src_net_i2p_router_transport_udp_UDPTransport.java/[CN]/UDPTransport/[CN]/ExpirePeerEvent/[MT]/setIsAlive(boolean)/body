{
  _alive=isAlive;
  if (isAlive) {
    SimpleTimer.getInstance().addEvent(ExpirePeerEvent.this,5 * 60 * 1000);
  }
 else {
    SimpleTimer.getInstance().removeEvent(ExpirePeerEvent.this);
synchronized (_toAdd) {
      _toAdd.clear();
    }
synchronized (_peers) {
      _peers.clear();
    }
  }
}
