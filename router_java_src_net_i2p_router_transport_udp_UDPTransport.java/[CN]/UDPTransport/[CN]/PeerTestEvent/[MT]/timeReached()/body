{
  if (shouldTest()) {
    long sinceRun=_context.clock().now() - _lastTested;
    if ((_forceRun && sinceRun >= MIN_TEST_FREQUENCY) || (sinceRun >= TEST_FREQUENCY)) {
      locked_runTest();
    }
  }
  if (_alive) {
    long delay=(TEST_FREQUENCY / 2) + _context.random().nextInt(TEST_FREQUENCY);
    schedule(delay);
  }
}
