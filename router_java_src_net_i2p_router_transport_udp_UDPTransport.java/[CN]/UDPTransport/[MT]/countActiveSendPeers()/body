{
  long now=_context.clock().now();
  int active=0;
  int inactive=0;
  for (Iterator<PeerState> iter=_peersByIdent.values().iterator(); iter.hasNext(); ) {
    PeerState peer=iter.next();
    if (now - peer.getLastSendFullyTime() > 1 * 60 * 1000)     inactive++;
 else     active++;
  }
  return active;
}
