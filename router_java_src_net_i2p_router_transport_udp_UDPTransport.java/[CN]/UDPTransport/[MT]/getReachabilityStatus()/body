{
  String override=_context.getProperty(PROP_REACHABILITY_STATUS_OVERRIDE);
  if (override == null)   return _reachabilityStatus;
  if ("ok".equals(override))   return CommSystemFacade.STATUS_OK;
 else   if ("err-reject".equals(override))   return CommSystemFacade.STATUS_REJECT_UNSOLICITED;
 else   if ("err-different".equals(override))   return CommSystemFacade.STATUS_DIFFERENT;
  return _reachabilityStatus;
}
