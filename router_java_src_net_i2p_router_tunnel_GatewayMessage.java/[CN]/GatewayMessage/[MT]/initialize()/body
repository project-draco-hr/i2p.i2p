{
  _iv=new byte[HOPS - 1][IV_SIZE];
  _eIV=new byte[HOPS - 1][IV_SIZE];
  _H=new byte[HOPS][Hash.HASH_LENGTH];
  _eH=new byte[COLUMNS][HASH_ROWS][COLUMN_WIDTH];
