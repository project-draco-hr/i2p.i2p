{
  RouterInfo info=getContext().netDb().lookupRouterInfoLocally(_target);
  if (info != null) {
    _log.debug("outbound distributor to " + _target.toBase64().substring(0,4) + "."+ (_tunnel != null ? _tunnel.getTunnelId() + "" : "")+ ": found on search");
    distribute(_message,info,_tunnel);
  }
 else {
    _log.error("outbound distributor to " + _target.toBase64().substring(0,4) + "."+ (_tunnel != null ? _tunnel.getTunnelId() + "" : "")+ ": NOT found on search");
  }
}
