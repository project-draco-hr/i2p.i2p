{
  if (_log.shouldLog(Log.DEBUG))   _log.debug("preprocessed data going out " + _config + ": "+ Base64.encode(preprocessed));
  _processor.process(preprocessed,0,preprocessed.length);
  if (_log.shouldLog(Log.DEBUG))   _log.debug("after wrapping up the preprocessed data on " + _config);
  long rv=receiver.receiveEncrypted(preprocessed);
  if (_log.shouldLog(Log.DEBUG))   _log.debug("after receiving on " + _config + ": receiver = "+ receiver);
  return rv;
}
