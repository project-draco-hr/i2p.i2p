{
  if (_log.shouldLog(Log.DEBUG))   _log.debug("outbound tunnel " + _config + " received a full message: "+ msg+ " to be forwarded on to "+ (toRouter != null ? toRouter.toBase64().substring(0,4) : "")+ (toTunnel != null ? toTunnel.getTunnelId() + "" : ""));
  if ((!_context.routerHash().equals(toRouter)) && _context.tunnelDispatcher().shouldDropParticipatingMessage("OBEP " + msg.getType(),msg.getMessageSize()))   return;
  _config.incrementSentMessages();
  _outDistributor.distribute(msg,toRouter,toTunnel);
}
