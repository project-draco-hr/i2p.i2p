{
  _messagesSent++;
  Pending cur=new PendingImpl(msg,toRouter,toTunnel);
  long beforeLock=System.currentTimeMillis();
synchronized (_prequeue) {
    _prequeue.add(cur);
  }
  _pumper.wantsPumping(this);
  if (_log.shouldLog(Log.DEBUG))   _log.debug("GW prequeue time: " + (System.currentTimeMillis() - beforeLock) + " for "+ msg.getUniqueId()+ " on "+ toString());
}
