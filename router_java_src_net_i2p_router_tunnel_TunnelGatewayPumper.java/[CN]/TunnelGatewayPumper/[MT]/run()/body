{
  PumpedTunnelGateway gw=null;
  List<TunnelGateway.Pending> queueBuf=new ArrayList(32);
  while (!_stop) {
    try {
      gw=_wantsPumping.take();
    }
 catch (    InterruptedException ie) {
    }
    if (gw != null) {
      if (gw.getMessagesSent() == POISON_PTG)       break;
      gw.pump(queueBuf);
      gw=null;
    }
  }
}
