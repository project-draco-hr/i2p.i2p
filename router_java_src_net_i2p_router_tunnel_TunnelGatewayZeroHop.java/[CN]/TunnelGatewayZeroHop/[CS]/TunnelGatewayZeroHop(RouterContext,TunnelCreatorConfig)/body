{
  super(context,null,null,null);
  _context=context;
  _log=context.logManager().getLog(TunnelGatewayZeroHop.class);
  _config=config;
  if (config.isInbound())   _inDistributor=new InboundMessageDistributor(_context,config.getDestination());
 else   _outDistributor=new OutboundMessageDistributor(context,400);
}
