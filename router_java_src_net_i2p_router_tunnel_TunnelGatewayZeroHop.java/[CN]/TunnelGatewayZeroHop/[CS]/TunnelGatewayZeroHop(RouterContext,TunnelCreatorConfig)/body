{
  super(context,null,null,null);
  TunnelContext=context;
  TunnelLog=context.logManager().getLog(TunnelGatewayZeroHop.class);
  _config=config;
  if (config.isInbound())   _inDistributor=new InboundMessageDistributor(TunnelContext,config.getDestination());
 else   _outDistributor=new OutboundMessageDistributor(context,400);
}
