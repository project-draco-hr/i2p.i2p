{
  long beforeBuild=System.currentTimeMillis();
  BuildRequestor.request(_context,pool,cfg,this);
  long buildTime=System.currentTimeMillis() - beforeBuild;
  if (cfg.getLength() <= 1)   _context.statManager().addRateData("tunnel.buildRequestZeroHopTime",buildTime,buildTime);
 else   _context.statManager().addRateData("tunnel.buildRequestTime",buildTime,buildTime);
  long id=cfg.getReplyMessageId();
  if (id > 0) {
synchronized (_recentBuildIds) {
      if (_recentBuildIds.size() > 98) {
        for (int i=0; i < 32; i++)         _recentBuildIds.remove(0);
      }
      _recentBuildIds.add(new Long(id));
    }
  }
}
