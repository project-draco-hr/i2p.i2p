{
  getContext().statManager().addRateData("tunnel.rejectTimeout",1,1);
  if (_log.shouldLog(Log.ERROR))   _log.error("Request " + _state.msg.getUniqueId() + " could no be satisfied, as the next peer could not be found: "+ _nextPeer.toBase64());
  getContext().messageHistory().tunnelRejected(_state.fromHash,new TunnelId(_req.readReceiveTunnelId()),_nextPeer,"rejected because we couldn't find " + _nextPeer.toBase64() + ": "+ _state.msg.getUniqueId()+ "/"+ _req.readNextTunnelId());
}
