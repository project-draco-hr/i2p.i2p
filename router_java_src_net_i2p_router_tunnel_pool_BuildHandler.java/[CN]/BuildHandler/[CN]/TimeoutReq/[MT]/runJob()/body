{
  getContext().statManager().addRateData("tunnel.rejectTimeout",1);
  getContext().statManager().addRateData("tunnel.buildLookupSuccess",0);
  getContext().messageHistory().tunnelRejected(_state.fromHash,new TunnelId(_req.readReceiveTunnelId()),_nextPeer,"rejected because we couldn't find " + _nextPeer + ": "+ _state.msg.getUniqueId()+ "/"+ _req.readNextTunnelId());
}
