{
  int c=getFailPercentage(ctx,"Client");
  int e=getFailPercentage(ctx,"Exploratory");
  Log l=ctx.logManager().getLog(getClass());
  if (l.shouldLog(Log.DEBUG))   l.debug("Client, Expl. Fail pct: " + c + ", "+ e);
  if (e <= c || e <= 25)   return 0;
  if (c >= 90)   return 100 - MIN_NONFAILING_PCT;
  return (100 * (e - c)) / (100 - c);
}
