{
  if (_log.shouldLog(Log.WARN))   _log.warn("request timeout: " + _config + " at hop "+ _currentHop+ " with nonce "+ _nonce);
  if (_isExploratory)   getContext().statManager().addRateData("tunnel.buildExploratoryTimeout",1,0);
 else   getContext().statManager().addRateData("tunnel.buildClientTimeout",1,0);
  peerFail(0);
}
