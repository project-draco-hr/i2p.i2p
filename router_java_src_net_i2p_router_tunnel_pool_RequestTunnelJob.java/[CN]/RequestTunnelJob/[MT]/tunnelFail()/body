{
  if (_log.shouldLog(Log.INFO))   _log.info("tunnel building failed: " + _config + " at hop "+ _currentHop);
  if (_onFailed != null)   getContext().jobQueue().addJob(_onFailed);
  if (_isExploratory)   getContext().statManager().addRateData("tunnel.buildExploratoryFailure",1,0);
 else   getContext().statManager().addRateData("tunnel.buildFailure",1,0);
}
