{
  if (!_alive)   return;
  int added;
  if (_settings.isExploratory())   added=refreshBuilders(0,2);
 else   added=refreshBuilders(0,1);
  if ((added > 0) && (_log.shouldLog(Log.WARN)))   _log.warn(added + " additional parallel rebuild(s) for " + TunnelPool.this.toString());
  requeue(30 * 1000);
}
