{
  _alive=true;
  _refreshJob.getTiming().setStartAfter(_context.clock().now() + 60 * 1000);
  _context.jobQueue().addJob(_refreshJob);
  int added=refreshBuilders();
  if (added <= 0) {
    LeaseSet ls=null;
synchronized (_tunnels) {
      if (_settings.isInbound() && (_settings.getDestination() != null))       ls=locked_buildNewLeaseSet();
    }
    if (ls != null)     _context.clientManager().requestLeaseSet(_settings.getDestination(),ls);
  }
}
