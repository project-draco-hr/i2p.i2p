{
  _alive=true;
  _started=System.currentTimeMillis();
  _lastRateUpdate=_started;
  _lastLifetimeProcessed=0;
  _manager.getExecutor().repoll();
  if (_settings.isInbound() && (_settings.getDestination() != null)) {
    LeaseSet ls=null;
synchronized (_tunnels) {
      ls=locked_buildNewLeaseSet();
    }
    if (ls != null)     _context.clientManager().requestLeaseSet(_settings.getDestination(),ls);
  }
  _context.statManager().createRateStat(_rateName,"Tunnel Bandwidth","Tunnels",new long[]{5 * 60 * 1000l});
}
