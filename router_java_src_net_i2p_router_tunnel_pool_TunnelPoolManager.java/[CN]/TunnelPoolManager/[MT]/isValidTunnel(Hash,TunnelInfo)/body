{
  if (tunnel.getExpiration() < _context.clock().now())   return false;
  TunnelPool pool;
  if (tunnel.isInbound())   pool=(TunnelPool)_clientInboundPools.get(client);
 else   pool=(TunnelPool)_clientOutboundPools.get(client);
  if (pool == null)   return false;
  return pool.listTunnels().contains(tunnel);
}
