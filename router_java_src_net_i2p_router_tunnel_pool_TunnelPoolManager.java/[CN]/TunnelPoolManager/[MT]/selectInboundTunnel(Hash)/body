{
  if (destination == null)   return selectInboundTunnel();
  TunnelPool pool=_clientInboundPools.get(destination);
  if (pool != null) {
    return pool.selectTunnel();
  }
  if (_log.shouldLog(Log.ERROR))   _log.error("Want the inbound tunnel for " + destination.calculateHash().toBase64() + " but there isn't a pool?");
  return null;
}
