{
  if (!_isStopped) {
    if (_log.shouldLog(Log.WARN))     _log.warn("Pool " + _poolId + ": Not starting the pool /again/ (its already running)");
    return;
  }
 else {
    if (_log.shouldLog(Log.INFO))     _log.info("Pool " + _poolId + ": Starting up the pool ");
  }
  _isStopped=false;
  if (_mgrJob == null) {
    _mgrJob=new ClientTunnelPoolManagerJob(_pool,this);
    JobQueue.getInstance().addJob(_mgrJob);
  }
  if (_leaseMgrJob == null) {
    _leaseMgrJob=new ClientLeaseSetManagerJob(this);
    JobQueue.getInstance().addJob(_leaseMgrJob);
  }
 else {
    _leaseMgrJob.forceRequestLease();
    _leaseMgrJob.getTiming().setStartAfter(Clock.getInstance().now());
    JobQueue.getInstance().addJob(_leaseMgrJob);
  }
  if (_tunnelExpirationJob == null) {
    _tunnelExpirationJob=new ClientTunnelPoolExpirationJob(this,_pool);
    JobQueue.getInstance().addJob(_tunnelExpirationJob);
  }
}
