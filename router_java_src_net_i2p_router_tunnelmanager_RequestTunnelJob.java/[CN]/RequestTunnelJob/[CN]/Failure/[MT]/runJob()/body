{
  if (_log.shouldLog(Log.WARN))   _log.warn("Tunnel creation timed out for tunnel " + _tunnel.getTunnelId() + " at router "+ _tunnel.getThisHop().toBase64()+ " with expiration "+ new Date(_expiration));
synchronized (_failedTunnelParticipants) {
    _failedTunnelParticipants.add(_tunnel.getThisHop());
    _failedTunnelParticipants.add(_replyThrough);
  }
  MessageHistory.getInstance().tunnelRequestTimedOut(_tunnel.getThisHop(),_tunnel.getTunnelId(),_replyThrough);
  ProfileManager.getInstance().tunnelRejected(_tunnel.getThisHop(),Clock.getInstance().now() - _started);
  ProfileManager.getInstance().messageFailed(_tunnel.getThisHop());
  StatManager.getInstance().updateFrequency("tunnel.buildFailFrequency");
  fail();
}
