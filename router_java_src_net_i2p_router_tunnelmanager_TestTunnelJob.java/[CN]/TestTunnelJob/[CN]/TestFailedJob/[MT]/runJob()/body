{
  if (_log.shouldLog(Log.WARN))   _log.warn("Test of tunnel " + _primaryId.getTunnelId() + " failed while waiting for nonce "+ _nonce+ ": "+ _pool.getTunnelInfo(_primaryId),getAddedBy());
  _pool.tunnelFailed(_primaryId);
  if (_secondaryId != null) {
    if (_log.shouldLog(Log.WARN))     _log.warn("Secondary test of tunnel " + _secondaryId.getTunnelId() + " failed while waiting for nonce "+ _nonce+ ": "+ _pool.getTunnelInfo(_secondaryId),getAddedBy());
  }
}
