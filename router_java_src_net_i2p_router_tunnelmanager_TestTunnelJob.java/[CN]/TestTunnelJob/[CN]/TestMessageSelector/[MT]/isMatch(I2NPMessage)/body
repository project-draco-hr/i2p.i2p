{
  if ((message != null) && (message instanceof DeliveryStatusMessage)) {
    DeliveryStatusMessage msg=(DeliveryStatusMessage)message;
    if (msg.getMessageId() == _id) {
      if (_log.shouldLog(Log.DEBUG))       _log.debug("Found successful test of tunnel " + _tunnelId + " after "+ (getContext().clock().now() - msg.getArrival().getTime())+ "ms waiting for "+ _id);
      _found=true;
      return true;
    }
 else {
      if (_log.shouldLog(Log.DEBUG))       _log.debug("Found a delivery status message, but it contains nonce " + msg.getMessageId() + " and not "+ _id);
    }
  }
 else {
  }
  return false;
}
