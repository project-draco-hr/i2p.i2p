{
  long time=(Clock.getInstance().now() - _msg.getArrival().getTime());
  if (_log.shouldLog(Log.INFO))   _log.info("Test of tunnel " + _id + " successfull after "+ time+ "ms waiting for "+ _nonce);
  TunnelInfo info=_pool.getTunnelInfo(_id);
  if (info != null)   MessageHistory.getInstance().tunnelValid(info,time);
}
