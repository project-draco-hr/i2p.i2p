{
  if (_log.shouldLog(Log.DEBUG))   _log.debug("Testing tunnel " + _primaryId.getTunnelId());
  TunnelInfo info=_pool.getTunnelInfo(_primaryId);
  if (info == null) {
    _log.error("wtf, why are we testing a tunnel that we do not know about? [" + _primaryId.getTunnelId() + "]",getAddedBy());
    return;
  }
  info.setLastTested(getContext().clock().now());
  if (isOutbound(info)) {
    testOutbound(info);
  }
 else {
    testInbound(info);
  }
}
