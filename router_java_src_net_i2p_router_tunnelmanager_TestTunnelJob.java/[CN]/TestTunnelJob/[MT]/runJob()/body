{
  if (_log.shouldLog(Log.INFO))   _log.info("Testing tunnel " + _id.getTunnelId());
  TunnelInfo info=_pool.getTunnelInfo(_id);
  if (info == null) {
    _log.error("wtf, why are we testing a tunnel that we do not know about? [" + _id.getTunnelId() + "]",getAddedBy());
    return;
  }
  if (isOutbound(info)) {
    testOutbound(info);
  }
 else {
    testInbound(info);
  }
}
