{
  long delay=_context.clock().now() - entry.getEnqueueTime();
  _context.statManager().addRateData(STAT_DROP + entry.getPriority(),delay);
  if (_log.shouldLog(Log.WARN))   _log.warn(_name + " dropped item with delay " + delay+ ", priority "+ entry.getPriority()+ ", seq "+ entry.getSeqNum()+ ", "+ size()+ " remaining in queue: "+ entry);
  entry.drop();
}
