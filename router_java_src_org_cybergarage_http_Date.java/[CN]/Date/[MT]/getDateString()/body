{
  Calendar _cal=getCalendar();
  return toWeekString(_cal.get(Calendar.DAY_OF_WEEK)) + ", " + toTimeString(_cal.get(Calendar.DATE))+ " "+ toMonthString(_cal.get(Calendar.MONTH))+ " "+ Integer.toString(_cal.get(Calendar.YEAR))+ " "+ toTimeString(_cal.get(Calendar.HOUR_OF_DAY))+ ":"+ toTimeString(_cal.get(Calendar.MINUTE))+ ":"+ toTimeString(_cal.get(Calendar.SECOND))+ " GMT";
}
