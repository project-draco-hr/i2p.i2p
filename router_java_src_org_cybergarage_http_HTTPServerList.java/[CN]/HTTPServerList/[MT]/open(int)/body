{
  int nHostAddrs=HostInterface.getNHostAddresses();
  for (int n=0; n < nHostAddrs; n++) {
    String bindAddr=HostInterface.getHostAddress(n);
    HTTPServer httpServer=new HTTPServer();
    if (httpServer.open(bindAddr,port) == false) {
      close();
      clear();
      return false;
    }
    add(httpServer);
  }
  return true;
}
