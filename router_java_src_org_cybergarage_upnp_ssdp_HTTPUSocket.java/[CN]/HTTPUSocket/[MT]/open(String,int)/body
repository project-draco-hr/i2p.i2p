{
  close();
  try {
    InetSocketAddress bindSock=new InetSocketAddress(bindPort);
    ssdpUniSock=new DatagramSocket(null);
    ssdpUniSock.setReuseAddress(true);
    ssdpUniSock.bind(bindSock);
  }
 catch (  Exception e) {
    Debug.warning(e);
    return false;
  }
  setLocalAddress(bindAddr);
  return true;
}
