{
  boolean ok=false;
  byte m[]=msg.toByteArray();
  if ((_expected != null) && (DataHelper.eq(_expected,m)))   ok=true;
  if (!ok && (_expected2 != null) && (DataHelper.eq(_expected2,m)))   ok=true;
  if (!ok && (_expected3 != null) && (DataHelper.eq(_expected3,m)))   ok=true;
  if (ok)   _received++;
}
