{
  String cfgFile="router.config";
  Properties envProps=getEnv();
  Router r=new Router(cfgFile,envProps);
  r.runRouter();
  _us=r.getContext();
  setupHandlers();
  try {
    Thread.sleep(30 * 1000);
  }
 catch (  InterruptedException ie) {
  }
  RouterInfo myInfo=_us.router().getRouterInfo();
  storeMyInfo(myInfo);
  loadPeers();
}
